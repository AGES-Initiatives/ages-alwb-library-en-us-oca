meMA.Exaposteilarion1.incipit = "Let us sing the praises…"
meMA.Exaposteilarion1.text = "Let us sing the praises of the Maccabees, ⁄ children of Eleazar and Solomonia, ⁄ for they laid low the arrogance of the enemy ⁄⁄ and were crowned in the service of the law!"

