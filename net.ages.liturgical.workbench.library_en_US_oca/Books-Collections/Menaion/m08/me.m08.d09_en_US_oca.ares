meVE.Stichera04.mode = misc_en_US_oca.Mode1
meVE.Stichera04.melody = he.a.m1_en_US_oca.PaneffimoiMartyres.notmetered
meVE.Stichera04.incipit = "Blessed Matthias, ⁄ you were…"
meVE.Stichera04.text = "Blessed Matthias, ⁄ you were sent out as a river flowing from Eden. ⁄ You watered the earth with godliness! ⁄ You bore the fruit of God's saving work! ⁄⁄ Pray to Him to grant our souls peace and great mercy!"

meVE.Stichera05.mode = misc_en_US_oca.Mode1
meVE.Stichera05.melody = he.a.m1_en_US_oca.PaneffimoiMartyres.notmetered
meVE.Stichera05.incipit = "Apostle Matthias, ⁄ you completed…"
meVE.Stichera05.text = "Apostle Matthias, ⁄ you completed God's choir after Judas was lost. ⁄ Your words of wisdom fall as divine lightning, ⁄ dispelling the darkness of ignorance. ⁄ By the power of the Spirit you overthrew the madness of idolatry; ⁄⁄ now pray the Lord to grant us peace and great mercy!"

meVE.Stichera06.mode = misc_en_US_oca.Mode1
meVE.Stichera06.melody = he.a.m1_en_US_oca.PaneffimoiMartyres.notmetered
meVE.Stichera06.incipit = "Divinely-inspired Matthias, ⁄ your proclamation…"
meVE.Stichera06.text = "Divinely-inspired Matthias, ⁄ your proclamation illumines the earth as flashes of lightning. ⁄ You have enlightened and instructed us in the knowledge of God; ⁄ we slept in the night of idolatry but are converted to works of light, ⁄⁄ for you have dispersed the shadows of godlessness."

