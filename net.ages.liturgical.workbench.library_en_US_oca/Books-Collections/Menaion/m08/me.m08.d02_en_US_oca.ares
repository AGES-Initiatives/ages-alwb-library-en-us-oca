meVE.AposGlory.mode = miscellanea_en_US_oca.misc.Mode6
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "Rejoice in the Lord, crown-bearer…"
meVE.AposGlory.text = "Rejoice in the Lord, crown-bearer Stephen! ⁄ You followed your Master, ⁄ for you were the first martyr of Christ our King. ⁄ You abolished the deception of the enemy ⁄⁄ and you pray to the Lord for the salvation of our souls."

