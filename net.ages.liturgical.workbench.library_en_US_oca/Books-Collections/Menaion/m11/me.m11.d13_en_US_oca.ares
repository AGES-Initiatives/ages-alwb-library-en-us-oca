meSV.Aposticha1.mode = miscellanea_en_US_oca.misc.Mode2
meSV.Aposticha1.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meSV.Aposticha1.incipit = "You are revealed…"
meSV.Aposticha1.text = "You are revealed, O John, ⁄ to the ends of the earth, ⁄ as a golden lyre, ⁄ a shining lampstand, ⁄⁄ proclaiming the mighty works of God."

meSV.Aposticha2.mode = miscellanea_en_US_oca.misc.Mode2
meSV.Aposticha2.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meSV.Aposticha2.incipit = "…"
meSV.Aposticha2.text = "Moses received the law ⁄ from the hands of the Lord, ⁄ and you, Chrysostom, ⁄ enlighten the world, ⁄⁄ by the wisdom of your divine teaching."

meSV.Aposticha3.mode = miscellanea_en_US_oca.misc.Mode2
meSV.Aposticha3.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meSV.Aposticha3.incipit = "…"
meSV.Aposticha3.text = "By your golden tongue, ⁄ you became the herald ⁄ of God's true kingdom, ⁄ crying out to all: ⁄⁄ Repent! Forsake the sea of ignorance!"

meSV.AposGlory.mode = miscellanea_en_US_oca.misc.Mode2
meSV.AposGlory.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meSV.AposGlory.incipit = "…"
meSV.AposGlory.text = ""

meSV.AposBoth.mode = miscellanea_en_US_oca.misc.Mode2
meSV.AposBoth.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meSV.AposBoth.incipit = "…"
meSV.AposBoth.text = "O Virgin ⁄ and Theotokos, ⁄ entreat the Word of God ⁄ Who was born of you ⁄⁄ that He may save our souls."

