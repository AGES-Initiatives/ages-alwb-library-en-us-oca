meMA.Ode1C11.ode = miscellanea_en_US_oca.misc.Ode1
meMA.Ode1C11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode1C11.greekmelody = he.h.m4_en_US_oca.heHE.AnoixoToStomaMou.greekname
meMA.Ode1C11.melody = he.h.m4_en_US_oca.heHE.AnoixoToStomaMou.notmetered
meMA.Ode1C11.incipit = "Most glorious Matthew, servant of…"
meMA.Ode1C11.text = "Most glorious Matthew, servant of Christ, ⁄ richly grant me the grace which abides in you, ⁄ that I may cry out, proclaiming in splendor, ⁄ rejoicing to sing hymns to your wonders!"

meMA.Ode1C12.ode = miscellanea_en_US_oca.misc.Ode1
meMA.Ode1C12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode1C12.melody = he.h.m4_en_US_oca.heHE.AnoixoToStomaMou.notmetered
meMA.Ode1C12.incipit = "You utterly abandoned the tumult…"
meMA.Ode1C12.text = "You utterly abandoned the tumult of the world, ⁄ for having listened to the voice of the incarnate Word, ⁄ you were shown to be a steward and God-pleasing herald of His grace."

meMA.Ode1C13.ode = miscellanea_en_US_oca.misc.Ode1
meMA.Ode1C13.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode1C13.melody = he.h.m4_en_US_oca.heHE.AnoixoToStomaMou.notmetered
meMA.Ode1C13.incipit = "As an eye-witness and servant…"
meMA.Ode1C13.text = "As an eye-witness and servant, Apostle, ⁄ of the shining Word of the pre-eternal Father, ⁄ you went forth to all the nations, ⁄ proclaiming the Gospel of His coming!"

meMA.Ode1C1T.ode = miscellanea_en_US_oca.misc.Ode1
meMA.Ode1C1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode1C1T.melody = he.h.m4_en_US_oca.heHE.AnoixoToStomaMou.notmetered
meMA.Ode1C1T.incipit = "You announced the Gospel of…"
meMA.Ode1C1T.text = "You announced the Gospel of the destruction of death, O wise one, ⁄ the abolition of corruption and the manifestation of life, ⁄ for the Virgin has restored the world, ⁄ aving conceived the unapproachable One in her womb!"

meMA.Ode3C11.ode = miscellanea_en_US_oca.misc.Ode3
meMA.Ode3C11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_en_US_oca.heHE.TousSousYmnologous.greekname
meMA.Ode3C11.melody = he.h.m4_en_US_oca.heHE.TousSousYmnologous.notmetered
meMA.Ode3C11.incipit = "Word of God, You have…"
meMA.Ode3C11.text = "Word of God, You have revealed your apostles ⁄ to be beacons of life, driving away the darkness of ungodliness ⁄ and illumining the whole world with Your divine glory, Master."

meMA.Ode3C12.ode = miscellanea_en_US_oca.misc.Ode3
meMA.Ode3C12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_en_US_oca.heHE.TousSousYmnologous.notmetered
meMA.Ode3C12.incipit = "You armed the most valiant…"
meMA.Ode3C12.text = "You armed the most valiant Matthew with Your armor, Savior. ⁄ You made him stronger than the tormentors. ⁄ You revealed him to be the destroyer of idolatry’s deception, ⁄ Master and Lover of mankind."

meMA.Ode3C13.ode = miscellanea_en_US_oca.misc.Ode3
meMA.Ode3C13.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_en_US_oca.heHE.TousSousYmnologous.notmetered
meMA.Ode3C13.incipit = "The sound of your fiery…"
meMA.Ode3C13.text = "The sound of your fiery tongue consumed the temples of the demons, ⁄ divinely inspired Matthew. ⁄ You are an instrument of the Comforter, ⁄ through whom Christ, the Life of all in Person, is proclaimed!"

meMA.Ode3C1T.ode = miscellanea_en_US_oca.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_en_US_oca.heHE.TousSousYmnologous.notmetered
meMA.Ode3C1T.incipit = "The trumpet of theology sounds…"
meMA.Ode3C1T.text = "The trumpet of theology sounds with might, ⁄ illumining the divinely wise Matthew ⁄ to proclaim to the people the radiance of the Trinity ⁄ and the incarnation of the Word from you, all-pure one, ⁄ in a manner beyond understanding!"

meMA.Ode3H.ode = miscellanea_en_US_oca.misc.Ode3
meMA.Ode3H.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode3H.greekmelody = he.h.m4_en_US_oca.heHE.TousSousYmnologous.greekname
meMA.Ode3H.melody = he.h.m4_en_US_oca.heHE.TousSousYmnologous.melody
meMA.Ode3H.incipit = he.h.m4_en_US_oca.heHE.TousSousYmnologous.incipit
meMA.Ode3H.text = he.h.m4_en_US_oca.heHE.TousSousYmnologous.text

meMA.Ode3Kathisma1.mode = miscellanea_en_US_oca.misc.Mode1
meMA.Ode3Kathisma1.greekmelody = he.a.m1_en_US_oca.heAU.TonTafonSouSotir.greekname
meMA.Ode3Kathisma1.melody = he.a.m1_en_US_oca.heAU.TonTafonSouSotir.notmetered
meMA.Ode3Kathisma1.incipit = "The sound of your wise…"
meMA.Ode3Kathisma1.text = "The sound of your wise words has indeed gone out ⁄ into all the ends of the earth, most praised apostle of the Lord, ⁄ for you openly preached to all the vision of God, ⁄ and brought the nations from ignorance to understanding. ⁄ therefore you dispelled the darkness of idolatry, most praised Matthew, ⁄ and shone forth the light of knowledge upon those who sit in darkness. ⁄ Entreat Christ God, that He may grant remission of sins ⁄⁄ to those who honor your holy memory with love."

meMA.Ode3Kathisma2.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode3Kathisma2.greekmelody = he.a.m8_en_US_oca.heAU.TinSofianKaiLogon.greekname
meMA.Ode3Kathisma2.melody = he.a.m8_en_US_oca.heAU.TinSofianKaiLogon.notmetered
meMA.Ode3Kathisma2.incipit = "We all praise you, Apostle…"
meMA.Ode3Kathisma2.text = "We all praise you, Apostle of the Lord, ⁄ as the lamp and disciple of the light, an eye-witness of the divine Word, ⁄ for spreading your nets of grace, by your command you caught reason-endowed fish! ⁄ Therefore, through the power of the Spirit you have trapped the nations, ⁄ who now are guided by faith, most praised initiate of heaven’s mysteries! ⁄ Entreat Christ God that He may grant remission of sins ⁄⁄ to those who honor your holy memory with love!"

meMA.Ode3KathismaT.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode3KathismaT.melody = he.a.m8_en_US_oca.heAU.TinSofianKaiLogon.notmetered
meMA.Ode3KathismaT.incipit = oc.m8.d1_en_US_oca.ocMA.Kathisma13.incipit
meMA.Ode3KathismaT.text = oc.m8.d1_en_US_oca.ocMA.Kathisma13.text

meMA.Ode4C11.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode4C11.greekmelody = he.h.m4_en_US_oca.heHE.TinAnexichniaston.greekname
meMA.Ode4C11.melody = he.h.m4_en_US_oca.heHE.TinAnexichniaston.notmetered
meMA.Ode4C11.incipit = "Taught by Your Word in…"
meMA.Ode4C11.text = "Taught by Your Word in Person, O God, ⁄ the God-seeing disciple destroyed worldly wisdom. ⁄ He cried aloud to You: ⁄ Glory to Your power, O God!"

meMA.Ode4C12.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode4C12.melody = he.h.m4_en_US_oca.heHE.TinAnexichniaston.notmetered
meMA.Ode4C12.incipit = "What praise can we offer…"
meMA.Ode4C12.text = "What praise can we offer you, ⁄ blessed of Christ and divinely eloquent one, ⁄ for no one is able to describe in words ⁄ the grace which blossomed forth from you, truly wondrous one!"

meMA.Ode4C13.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C13.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode4C13.melody = he.h.m4_en_US_oca.heHE.TinAnexichniaston.notmetered
meMA.Ode4C13.incipit = "Looking upon your undefiled and…"
meMA.Ode4C13.text = "Looking upon your undefiled and pure mind ⁄ as a lamp placed on a raised stand, shining on those in darkness, ⁄ the Most High has set you before all, ⁄ divinely manifested one."

meMA.Ode4C1T.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode4C1T.melody = he.h.m4_en_US_oca.heHE.TinAnexichniaston.notmetered
meMA.Ode4C1T.incipit = "Blessed and most praised Maiden…"
meMA.Ode4C1T.text = "Blessed and most praised Maiden, ⁄ you have raised fallen human nature, ⁄ for in a manner beyond nature, ⁄ you gave birth to the strength of the Most High, ⁄ as the Evangelist Matthew has taught!"

meMA.Ode5C11.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode5C11.greekmelody = he.h.m4_en_US_oca.heHE.ExestiTaSympanta.greekname
meMA.Ode5C11.melody = he.h.m4_en_US_oca.heHE.ExestiTaSympanta.notmetered
meMA.Ode5C11.incipit = "Receiving the grace of the…"
meMA.Ode5C11.text = "Receiving the grace of the most Holy Spirit, ⁄ you were revealed to be a dwelling of God, ⁄ for in a Godly manner you taught the light of divine knowledge. ⁄ You were entrusted with the task of committing the teachings of Christ to writing ⁄ as a chosen eye-witness of God!"

meMA.Ode5C12.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_en_US_oca.heHE.ExestiTaSympanta.notmetered
meMA.Ode5C12.incipit = "Your tongue was the pen…"
meMA.Ode5C12.text = "Your tongue was the pen of the Paraclete, ⁄ Who gives swift deliverance to those who acknowledge His dominion. ⁄ He breathes heavenly understanding into the souls of the wise, ⁄ most honored Matthew."

meMA.Ode5C13.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C13.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode5C13.melody = he.h.m4_en_US_oca.heHE.ExestiTaSympanta.notmetered
meMA.Ode5C13.incipit = "Christ sent you to the…"
meMA.Ode5C13.text = "Christ sent you to the world shining with miracles ⁄ and with the grace of healing, ⁄ trampling upon the might of the demons ⁄ and enlightening the souls of all the faithful who sing hymns to you, ⁄ the preacher of the world."

meMA.Ode5C1T.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode5C1T.melody = he.h.m4_en_US_oca.heHE.ExestiTaSympanta.notmetered
meMA.Ode5C1T.incipit = "Virgin, you gave birth to…"
meMA.Ode5C1T.text = "Virgin, you gave birth to the pre-eternal Son ⁄ Who clothed Himself in human nature ⁄ and in His complete goodness has saved it from corruption, ⁄ and as the divinely eloquent one said in his teaching, ⁄ you remained a Virgin."

meMA.Ode6C11.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode6C11.greekmelody = he.h.m4_en_US_oca.heHE.IlthonEisTaVathi.greekname
meMA.Ode6C11.melody = he.h.m4_en_US_oca.heHE.IlthonEisTaVathi.notmetered
meMA.Ode6C11.incipit = "By Your omnipotent might, Creator…"
meMA.Ode6C11.text = "By Your omnipotent might, Creator of All, ⁄ the divinely eloquent and most excellent Matthew ⁄ was transformed from a publican into an evangelist, ⁄ as he followed You!"

meMA.Ode6C12.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode6C12.melody = he.h.m4_en_US_oca.heHE.IlthonEisTaVathi.notmetered
meMA.Ode6C12.incipit = "By the spiritually nourishing words…"
meMA.Ode6C12.text = "By the spiritually nourishing words of your Gospel, ⁄ most blessed Matthew, ⁄ a multitude of the gentiles are raised up ⁄ and directed to the lofty summit of virtues."

meMA.Ode6C13.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C13.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode6C13.melody = he.h.m4_en_US_oca.heHE.IlthonEisTaVathi.notmetered
meMA.Ode6C13.incipit = "With the divinely woven nets…"
meMA.Ode6C13.text = "With the divinely woven nets of the disciple Matthew, ⁄ the assemblies of the faithful are ever caught and raised, ⁄ guided to the knowledge of You, our Benefactor!"

meMA.Ode6C1T.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode6C1T.melody = he.h.m4_en_US_oca.heHE.IlthonEisTaVathi.notmetered
meMA.Ode6C1T.incipit = "Most immaculate one, the most…"
meMA.Ode6C1T.text = "Most immaculate one, the most excellent Matthew ⁄ announces the Gospel, ⁄ describing you as the Virgin who gave birth without seed ⁄ and without corruption ⁄ to Him Who fashions all creation!"

meMA.Ode6H.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6H.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode6H.greekmelody = he.h.m4_en_US_oca.heHE.IlthonEisTaVathi.greekname
meMA.Ode6H.melody = he.h.m4_en_US_oca.heHE.IlthonEisTaVathi.melody
