meMA.Ode9C1T.ode = misc_en_US_oca.Ode9
meMA.Ode9C1T.mode = misc_en_US_oca.Mode8
meMA.Ode9C1T.melody = he.h.m8_en_US_oca.OrousParilthes.notmetered
meMA.Ode9C1T.incipit = "…"
meMA.Ode9C1T.text = ""

meMA.Ode9C21.ode = misc_en_US_oca.Ode9
meMA.Ode9C21.mode = misc_en_US_oca.Mode8
meMA.Ode9C21.melody = he.h.m8_en_US_oca.KyriosTheotokon.notmetered
meMA.Ode9C21.incipit = "…"
meMA.Ode9C21.text = "Michael and Gabriel, chiefs of the bodiless angels, ⁄ you appear as champions for the salvation of those in wickedness, ⁄ for those sailing on the seas, or for those in need!"

meMA.Ode9C22.ode = misc_en_US_oca.Ode9
meMA.Ode9C22.mode = misc_en_US_oca.Mode8
meMA.Ode9C22.melody = he.h.m8_en_US_oca.KyriosTheotokon.notmetered
meMA.Ode9C22.incipit = "…"
meMA.Ode9C22.text = "Gabriel, rejoicing with the choirs of powers and thrones, ⁄ as chief captain of the angels and friend of God, ⁄ intercede for us with the Savior."

meMA.Ode9C23.ode = misc_en_US_oca.Ode9
meMA.Ode9C23.mode = misc_en_US_oca.Mode8
meMA.Ode9C23.melody = he.h.m8_en_US_oca.KyriosTheotokon.notmetered
meMA.Ode9C23.incipit = "…"
meMA.Ode9C23.text = "As guardians of the world and the Church, ⁄ captains of the hosts on high, ⁄ pray to the Savior for us."

meMA.Ode9C2T.ode = misc_en_US_oca.Ode9
meMA.Ode9C2T.mode = misc_en_US_oca.Mode8
meMA.Ode9C2T.melody = he.h.m8_en_US_oca.KyriosTheotokon.notmetered
meMA.Ode9C2T.incipit = "…"
meMA.Ode9C2T.text = "We glorify you with one accord, Theotokos, ⁄ that we may be delivered from passions and danger ⁄ and that our rightful prayers may be fulfilled!"

