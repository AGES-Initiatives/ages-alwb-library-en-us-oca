meMA.Ode3C22.ode = misc_en_US_oca.Ode3
meMA.Ode3C22.mode = misc_en_US_oca.Mode8
meMA.Ode3C22.melody = he.h.m8_en_US_oca.SyEiToStereoma.notmetered
meMA.Ode3C22.incipit = "…"
meMA.Ode3C22.text = "Gabriel faithfully filled the whole earth ⁄ with the radiance of divine grace ⁄ by proclaiming the descent of God in the flesh."

meMA.Ode3C23.ode = misc_en_US_oca.Ode3
meMA.Ode3C23.mode = misc_en_US_oca.Mode8
meMA.Ode3C23.melody = he.h.m8_en_US_oca.SyEiToStereoma.notmetered
meMA.Ode3C23.incipit = "…"
meMA.Ode3C23.text = "Renowned pair and immaterial couple, bright as light: ⁄ show those who faithfully celebrate your memorial ⁄ the illumination of God."

meMA.Ode3C2T.ode = misc_en_US_oca.Ode3
meMA.Ode3C2T.mode = misc_en_US_oca.Mode8
meMA.Ode3C2T.melody = he.h.m8_en_US_oca.SyEiToStereoma.notmetered
meMA.Ode3C2T.incipit = "…"
meMA.Ode3C2T.text = "Pure Virgin, Isaiah sang of your conceiving past understanding; ⁄ I also sing, that I may be purified."

