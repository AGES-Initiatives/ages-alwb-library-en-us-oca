meVE.Aposticha1.mode = miscellanea_en_US_oca.misc.Mode2
meVE.Aposticha1.greekmelody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.greekname
meVE.Aposticha1.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meVE.Aposticha1.incipit = "Let all creation…"
meVE.Aposticha1.text = "Let all creation, ⁄ now set free from bondage, ⁄ sing a new song to Christ ⁄ Who shone from the Virgin, ⁄⁄ and was baptized in the Jordan today!"

meVE.Aposticha2.mode = miscellanea_en_US_oca.misc.Mode2
meVE.Aposticha2.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meVE.Aposticha2.incipit = "The trumpet of prophets…"
meVE.Aposticha2.text = "The trumpet of prophets, ⁄ the Forerunner cried out: ⁄ Offer to the Lord ⁄ Who is baptized in the waters ⁄⁄ the fruits of repentance and good works."

meVE.Aposticha3.mode = miscellanea_en_US_oca.misc.Mode2
meVE.Aposticha3.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meVE.Aposticha3.incipit = "The Holy Spirit…"
meVE.Aposticha3.text = "The Holy Spirit ⁄ Who shines from the Father ⁄ wonderfully descended from on high ⁄ in the form of a dove ⁄⁄ and was seen to rest on Christ."

meVE.AposGlory.poet = poets_en_US_oca.po.Anatoliou
meVE.AposGlory.mode = miscellanea_en_US_oca.misc.Mode4
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "God-revealing Gregory…"
meVE.AposGlory.text = "God-revealing Gregory, ⁄ you passed your life in godliness. ⁄ You made your actions radiant through contemplation, ⁄ you desired divine wisdom for your love; ⁄ you were enriched with graces flowing from the Spirit! ⁄ Your words drip with sweetness, like honey from the comb, ⁄ you gladden the Church of God with your divine thoughts! ⁄ Now you gaze upon the Lord in heaven: ⁄⁄ Never cease to pray for us who honor your memory."

