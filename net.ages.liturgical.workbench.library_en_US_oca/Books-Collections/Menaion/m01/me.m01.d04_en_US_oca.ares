meVE.Aposticha1.mode = miscellanea_en_US_oca.misc.Mode2
meVE.Aposticha1.greekmelody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.greekname
meVE.Aposticha1.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meVE.Aposticha1.incipit = "Candlestick of the Light…"
meVE.Aposticha1.text = "Candlestick of the Light, ⁄ ray of the Sun, ⁄ Forerunner of the Word, ⁄ friend of the Bridegroom, ⁄⁄ fulfill your ministry, O Prophet!"

meVE.Aposticha2.mode = miscellanea_en_US_oca.misc.Mode2
meVE.Aposticha2.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meVE.Aposticha2.incipit = "Our illumination…"
meVE.Aposticha2.text = "Our illumination draws near; ⁄ our salvation is dawning! ⁄ Come, let us assemble ⁄ to be cleansed in Jordan, ⁄⁄ singing the hymns of the Prefeast!"

meVE.Aposticha3.mode = miscellanea_en_US_oca.misc.Mode2
meVE.Aposticha3.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meVE.Aposticha3.incipit = "You assume…"
meVE.Aposticha3.text = "You assume, O God made Man, ⁄ all my nature ⁄ from the Theotokos. ⁄ Now you make me radiant ⁄⁄ in the Jordan."

