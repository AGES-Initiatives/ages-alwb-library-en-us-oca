meMA.AposGlory.mode = miscellanea_en_US_oca.misc.Mode2
meMA.AposGlory.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meMA.AposGlory.incipit = "Today the Lord’s Anointed…"
meMA.AposGlory.text = "Today the Lord’s Anointed ⁄ grants to men salvation ⁄ and remission of sins: ⁄ souls are cleansed of filth, ⁄⁄ by the fire of baptism."

