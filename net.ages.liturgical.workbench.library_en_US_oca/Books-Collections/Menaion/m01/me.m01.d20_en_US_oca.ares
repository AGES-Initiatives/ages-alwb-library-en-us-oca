meVE.AposTheotokion.incipit = oc.m5.d3_en_US_oca.ocVE.AposTheotokion.incipit
meVE.AposTheotokion.text = oc.m5.d3_en_US_oca.ocVE.AposTheotokion.text

meVE.Apolytikion1.mode = miscellanea_en_US_oca.misc.Mode4
meVE.Apolytikion1.melody = ""
meVE.Apolytikion1.incipit = "Rejoice, barren wilderness! ⁄ Be…"
meVE.Apolytikion1.text = "Rejoice, barren wilderness! ⁄ Be glad, sterile desert, that has never known the travail of birth! ⁄ The man of desires has multiplied your sons; ⁄ he has planted them in faith and piety. ⁄ He has watered them with the Holy Spirit: ⁄ They grow in self-denial and perfect virtue. ⁄⁄ Through his intercessions, Christ our God, preserve Your people in peace!"

meMA.Kathisma11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Kathisma11.melody = he.a.m4_en_US_oca.heAU.EpefanisSimeron.notmetered
meMA.Kathisma11.incipit = "Shining with unapproachable light, ⁄…"
meMA.Kathisma11.text = "Shining with unapproachable light, ⁄ you are radiant like a star in the deserts, ⁄ enlightening with your teachings ⁄ those who approach you with firmness of soul, ⁄⁄ Father Euthymius."

meMA.Kathisma12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Kathisma12.melody = he.a.m4_en_US_oca.heAU.EpefanisSimeron.notmetered
meMA.Kathisma12.incipit = "…"
meMA.Kathisma12.text = ""

meMA.Kathisma1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Kathisma1T.melody = he.a.m4_en_US_oca.heAU.EpefanisSimeron.notmetered
meMA.Kathisma1T.incipit = "Accept our entreaty as we…"
meMA.Kathisma1T.text = "Accept our entreaty as we turn to your protection, Virgin, ⁄ and never cease to pray to the Lover of mankind, ⁄⁄ that He may save His servants."

meMA.Kathisma21.mode = miscellanea_en_US_oca.misc.Mode5
meMA.Kathisma21.melody = he.a.m5_en_US_oca.heAU.TonSynanarchonLogon.notmetered
meMA.Kathisma21.incipit = "You rejected the cares of…"
meMA.Kathisma21.text = "You rejected the cares of life ⁄ and embraced the angelic life, ⁄ adorning your soul with abstinence. ⁄ You richly received grace from God, God-bearing Euthymius. ⁄⁄ Pray on behalf of us who praise you with devotion."

meMA.Kathisma2T.mode = miscellanea_en_US_oca.misc.Mode5
meMA.Kathisma2T.melody = he.a.m5_en_US_oca.heAU.TonSynanarchonLogon.notmetered
meMA.Kathisma2T.incipit = "Have mercy, most holy Virgin…"
meMA.Kathisma2T.text = "Have mercy, most holy Virgin, ⁄ on us who turn to you with faith, ⁄ for we ask for your fervent help, compassionate one. ⁄ As the good Mother of God most high, ⁄ you are filled with the grace of God. ⁄ You ever pray with the supplications of a Mother ⁄⁄ that He may save us all!"

