meVE.Stichera01.mode = miscellanea_en_US_oca.misc.Mode5
meVE.Stichera01.melody = he.a.m5_en_US_oca.heAU.OsiePater.melody
meVE.Stichera01.incipit = he.a.m5_en_US_oca.heAU.OsiePater.incipit
