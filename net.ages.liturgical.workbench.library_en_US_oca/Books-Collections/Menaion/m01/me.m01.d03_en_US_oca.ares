meVE.SticGlory.poet = poets_en_US_oca.po.IoannouMonachou
meVE.SticGlory.mode = miscellanea_en_US_oca.misc.Mode4
meVE.SticGlory.melody = ""
meVE.SticGlory.incipit = "Most strange wonder…"
meVE.SticGlory.text = "Most strange wonder! ⁄ He who baptizes by the Holy Spirit and by fire ⁄ comes to be baptized by John in the Jordan. ⁄ God is stripped naked as a Man: ⁄ In both His natures, He is one only-begotten Son. ⁄ In His humanity, He asks a servant to baptize Him; ⁄ in His divinity, He takes away the sins of the world ⁄⁄ granting great mercy to all."

meVE.Aposticha1.mode = miscellanea_en_US_oca.misc.Mode6
meVE.Aposticha1.greekmelody = he.a.m6_en_US_oca.heAU.TriimerosAnestis.greekname
meVE.Aposticha1.melody = he.a.m6_en_US_oca.heAU.TriimerosAnestis.notmetered
meVE.Aposticha1.incipit = "Behold, the light…"
meVE.Aposticha1.text = "Behold, the light of the faithful, ⁄ see, our Atonement descends into the river’s streams ⁄ that He may wash away man’s filth and corruption, ⁄⁄ refashioning us, who had been broken to pieces."

meVE.Aposticha2.mode = miscellanea_en_US_oca.misc.Mode6
meVE.Aposticha2.melody = he.a.m6_en_US_oca.heAU.TriimerosAnestis.notmetered
meVE.Aposticha2.incipit = "Lord, Your Forerunner…"
meVE.Aposticha2.text = "Lord, Your Forerunner cried out in fear ⁄ when he saw You coming to demand baptism: ⁄ My God! My Creator! ⁄⁄ How can I baptize You, sinless One!"

meVE.Aposticha3.mode = miscellanea_en_US_oca.misc.Mode6
meVE.Aposticha3.melody = he.a.m6_en_US_oca.heAU.TriimerosAnestis.notmetered
meVE.Aposticha3.incipit = "Faithful, let us assemble…"
meVE.Aposticha3.text = "Faithful, let us assemble in spirit, ⁄ at the streams of Jordan, ⁄ that we may behold a great and mighty wonder! ⁄ We shall see the Creator of all made manifest, ⁄⁄ as He comes to be baptized."

meVE.AposGlory.poet = poets_en_US_oca.po.Vyzantiou
meVE.AposGlory.mode = miscellanea_en_US_oca.misc.Mode5
meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "Christ our God comes…"
meVE.AposGlory.text = "Christ our God comes to the river Jordan. ⁄ He will be baptized, wishing to cleanse us from our sins. ⁄⁄ He is made manifest as the only good Lover of mankind."

