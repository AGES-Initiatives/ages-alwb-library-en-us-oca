meCO.Ode9C11.ode = miscellanea_en_US_oca.misc.Ode9
meCO.Ode9C11.mode = miscellanea_en_US_oca.misc.Mode2
meCO.Ode9C11.melody = he.h.m2_en_US_oca.heHE.PsychaisKatharais.notmetered
meCO.Ode9C11.incipit = "Let no one in ignorance…"
meCO.Ode9C11.text = "Let no one in ignorance be ungrateful to Christ! ⁄ Let no one insult His kindness and long-suffering, ⁄ but serve Him in fear and joy, singing to Him: ⁄ Spare our souls, O Christ our God, and save us!"

meCO.Ode9C12.ode = miscellanea_en_US_oca.misc.Ode9
meCO.Ode9C12.mode = miscellanea_en_US_oca.misc.Mode2
meCO.Ode9C12.melody = he.h.m2_en_US_oca.heHE.PsychaisKatharais.notmetered
meCO.Ode9C12.incipit = "…"
meCO.Ode9C12.text = "Proclaim to the faithless people, Forerunner: ⁄ Generation of vipers, who told you to flee the wrath to come? ⁄ Bear the fruits of repentance! ⁄ Show yourselves worthy to cry: ⁄ Spare our souls, O Christ our God, and save us!"

meCO.Ode9C13.ode = miscellanea_en_US_oca.misc.Ode9
meCO.Ode9C13.mode = miscellanea_en_US_oca.misc.Mode2
meCO.Ode9C13.melody = he.h.m2_en_US_oca.heHE.PsychaisKatharais.notmetered
meCO.Ode9C13.incipit = "…"
meCO.Ode9C13.text = "Blind and shameless ignorance! ⁄ Strangers worship Christ as Redeemer, ⁄ honoring Him with gifts, ⁄ but His own household seeks to kill Him! ⁄ Spare our souls from this madness, ⁄ and save us, O Christ our God!"

