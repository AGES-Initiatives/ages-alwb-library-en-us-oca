meCO.Ode1C11.ode = miscellanea_en_US_oca.misc.Ode1
meCO.Ode1C11.mode = miscellanea_en_US_oca.misc.Mode2
meCO.Ode1C11.melody = he.h.m2_en_US_oca.heHE.ToTinAvaton.notmetered
meCO.Ode1C11.incipit = "The ineffable descent…"
meCO.Ode1C11.text = "The ineffable descent of the Creator ⁄ has shone forth from the Virgin: ⁄ Now Christ enlightens the world again! ⁄ He hastens to fulfill the mystery, ⁄ drawing near the Jordan as a mortal man, ⁄ to accomplish a strange regeneration."

meCO.Ode1C12.ode = miscellanea_en_US_oca.misc.Ode1
meCO.Ode1C12.mode = miscellanea_en_US_oca.misc.Mode2
meCO.Ode1C12.melody = he.h.m2_en_US_oca.heHE.ToTinAvaton.notmetered
meCO.Ode1C12.incipit = "…"
meCO.Ode1C12.text = "The Forerunner advances to the Jordan. ⁄ He obeys the voice of the Word, ⁄ performing a fearful service! ⁄ Christ comes to enrich needy Adam ⁄ with all the riches of the Godhead, ⁄ bestowed by the Holy Spirit through the gift of Baptism."

