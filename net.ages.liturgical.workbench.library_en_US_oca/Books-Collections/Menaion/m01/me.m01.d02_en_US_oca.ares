meMA.Ode6C11.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C11.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Ode6C11.greekmelody = he.h.m2_en_US_oca.heHE.AvyssosAmartimaton.greekname
meMA.Ode6C11.melody = he.h.m2_en_US_oca.heHE.AvyssosAmartimaton.notmetered
meMA.Ode6C11.incipit = "Lover of mankind…"
meMA.Ode6C11.text = "Lover of mankind, like rain into a fleece of wool, ⁄ You drop down remission of our offences ⁄ as You drowned the transgressions of those born on earth ⁄ in the streams of Jordan, in Your great mercy!"

meMA.Ode6C12.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C12.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Ode6C12.melody = he.h.m2_en_US_oca.heHE.AvyssosAmartimaton.notmetered
meMA.Ode6C12.incipit = "…"
meMA.Ode6C12.text = "You are an abyss of righteousness, O Christ, ⁄ and now You come to the River Jordan ⁄ to drown the serpent and wash away the sin of Adam."

meMA.Ode6C13.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C13.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Ode6C13.melody = he.h.m2_en_US_oca.heHE.AvyssosAmartimaton.notmetered
meMA.Ode6C13.incipit = "…"
meMA.Ode6C13.text = "How can the river’s streams hold You, O Christ, ⁄ for You are truly a stream of incorruption! ⁄ Cried the Forerunner, glorifying Your great compassion."

