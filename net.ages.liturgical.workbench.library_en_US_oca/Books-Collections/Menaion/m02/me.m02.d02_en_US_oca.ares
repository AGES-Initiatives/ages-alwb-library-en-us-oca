meSV.Aposticha1.mode = miscellanea_en_US_oca.misc.Mode2
meSV.Aposticha1.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meSV.Aposticha1.incipit = "Today the Savior, ⁄ carried…"
meSV.Aposticha1.text = "Today the Savior, ⁄ carried as an Infant, ⁄ is brought into the Temple of the Lord, ⁄ and with aged arms, ⁄⁄ the Elder receives Him."

meSV.Aposticha2.mode = miscellanea_en_US_oca.misc.Mode2
meSV.Aposticha2.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meSV.Aposticha2.incipit = "Christ, the Coal of fire…"
meSV.Aposticha2.text = "Christ, the Coal of fire ⁄ whom holy Isaiah foresaw, ⁄ now rests in the arms of the Theotokos ⁄ as in a pair of tongs. ⁄⁄ Now He is given to the Elder!"

meSV.Aposticha3.mode = miscellanea_en_US_oca.misc.Mode2
meSV.Aposticha3.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meSV.Aposticha3.incipit = "In fear and joy, Simeon…"
meSV.Aposticha3.text = "In fear and joy, Simeon ⁄ held the Master in his arms ⁄ and asked release from life ⁄ singing the praises ⁄⁄ of the Mother of God!"

meSV.AposGlory.mode = miscellanea_en_US_oca.misc.Mode2
meSV.AposGlory.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meSV.AposGlory.incipit = "Receive Him, O Simeon, ⁄…"
meSV.AposGlory.text = "Receive Him, O Simeon, ⁄ Cried the all-pure Virgin. ⁄ Bear Him in your arms: ⁄ He is the Lord of glory, ⁄⁄ and the Salvation of the world!"

