meVE.AposGlory.melody = ""
meVE.AposGlory.incipit = "Faithful, let us fittingly bless…"
meVE.AposGlory.text = "Faithful, let us fittingly bless the great and noble martyr Theodore, ⁄ for he is the namesake of the gifts of God ⁄ and the heir of His blessedness; ⁄⁄ the champion of the world who prays to God for us!"

