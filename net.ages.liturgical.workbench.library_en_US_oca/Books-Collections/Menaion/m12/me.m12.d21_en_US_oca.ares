meCO.Ode8C11.ode = miscellanea_en_US_oca.misc.Ode8
meCO.Ode8C11.mode = miscellanea_en_US_oca.misc.Mode2
meCO.Ode8C11.melody = he.h.m2_en_US_oca.heHE.ToDogmatiToTyranniko.notmetered
meCO.Ode8C11.incipit = "Deliver us from the sleep…"
meCO.Ode8C11.text = "Deliver us from the sleep of heedlessness; ⁄ preserve our souls in vigilance, Christ. ⁄ Through the night we will raise the song of praise, ⁄ singing to You who comes forth from the Virgin womb, ⁄ Bless the Lord, O works of the Lord!"

meCO.Ode8C12.ode = miscellanea_en_US_oca.misc.Ode8
meCO.Ode8C12.mode = miscellanea_en_US_oca.misc.Mode2
meCO.Ode8C12.melody = he.h.m2_en_US_oca.heHE.ToDogmatiToTyranniko.notmetered
meCO.Ode8C12.incipit = "…"
meCO.Ode8C12.text = "Impart good works to the manger of our souls, O Lord, ⁄ that with faces bright and shining, ⁄ we may sing to You who comes forth to be born, ⁄ Bless the Lord, O works of the Lord!"

meCO.Ode8C13.ode = miscellanea_en_US_oca.misc.Ode8
meCO.Ode8C13.mode = miscellanea_en_US_oca.misc.Mode2
meCO.Ode8C13.melody = he.h.m2_en_US_oca.heHE.ToDogmatiToTyranniko.notmetered
meCO.Ode8C13.incipit = "…"
meCO.Ode8C13.text = "Increase in us the talent of good deeds, ⁄ that we may offer You fitting gifts: ⁄ Instead of gold, myrrh, and frankincense, ⁄ we offer the service of loving hearts, ⁄ praising the Giver of all good gifts ⁄ Who comes to be born of the Virgin and Child of God."

meCO.Ode9C1H.ode = miscellanea_en_US_oca.misc.Ode9
meCO.Ode9C1H.mode = miscellanea_en_US_oca.misc.Mode2
meCO.Ode9C1H.melody = he.h.m2_en_US_oca.heHE.ITonAchoritonTheon.melody
meCO.Ode9C1H.incipit = he.h.m2_en_US_oca.heHE.ITonAchoritonTheon.incipit
meCO.Ode9C1H.text = he.h.m2_en_US_oca.heHE.ITonAchoritonTheon.text

meCO.Ode9C11.ode = miscellanea_en_US_oca.misc.Ode9
meCO.Ode9C11.mode = miscellanea_en_US_oca.misc.Mode2
meCO.Ode9C11.melody = he.h.m2_en_US_oca.heHE.ITonAchoritonTheon.notmetered
meCO.Ode9C11.incipit = "Christ our Judge…"
meCO.Ode9C11.text = "Christ our Judge commands us to be vigilant; ⁄ we wait expectantly for His visitation, ⁄ for He comes to be born from the Virgin."

meCO.Ode9C12.ode = miscellanea_en_US_oca.misc.Ode9
meCO.Ode9C12.mode = miscellanea_en_US_oca.misc.Mode2
meCO.Ode9C12.melody = he.h.m2_en_US_oca.heHE.ITonAchoritonTheon.notmetered
meCO.Ode9C12.incipit = "…"
meCO.Ode9C12.text = "At Your fearful second coming, O Christ, ⁄ number me with the sheep at Your right hand, ⁄ for You took up Your abode in the flesh to save us."

meCO.Ode9C13.ode = miscellanea_en_US_oca.misc.Ode9
meCO.Ode9C13.mode = miscellanea_en_US_oca.misc.Mode2
meCO.Ode9C13.melody = he.h.m2_en_US_oca.heHE.ITonAchoritonTheon.notmetered
meCO.Ode9C13.incipit = "…"
meCO.Ode9C13.text = "At Your first coming to us, O Christ, ⁄ You desired to save the race of Adam. ⁄ When You will come again to judge us, ⁄ show mercy to those who honor Your holy birth."

meMA.Kathisma11.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Kathisma11.greekmelody = ""
meMA.Kathisma11.melody = ""
meMA.Kathisma11.incipit = "The unapproachable God…"
meMA.Kathisma11.text = "The unapproachable God in His tender mercy approaches me; ⁄ He wills to be born in the flesh in the city of Bethlehem. ⁄ He receives human nature from a Virgin maiden. ⁄ Let us eagerly run to greet Him, crying out in fear: ⁄⁄ Glory to You, O Lord!"

meMA.Kathisma21.mode = miscellanea_en_US_oca.misc.Mode3
meMA.Kathisma21.greekmelody = he.a.m3_en_US_oca.heAU.TinOraiotita.greekname
meMA.Kathisma21.melody = he.a.m3_en_US_oca.heAU.TinOraiotita.notmetered
meMA.Kathisma21.incipit = "A strange mystery…"
meMA.Kathisma21.text = "A strange mystery is revealed today: ⁄ In the cave our Savior presses forward to be born in the flesh! ⁄ The Magi are coming with gifts to worship the King; ⁄ the shepherds glorify Him with the angels! ⁄ Together with them let us cry aloud, ⁄⁄ Glory to Him Who for our sake has become a man!"

