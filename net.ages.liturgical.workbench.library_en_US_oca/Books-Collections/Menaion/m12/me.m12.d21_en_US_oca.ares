meVE.SticGlory.mode = miscellanea_en_US_oca.misc.Mode6
meVE.SticGlory.melody = ""
meVE.SticGlory.incipit = "Triumph, Zion…"
meVE.SticGlory.text = "Triumph, Zion! ⁄ Make glad, Jerusalem, city of Christ our God! ⁄ Receive the Creator contained within a cave and a manger. ⁄ Open the gates to me; ⁄ I will enter to see a Child wrapped in swaddling clothes ⁄ Who upholds the creation in the palm of His hand, ⁄ Whose praises the angels sing with unceasing voice: ⁄⁄ The Lord and Giver of life who saves the human race."

meVE.Aposticha1.mode = miscellanea_en_US_oca.misc.Mode2
meVE.Aposticha1.greekmelody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.greekname
meVE.Aposticha1.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meVE.Aposticha1.incipit = "…"
meVE.Aposticha1.text = "The prophecy ⁄ of all the prophets is fulfilled: ⁄ Christ is being born ⁄ in the city of Bethlehem ⁄⁄ from a pure handmaiden of God."

meVE.Aposticha2.mode = miscellanea_en_US_oca.misc.Mode2
meVE.Aposticha2.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meVE.Aposticha2.incipit = "…"
meVE.Aposticha2.text = "O honorable Bethlehem, ⁄ the divine city, ⁄ glory of the human race, ⁄ our joy and our praise, ⁄⁄ receive now your Creator!"

meVE.Aposticha3.mode = miscellanea_en_US_oca.misc.Mode2
meVE.Aposticha3.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meVE.Aposticha3.incipit = "…"
meVE.Aposticha3.text = "The Word of the Father, ⁄ by whom all things were created, ⁄ is beheld today ⁄ as one Person in two natures ⁄⁄ without division or change."

meVE.AposGlory.mode = miscellanea_en_US_oca.misc.Mode2
