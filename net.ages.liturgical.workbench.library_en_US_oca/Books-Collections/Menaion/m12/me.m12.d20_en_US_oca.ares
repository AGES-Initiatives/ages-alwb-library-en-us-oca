meVE.Aposticha1.poet = poets_en_US_oca.Kyprianou
meVE.Aposticha1.mode = misc_en_US_oca.Mode2
meVE.Aposticha1.melody = ""
meVE.Aposticha1.incipit = "Bethlehem, land of Judah…"
meVE.Aposticha1.text = "Bethlehem, land of Judah, ⁄ prepare the divine cavern ⁄ as a dwelling for God to be born in the flesh ⁄ from a Virgin who knew not a man ⁄⁄ to save the human race."

meVE.Aposticha2.poet = poets_en_US_oca.AndreouPyrou
meVE.Aposticha2.mode = misc_en_US_oca.Mode2
meVE.Aposticha2.melody = ""
meVE.Aposticha2.incipit = "Come, all you faithful…"
meVE.Aposticha2.text = "Come, all you faithful, ⁄ begin the celebration: ⁄ Sing with the Magi and shepherds: ⁄ Salvation comes from the Virgin's womb, ⁄⁄ recalling the faithful to life!"

meVE.Aposticha3.poet = poets_en_US_oca.Kyprianou
