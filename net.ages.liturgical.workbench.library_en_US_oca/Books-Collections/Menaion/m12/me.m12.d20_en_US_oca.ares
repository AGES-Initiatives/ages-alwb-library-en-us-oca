meCO.Ode8C11.ode = misc_en_US_oca.Ode8
meCO.Ode8C11.mode = misc_en_US_oca.Mode2
meCO.Ode8C11.melody = he.h.m2_en_US_oca.EfrixePaidonEvagon.notmetered
meCO.Ode8C11.incipit = "If you keep My commandments…"
meCO.Ode8C11.text = "If you keep My commandments, you are My brothers, said Christ; ⁄ He proceeds from the womb of the Pure One giving peace to all. ⁄ He appears in humility, teaching all to recognize Him as Lord, ⁄ blessing Him throughout all ages."

meCO.Ode8C12.ode = misc_en_US_oca.Ode8
meCO.Ode8C12.mode = misc_en_US_oca.Mode2
meCO.Ode8C12.melody = he.h.m2_en_US_oca.EfrixePaidonEvagon.notmetered
meCO.Ode8C12.incipit = "Word, made flesh contrary to…"
meCO.Ode8C12.text = "Word, made flesh contrary to Your nature, ⁄ Your holy Mother provides a body for You, ⁄ not by the will of flesh and blood, ⁄ but by the coming of the Holy Spirit ⁄ and the overshadowing of the Most High. ⁄ Knowing this, we praise You, O Lord, ⁄ blessing You throughout all ages."

meCO.Ode9C1H.ode = misc_en_US_oca.Ode9
meCO.Ode9C1H.mode = misc_en_US_oca.Mode2
meCO.Ode9C1H.melody = misc_en_US_oca.HeirmosNotMetered
meCO.Ode9C1H.incipit = he.h.m2_en_US_oca.EmegalynasChriste.incipit
meCO.Ode9C1H.text = he.h.m2_en_US_oca.EmegalynasChriste.text

meCO.Ode9C11.ode = misc_en_US_oca.Ode9
meCO.Ode9C11.mode = misc_en_US_oca.Mode2
meCO.Ode9C11.melody = he.h.m2_en_US_oca.EmegalynasChriste.notmetered
meCO.Ode9C11.incipit = "Let us reject the corruption…"
meCO.Ode9C11.text = "Let us reject the corruption of the passions, ⁄ awaiting the visitation of Christ. ⁄ Let us come to our senses and receive knowledge: ⁄ the gift of the undefiled Lord who comes to be clothed in our flesh, ⁄ refashioning us in Godliness."

meCO.Ode9C12.ode = misc_en_US_oca.Ode9
meCO.Ode9C12.mode = misc_en_US_oca.Mode2
meCO.Ode9C12.melody = he.h.m2_en_US_oca.EmegalynasChriste.notmetered
meCO.Ode9C12.incipit = "Beholding Christ, let us be…"
meCO.Ode9C12.text = "Beholding Christ, let us be exalted through humility; ⁄ let us abandon earthly passions with zeal for good; ⁄ let us learn through faith not to be proud of heart; ⁄ let us humiliate ourselves in Spirit, ⁄ that by good deeds we may exalt Him who comes to be born."

meMA.Kathisma11.mode = misc_en_US_oca.Mode1
meMA.Kathisma11.melody = he.a.m1_en_US_oca.TonTafonSouSotir.notmetered
meMA.Kathisma11.incipit = "He has bowed the heavens…"
meMA.Kathisma11.text = "He has bowed the heavens and taken up his abode in the Virgin; ⁄ as it is written, He comes to be born in our flesh in the cave of Bethlehem; ⁄ He Who gives life to children in the womb Himself becomes a Child! ⁄⁄ Let us all go forth to meet Him, our hearts filled with fervor and joy!"

meMA.Kathisma21.mode = misc_en_US_oca.Mode1
meMA.Kathisma21.melody = he.a.m1_en_US_oca.TonTafonSouSotir.notmetered
meMA.Kathisma21.incipit = "Rejoice, Zion!…"
meMA.Kathisma21.text = "Rejoice, Zion! Adorn yourself, O Bethlehem! ⁄ for the Sustainer of all has sent forth a star! ⁄ He has announced His infinite condescension ⁄ before which the hosts of heaven tremble. ⁄⁄ The only Lover of mankind is indeed born without change of the Virgin!"

