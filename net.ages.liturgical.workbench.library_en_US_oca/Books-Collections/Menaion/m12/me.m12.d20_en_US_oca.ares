meCO.Ode1C11.ode = misc_en_US_oca.Ode1
meCO.Ode1C11.mode = misc_en_US_oca.Mode2
meCO.Ode1C11.melody = he.h.m2_en_US_oca.ToTinAvaton.notmetered
meCO.Ode1C11.incipit = "The ineffable descent of the…"
meCO.Ode1C11.text = "The ineffable descent of the Word of God ⁄ reveals Him as the Christ, both God and Man. ⁄ He comes forth from a pure maiden, ⁄ assuming the form of a servant, ⁄ for He thought it not robbery to be called God."

meCO.Ode1C12.ode = misc_en_US_oca.Ode1
meCO.Ode1C12.mode = misc_en_US_oca.Mode2
meCO.Ode1C12.melody = he.h.m2_en_US_oca.ToTinAvaton.notmetered
meCO.Ode1C12.incipit = "Christ comes willingly to serve…"
meCO.Ode1C12.text = "Christ comes willingly to serve us; ⁄ the Creator puts on the form of a creature. ⁄ He who is rich in divine mercy ⁄ bestows upon needy Adam a new creation and birth."

