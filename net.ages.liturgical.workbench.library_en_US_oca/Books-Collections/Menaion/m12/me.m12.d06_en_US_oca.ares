meMA.Kathisma21.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Kathisma21.greekmelody = he.a.m4_en_US_oca.heAU.EpefanisSimeron.greekname
meMA.Kathisma21.melody = he.a.m4_en_US_oca.heAU.EpefanisSimeron.notmetered
meMA.Kathisma21.incipit = "Blessed Saint Nicholas, ⁄ you…"
meMA.Kathisma21.text = "Blessed Saint Nicholas, ⁄ you make supplication for the faithful, ⁄ protecting and guarding them; ⁄ clearly delivering them from every affliction, ⁄⁄ as the most eminent glory of bishops!"

meMA.Kathisma2T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Kathisma2T.melody = he.a.m4_en_US_oca.heAU.EpefanisSimeron.notmetered
meMA.Kathisma2T.incipit = //me.m07.d05_en_US_oca.meMA.Kathisma2T.incipit
meMA.Kathisma2T.text = //me.m07.d05_en_US_oca.meMA.Kathisma2T.text "You are the invincible advocate of those in need ⁄ and the ready intercessor for those who seek you. ⁄ Deliver me from danger ⁄ and do not turn away from me, ⁄⁄ for you are the helper of all!"

