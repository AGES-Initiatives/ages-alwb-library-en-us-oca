meMA.FestalMagnification.incipit = "…"
meMA.FestalMagnification.text = "We magnify, ⁄ we magnify you, ⁄ holy Hierarch Father Nicholas! ⁄ And we honor your holy memory, ⁄ for you pray to Christ our God for us!"

