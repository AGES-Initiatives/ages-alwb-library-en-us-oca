meVE.Aposticha1.mode = miscellanea_en_US_oca.misc.Mode2
meVE.Aposticha1.greekmelody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.greekname
meVE.Aposticha1.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meVE.Aposticha1.incipit = "Christ is born on earth…"
meVE.Aposticha1.text = "Christ is born on earth ⁄ to crush the power of evil, ⁄ to enlighten those in darkness, ⁄ and to free the captives. ⁄⁄ Let us go forth to meet Him."

meVE.Aposticha2.mode = miscellanea_en_US_oca.misc.Mode2
meVE.Aposticha2.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meVE.Aposticha2.incipit = "The families of nations…"
meVE.Aposticha2.text = "The families of nations ⁄ offer glory and praise. ⁄ The Magi come with gifts; ⁄ the shepherds watch in the fields, ⁄⁄ joyfully preparing the way."

meVE.Aposticha3.mode = miscellanea_en_US_oca.misc.Mode2
meVE.Aposticha3.melody = he.a.m2_en_US_oca.heAU.OikosTouEfratha.notmetered
meVE.Aposticha3.incipit = "Hills and mountains…"
meVE.Aposticha3.text = "Hills and mountains, ⁄ plains and valleys, ⁄ rivers and all creation, ⁄ magnify your Lord, ⁄⁄ Who comes to be born for your sake."

