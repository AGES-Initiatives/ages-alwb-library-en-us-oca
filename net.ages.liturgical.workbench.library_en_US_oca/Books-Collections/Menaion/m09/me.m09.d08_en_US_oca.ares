meSV.Stichera1.mode = miscellanea_en_US_oca.misc.Mode1
meSV.Stichera1.melody = he.a.m1_en_US_oca.heAU.TonOuranionTagmaton.notmetered
meSV.Stichera1.incipit = "Joachim and Anna celebrate the…"
meSV.Stichera1.text = "Joachim and Anna celebrate the feast, ⁄ for they gave birth to the only Theotokos, ⁄ the beginning of our salvation! ⁄ Together with them we also celebrate today: ⁄⁄ We bless the pure Virgin who sprang from the root of Jesse!"

meSV.Stichera2.mode = miscellanea_en_US_oca.misc.Mode1
meSV.Stichera2.melody = he.a.m1_en_US_oca.heAU.TonOuranionTagmaton.notmetered
