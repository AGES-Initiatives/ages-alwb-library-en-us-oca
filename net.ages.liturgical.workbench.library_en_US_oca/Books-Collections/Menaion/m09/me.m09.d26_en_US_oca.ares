meMA.Ode4C13.text = me.m07.d27_en_US_oca.meMA.Ode4C13.text

meMA.Ode4C21.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C21.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Ode4C21.greekmelody = he.h.m2_en_US_oca.heHE.ElilythasEkParthenou.greekname
meMA.Ode4C21.melody = he.h.m2_en_US_oca.heHE.ElilythasEkParthenou.notmetered
meMA.Ode4C21.incipit = "The Word rightly manifested…"
meMA.Ode4C21.text = "The Word rightly manifested you as the Theologian, ⁄ initiating you into the mystery of divinity, ⁄ and teaching you the ineffable dispensation of humanity."

meMA.Ode4C22.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C22.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Ode4C22.melody = he.h.m2_en_US_oca.heHE.ElilythasEkParthenou.notmetered
meMA.Ode4C22.incipit = "Acquiring a divine mind…"
meMA.Ode4C22.text = "Acquiring a divine mind and virgin body, ⁄ you were a living temple and dwelling of the all-praised and holy Trinity, ⁄ living and glorious Saint!"

meMA.Ode4C23.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C23.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Ode4C23.melody = he.h.m2_en_US_oca.heHE.ElilythasEkParthenou.notmetered
meMA.Ode4C23.incipit = "Blessed virgin John…"
meMA.Ode4C23.text = "Blessed virgin John, ⁄ you were honored by being made the Son of the most holy and pure Virgin, ⁄ thus showing yourself to be the brother of the One Who chose you, ⁄ making you a perfect disciple and divine Theologian."

meMA.Ode4C2T.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C2T.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Ode4C2T.melody = he.h.m2_en_US_oca.heHE.ElilythasEkParthenou.notmetered
meMA.Ode4C2T.incipit = "The supreme God…"
meMA.Ode4C2T.text = "The supreme God went to dwell in you, spotless one, ⁄ healing the ancient transgression of Eve, ⁄ and fashioning me anew, who had completely fallen."

meMA.Ode4C31.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C31.mode = miscellanea_en_US_oca.misc.Mode6
meMA.Ode4C31.greekmelody = he.h.m6_en_US_oca.heHE.ChristosMouDynamis.greekname
meMA.Ode4C31.melody = he.h.m6_en_US_oca.heHE.ChristosMouDynamis.notmetered
meMA.Ode4C31.incipit = "You watered all things…"
meMA.Ode4C31.text = "You watered all things beneath the sun, glorious Saint, ⁄ drying up the muddy waters of ungodliness. ⁄ Therefore we honor you!"

meMA.Ode4C32.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C32.mode = miscellanea_en_US_oca.misc.Mode6
meMA.Ode4C32.melody = he.h.m6_en_US_oca.heHE.ChristosMouDynamis.notmetered
meMA.Ode4C32.incipit = "Gird my soul…"
meMA.Ode4C32.text = "Gird my soul with strength and power, ⁄ I entreat you, most wise one, ⁄ for I am weakened by many afflictions from unclean enemies, ⁄ and I hasten to you for protection."

meMA.Ode4C33.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C33.mode = miscellanea_en_US_oca.misc.Mode6
meMA.Ode4C33.melody = he.h.m6_en_US_oca.heHE.ChristosMouDynamis.notmetered
meMA.Ode4C33.incipit = "As you were a temple…"
meMA.Ode4C33.text = "As you were a temple of the Holy Spirit, Theologian, ⁄ by your mediation, ⁄ manifest those who ever pray with faith in your divine Church ⁄ to be temples of God indeed."

meMA.Ode4C3T.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C3T.mode = miscellanea_en_US_oca.misc.Mode6
meMA.Ode4C3T.melody = he.h.m6_en_US_oca.heHE.ChristosMouDynamis.notmetered
meMA.Ode4C3T.incipit = "Let us sing the praises…"
meMA.Ode4C3T.text = "Let us sing the praises of the divine temple of God, the holy Virgin, ⁄ and through her, let us all become divine, ⁄ wholly delivered from tribulation."

meMA.Ode5C1H.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_en_US_oca.misc.Mode2
