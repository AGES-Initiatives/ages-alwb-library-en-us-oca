meVE.AposGlory.mode = miscellanea_en_US_oca.misc.Mode8
meVE.AposGlory.melody = miscellanea_en_US_oca.misc.Idiomelon
meVE.AposGlory.incipit = "Behold Elizabeth, who said to…"
meVE.AposGlory.text = "Behold Elizabeth, who said to the Virgin Mary: ⁄ Mother of my Lord, why have you come to me? ⁄ You bear the King and I the warrior. ⁄ You, the Law-giver; I, the transmitter of the law. ⁄⁄ You, the Word and I, the voice proclaiming the Kingdom of heaven!" //me.m06.d24_en_US_oca.meVE.AposBoth.text

