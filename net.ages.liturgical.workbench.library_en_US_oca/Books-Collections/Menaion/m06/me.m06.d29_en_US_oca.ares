meMA.Ode3C11.ode = miscellanea_en_US_oca.misc.Ode3
meMA.Ode3C11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode3C11.greekmelody = he.h.m4_en_US_oca.heHE.OukEnSofia.greekname
meMA.Ode3C11.melody = he.h.m4_en_US_oca.heHE.OukEnSofia.notmetered
meMA.Ode3C11.incipit = "Christ showed you to be…"
meMA.Ode3C11.text = "Christ showed you to be blessed, ⁄ sweetest spokesman of Christ our God ⁄ and the safe-keeper of the Kingdom. ⁄ Therefore we sing your praises, Peter the Apostle!"

meMA.Ode3C12.ode = miscellanea_en_US_oca.misc.Ode3
meMA.Ode3C12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode3C12.melody = he.h.m4_en_US_oca.heHE.OukEnSofia.notmetered
meMA.Ode3C12.incipit = "The Master Christ has built…"
meMA.Ode3C12.text = "The Master Christ has built the unshakable Church ⁄ on the rock of your Divine teaching ⁄ in which we glorify you, Peter."

meMA.Ode3C13.ode = miscellanea_en_US_oca.misc.Ode3
meMA.Ode3C13.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode3C13.melody = he.h.m4_en_US_oca.heHE.OukEnSofia.notmetered
meMA.Ode3C13.incipit = "As Peter in the flesh…"
meMA.Ode3C13.text = "As Peter in the flesh is above the angels ⁄ so Christ in His visible coming told him ⁄ that he would sit as a judge with Him."

meMA.Ode3C1T.ode = miscellanea_en_US_oca.misc.Ode3
meMA.Ode3C1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode3C1T.melody = he.h.m4_en_US_oca.heHE.OukEnSofia.notmetered
meMA.Ode3C1T.incipit = "Unwedded, pure One, ⁄ you…"
meMA.Ode3C1T.text = "Unwedded, pure One, ⁄ you conceived the Incarnate God: ⁄ Strengthen me, for I am tossed about by the play of the passions, ⁄ for there is no help but you."

meMA.Ode3C21.ode = miscellanea_en_US_oca.misc.Ode3
meMA.Ode3C21.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode3C21.greekmelody = he.h.m8_en_US_oca.heHE.SyEiToStereoma.greekname
meMA.Ode3C21.melody = he.h.m8_en_US_oca.heHE.SyEiToStereoma.notmetered
meMA.Ode3C21.incipit = "You are the foundation stone…"
meMA.Ode3C21.text = "You are the foundation stone ⁄ set up for the souls of the faithful. ⁄ One of great price: ⁄ You are the cornerstone of the Savior and Lord."

meMA.Ode3C22.ode = miscellanea_en_US_oca.misc.Ode3
meMA.Ode3C22.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode3C22.melody = he.h.m8_en_US_oca.heHE.SyEiToStereoma.notmetered
meMA.Ode3C22.incipit = "Most blessed Paul, ⁄…"
meMA.Ode3C22.text = "Most blessed Paul, ⁄ pray that the illumination of the virtues ⁄ may be established on your foundation."

meMA.Ode3C23.ode = miscellanea_en_US_oca.misc.Ode3
meMA.Ode3C23.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode3C23.melody = he.h.m8_en_US_oca.heHE.SyEiToStereoma.notmetered
meMA.Ode3C23.incipit = "You, Paul, who bear truthfully…"
meMA.Ode3C23.text = "You, Paul, who bear truthfully in your body ⁄ the death of Jesus at all times, ⁄ have been counted worthy of Life."

meMA.Ode3C2T.ode = miscellanea_en_US_oca.misc.Ode3
meMA.Ode3C2T.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode3C2T.melody = he.h.m8_en_US_oca.heHE.SyEiToStereoma.notmetered
meMA.Ode3C2T.incipit = "All the generations now saved…"
meMA.Ode3C2T.text = "All the generations now saved through you, ⁄ call you blessed, as was prophesied, O pure One!"

meMA.Hypakoe.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Hypakoe.melody = ""
meMA.Hypakoe.incipit = "What guard did not hold…"
meMA.Hypakoe.text = "What guard did not hold you prisoner? ⁄ What Church has not had you as a preacher? ⁄ Damascus esteems you, Paul, for it beheld you blinded. ⁄ Rome received your blood, and through it has gained great renown, ⁄ and Tarsus greatly rejoices in the precious relics of your beloved grave clothes. ⁄ Even so establish us, Apostle Paul, ⁄⁄ the boast of the whole universe!"

meMA.Ode4C1H.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C1H.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode4C1H.greekmelody = he.h.m4_en_US_oca.heHE.OutosOTheosImon.greekname
meMA.Ode4C1H.melody = he.h.m4_en_US_oca.heHE.OutosOTheosImon.melody
meMA.Ode4C1H.incipit = he.h.m4_en_US_oca.heHE.OutosOTheosImon.incipit
meMA.Ode4C1H.text = he.h.m4_en_US_oca.heHE.OutosOTheosImon.text

meMA.Ode4C11.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode4C11.greekmelody = he.h.m4_en_US_oca.heHE.OutosOTheosImon.greekname
meMA.Ode4C11.melody = he.h.m4_en_US_oca.heHE.OutosOTheosImon.notmetered
meMA.Ode4C11.incipit = "You were a fisher of…"
meMA.Ode4C11.text = "You were a fisher of mortal persons, as Christ promised you. ⁄ He made you divine for His Church, ⁄ for even at the start He entrusted you with its guidance."

meMA.Ode4C12.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode4C12.melody = he.h.m4_en_US_oca.heHE.OutosOTheosImon.notmetered
meMA.Ode4C12.incipit = "Jesus, the Giver of Life…"
meMA.Ode4C12.text = "Jesus, the Giver of Life to whom you intercede, ⁄ gave you the right to bind and loose: ⁄ Peter, may He be gracious to me!"

meMA.Ode4C13.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C13.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode4C13.melody = he.h.m4_en_US_oca.heHE.OutosOTheosImon.notmetered
meMA.Ode4C13.incipit = "Pray earnestly, Peter, ⁄ that…"
meMA.Ode4C13.text = "Pray earnestly, Peter, ⁄ that the kingdom of heaven may be opened ⁄ for those who honor your memory in unswerving faith."

meMA.Ode4C1T.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode4C1T.melody = he.h.m4_en_US_oca.heHE.OutosOTheosImon.notmetered
meMA.Ode4C1T.incipit = "Honored Queen who conceived God…"
meMA.Ode4C1T.text = "Honored Queen who conceived God, ⁄ cleanse our thoughts by your intercessions, ⁄ and show me to be fruitful, ⁄ Mother of the God of all."

meMA.Ode4C21.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C21.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode4C21.greekmelody = he.h.m8_en_US_oca.heHE.EpevisEfIppous.greekname
meMA.Ode4C21.melody = he.h.m8_en_US_oca.heHE.EpevisEfIppous.notmetered
meMA.Ode4C21.incipit = "Once Saul crouched as a…"
meMA.Ode4C21.text = "Once Saul crouched as a lion, ⁄ vigilantly destroying the Church of Christ. ⁄ He was seized by the divine voice of the Lamb of God, ⁄ the One whom he had been persecuting. ⁄ He was given charge of the flock, ⁄ entrusted as a preeminent shepherd."

meMA.Ode4C22.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C22.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode4C22.melody = he.h.m8_en_US_oca.heHE.EpevisEfIppous.notmetered
meMA.Ode4C22.incipit = "He who was to enlighten…"
meMA.Ode4C22.text = "He who was to enlighten the universe was put in darkness, ⁄ and Ananias was sent to him. ⁄ By Your divine appearing You brought light to his soul and body, ⁄ showing him to be a chosen vessel."

meMA.Ode4C23.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C23.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode4C23.melody = he.h.m8_en_US_oca.heHE.EpevisEfIppous.notmetered
meMA.Ode4C23.incipit = "Damascus was worthily tended by…"
meMA.Ode4C23.text = "Damascus was worthily tended by Paul, ⁄ as once from Paradise ⁄ a great river of divine waters flowed, ⁄ moistening the whole earth with the knowledge of God."

meMA.Ode4C2T.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C2T.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode4C2T.melody = he.h.m8_en_US_oca.heHE.EpevisEfIppous.notmetered
meMA.Ode4C2T.incipit = "The Virgin and Mother, as…"
meMA.Ode4C2T.text = "The Virgin and Mother, as God had revealed ⁄ put down the mighty from their thrones, ⁄ exalting those of low degree, who sing in faith: ⁄ Glory to Your might, O Lord!"

meMA.Ode5C1H.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode5C1H.greekmelody = he.h.m4_en_US_oca.heHE.OKtisamenosImas.greekname
meMA.Ode5C1H.melody = he.h.m4_en_US_oca.heHE.OKtisamenosImas.melody
meMA.Ode5C1H.incipit = he.h.m4_en_US_oca.heHE.OKtisamenosImas.incipit
meMA.Ode5C1H.text = he.h.m4_en_US_oca.heHE.OKtisamenosImas.text

meMA.Ode5C11.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode5C11.greekmelody = he.h.m4_en_US_oca.heHE.OKtisamenosImas.greekname
meMA.Ode5C11.melody = he.h.m4_en_US_oca.heHE.OKtisamenosImas.notmetered
meMA.Ode5C11.incipit = "The rays of the sun…"
meMA.Ode5C11.text = "The rays of the sun were rightly struck with awe, ⁄ before your boldness towards God, springing from love, ⁄ most glorious fisherman who worked wonders through grace!"

meMA.Ode5C12.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode5C12.melody = he.h.m4_en_US_oca.heHE.OKtisamenosImas.notmetered
meMA.Ode5C12.incipit = "Your divine Apostle, O Christ…"
meMA.Ode5C12.text = "Your divine Apostle, O Christ, ⁄ did not gain gold or money for You, ⁄ but by the creation of virtue, ⁄ he was rich in the might of wonders."

meMA.Ode5C13.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C13.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode5C13.melody = he.h.m4_en_US_oca.heHE.OKtisamenosImas.notmetered
meMA.Ode5C13.incipit = "People have endured torture…"
meMA.Ode5C13.text = "People have endured torture, ⁄ the hammer and the rack, because of your words, ⁄ for through the Spirit of God, ⁄ you have brought new and miraculous things to perfection."

meMA.Ode5C1T.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode5C1T.melody = he.h.m4_en_US_oca.heHE.OKtisamenosImas.notmetered
meMA.Ode5C1T.incipit = "The Incarnate God has united…"
meMA.Ode5C1T.text = "The Incarnate God has united Himself to flesh in His Person ⁄ through you, pure One, ⁄ while remaining unchanged in His bodiless Divine Nature."

meMA.Ode5C21.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C21.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode5C21.greekmelody = he.h.m8_en_US_oca.heHE.FotisonImas.greekname
meMA.Ode5C21.melody = he.h.m8_en_US_oca.heHE.FotisonImas.notmetered
meMA.Ode5C21.incipit = "Paul, you chose the Cross…"
meMA.Ode5C21.text = "Paul, you chose the Cross of the Lord Christ ⁄ as your true boast, ⁄ always carrying it, apostle, as a sign of victory."

meMA.Ode5C22.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C22.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode5C22.melody = he.h.m8_en_US_oca.heHE.FotisonImas.notmetered
meMA.Ode5C22.incipit = "For you, to live is…"
meMA.Ode5C22.text = "For you, to live is Christ, and to die, great gain. ⁄ for by love you were crucified ⁄ to the One who was crucified for us, ⁄ most glorious Paul."

meMA.Ode5C23.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C23.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode5C23.melody = he.h.m8_en_US_oca.heHE.FotisonImas.notmetered
meMA.Ode5C23.incipit = "You rejoice indeed in the…"
meMA.Ode5C23.text = "You rejoice indeed in the Lord, ⁄ honorable Paul, ⁄ for you have put off the body and gone to Christ, ⁄ our God and the Giver of Life."

meMA.Ode5C2T.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C2T.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode5C2T.melody = he.h.m8_en_US_oca.heHE.FotisonImas.notmetered
meMA.Ode5C2T.incipit = "We truly greet you with…"
meMA.Ode5C2T.text = "We truly greet you with joy, ⁄ storehouse of virginity, ⁄ recall of our first Mother Eve, ⁄ destroyer of the curse of Adam."

meMA.Ode6C1H.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C1H.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode6C1H.greekmelody = he.h.m4_en_US_oca.heHE.EnKiteiChriste.greekname
meMA.Ode6C1H.melody = he.h.m4_en_US_oca.heHE.EnKiteiChriste.melody
meMA.Ode6C1H.incipit = he.h.m4_en_US_oca.heHE.EnKiteiChriste.incipit
meMA.Ode6C1H.text = he.h.m4_en_US_oca.heHE.EnKiteiChriste.text

meMA.Ode6C11.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode6C11.greekmelody = he.h.m4_en_US_oca.heHE.EnKiteiChriste.greekname
meMA.Ode6C11.melody = he.h.m4_en_US_oca.heHE.EnKiteiChriste.notmetered
meMA.Ode6C11.incipit = "In days of old, You…"
meMA.Ode6C11.text = "In days of old, You saved Peter, O Christ, ⁄ stretching out Your hand as he walked upon the waters. ⁄ Lead me up as I sink in a storm of great temptations!"

meMA.Ode6C12.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode6C12.melody = he.h.m4_en_US_oca.heHE.EnKiteiChriste.notmetered
meMA.Ode6C12.incipit = "You forsook things that were…"
meMA.Ode6C12.text = "You forsook things that were worthless, Peter, ⁄ and as a merchant found what is reality. ⁄ You clearly caught Christ, the Pearl of great price."

meMA.Ode6C13.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C13.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode6C13.melody = he.h.m4_en_US_oca.heHE.EnKiteiChriste.notmetered
meMA.Ode6C13.incipit = "Yet, Peter, senselessly trying to…"
meMA.Ode6C13.text = "Yet, Peter, senselessly trying to tempt the Holy Spirit, ⁄ you put to death the One who blessed you as first ⁄ when you clearly confessed God, O blessed one."

meMA.Ode6C1T.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode6C1T.melody = he.h.m4_en_US_oca.heHE.EnKiteiChriste.notmetered
meMA.Ode6C1T.incipit = "You brought forth the Word…"
meMA.Ode6C1T.text = "You brought forth the Word of God, ⁄ Who beyond all nature was made flesh for us. ⁄ Therefore with heart and voice we proclaim you as the Theotokos."

meMA.Ode6C21.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C21.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode6C21.greekmelody = he.h.m8_en_US_oca.heHE.TinDeisinEkcheo.greekname
meMA.Ode6C21.melody = he.h.m8_en_US_oca.heHE.TinDeisinEkcheo.notmetered
meMA.Ode6C21.incipit = "You turned from created beauty…"
meMA.Ode6C21.text = "You turned from created beauty, ⁄ struck by love for the Master ⁄ and your desire that all be saved. ⁄ You determined to bear witness to this, blessed Paul, ⁄ and now you pray for the world."

meMA.Ode6C22.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C22.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode6C22.melody = he.h.m8_en_US_oca.heHE.TinDeisinEkcheo.notmetered
meMA.Ode6C22.incipit = "You nobly imitated the Master…"
meMA.Ode6C22.text = "You nobly imitated the Master, ⁄ and put Him on as a garment, Paul. ⁄ You were truly all things to all men ⁄ so that you might gain all and save people, ⁄ and truly you netted the ends of the earth for Christ!"

meMA.Ode6C23.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C23.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode6C23.melody = he.h.m8_en_US_oca.heHE.TinDeisinEkcheo.notmetered
meMA.Ode6C23.incipit = "Christ worthily gave you life…"
meMA.Ode6C23.text = "Christ worthily gave you life in heaven, Apostle Paul: ⁄ for while remaining here, you did not desire an earthly city, blessed one, ⁄ but you were a faithful servant and builder of His mysteries."

meMA.Ode6C2T.ode = miscellanea_en_US_oca.misc.Ode6
meMA.Ode6C2T.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode6C2T.melody = he.h.m8_en_US_oca.heHE.TinDeisinEkcheo.notmetered
meMA.Ode6C2T.incipit = "The Almighty Lord looked down…"
meMA.Ode6C2T.text = "The Almighty Lord looked down on you. ⁄ He renewed my being as He did great things for you ⁄ as He foretold, all-blameless Mother of God. ⁄ Through you, He saved me from corruption ⁄ as the compassionate One, Theotokos."

meMA.Kontakion1.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Kontakion1.greekmelody = he.a.m2_en_US_oca.heAU.TousAsfaleis.greekname
meMA.Kontakion1.melody = he.a.m2_en_US_oca.heAU.TousAsfaleis.melody
meMA.Kontakion1.incipit = he.a.m2_en_US_oca.heAU.TousAsfaleis.incipit
meMA.Kontakion1.text = he.a.m2_en_US_oca.heAU.TousAsfaleis.text

meMA.Oikos1.text = "Open my mouth, my Savior, and let my tongue speak plainly. Fill me full and give my heart compunction, that I may follow the things I say and first do the things I teach. For Paul has said that whoever does what he teaches is great, but if I say things and do not follow them, I am counted as sounding brass. Therefore teach me what to speak, and let me do what is right, ⁄⁄ Only Searcher of the hearts of men."

meMA.Ode7C1H.ode = miscellanea_en_US_oca.misc.Ode7
meMA.Ode7C1H.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode7C1H.greekmelody = he.h.m4_en_US_oca.heHE.OEnArchiAnarchos.greekname
meMA.Ode7C1H.melody = he.h.m4_en_US_oca.heHE.OEnArchiAnarchos.melody
meMA.Ode7C1H.incipit = he.h.m4_en_US_oca.heHE.OEnArchiAnarchos.incipit
meMA.Ode7C1H.text = he.h.m4_en_US_oca.heHE.OEnArchiAnarchos.text

meMA.Ode7C11.ode = miscellanea_en_US_oca.misc.Ode7
meMA.Ode7C11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode7C11.greekmelody = he.h.m4_en_US_oca.heHE.OEnArchiAnarchos.greekname
meMA.Ode7C11.melody = he.h.m4_en_US_oca.heHE.OEnArchiAnarchos.notmetered
meMA.Ode7C11.incipit = "Raised up by the divine…"
meMA.Ode7C11.text = "Raised up by the divine providence of Christ, Peter, ⁄ you were allowed to imitate Him fully, ⁄ yet also to fall before His Passion beneath the flood of denial."

meMA.Ode7C12.ode = miscellanea_en_US_oca.misc.Ode7
meMA.Ode7C12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode7C12.melody = he.h.m4_en_US_oca.heHE.OEnArchiAnarchos.notmetered
meMA.Ode7C12.incipit = "Christ who is risen from…"
meMA.Ode7C12.text = "Christ who is risen from the tomb ⁄ appeared first to you, first-named one: ⁄ the greatly-loved leader of the apostles."

meMA.Ode7C13.ode = miscellanea_en_US_oca.misc.Ode7
meMA.Ode7C13.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode7C13.melody = he.h.m4_en_US_oca.heHE.OEnArchiAnarchos.notmetered
meMA.Ode7C13.incipit = "By His three-fold command, Peter…"
meMA.Ode7C13.text = "By His three-fold command, Peter, ⁄ the Master strengthens your love, ⁄ healing the three-fold denial that came before the Passion."

meMA.Ode7C14.ode = miscellanea_en_US_oca.misc.Ode7
meMA.Ode7C14.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode7C14.melody = he.h.m4_en_US_oca.heHE.OEnArchiAnarchos.notmetered
meMA.Ode7C14.incipit = "You set a witness of…"
meMA.Ode7C14.text = "You set a witness of love, Peter, ⁄ before Christ God, the all-seeing Word. ⁄ Therefore He entrusted you with His beloved flock."

meMA.Ode7C1T.ode = miscellanea_en_US_oca.misc.Ode7
meMA.Ode7C1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode7C1T.melody = he.h.m4_en_US_oca.heHE.OEnArchiAnarchos.notmetered
meMA.Ode7C1T.incipit = "We all bless you as…"
meMA.Ode7C1T.text = "We all bless you as Mother of the God of all ⁄ for you conceived the Incarnate God the Word ⁄ Who shone forth from the pre-eternal Father."

meMA.Ode7C21.ode = miscellanea_en_US_oca.misc.Ode7
meMA.Ode7C21.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode7C21.greekmelody = he.h.m8_en_US_oca.heHE.EnPedioDeeira.greekname
meMA.Ode7C21.melody = he.h.m8_en_US_oca.heHE.EnPedioDeeira.notmetered
meMA.Ode7C21.incipit = "You are the Seal and…"
meMA.Ode7C21.text = "You are the Seal and Crown of Your Apostles, O Christ, ⁄ and he who was called last of all was before them all in zeal. ⁄ With him, the people of the Church sing to You: ⁄ Blessed are You, the God of our fathers!"

meMA.Ode7C22.ode = miscellanea_en_US_oca.misc.Ode7
meMA.Ode7C22.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode7C22.melody = he.h.m8_en_US_oca.heHE.EnPedioDeeira.notmetered
meMA.Ode7C22.incipit = "If Your bound servant Paul…"
meMA.Ode7C22.text = "If Your bound servant Paul formerly persecuted the Church, ⁄ yet he changed his old impudence by his lasting zeal for You. ⁄ For he gathered together, O Christ, the gentiles who cried: ⁄ Blessed are You, the God of our fathers."

meMA.Ode7C23.ode = miscellanea_en_US_oca.misc.Ode7
meMA.Ode7C23.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode7C23.melody = he.h.m8_en_US_oca.heHE.EnPedioDeeira.notmetered
meMA.Ode7C23.incipit = "You preached the Gospel to…"
meMA.Ode7C23.text = "You preached the Gospel to all, Paul, ⁄ beginning from Jerusalem to the ends of the earth. ⁄ You reached even the bounds of Illyrium ⁄ where you raised your cry: ⁄ Blessed are You, the God of our fathers!"

meMA.Ode7C24.ode = miscellanea_en_US_oca.misc.Ode7
meMA.Ode7C24.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode7C24.melody = he.h.m8_en_US_oca.heHE.EnPedioDeeira.notmetered
meMA.Ode7C24.incipit = "You were taken up in…"
meMA.Ode7C24.text = "You were taken up in a vision; ⁄ you reached the third heaven, gifted one! ⁄ You heard ineffable words, and cried aloud: ⁄ Glory to the Father most High, and to the Son, ⁄ one throne in brightness, ⁄ for you clearly felt the depth of the Spirit of God."

meMA.Ode7C2T.ode = miscellanea_en_US_oca.misc.Ode7
meMA.Ode7C2T.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode7C2T.melody = he.h.m8_en_US_oca.heHE.EnPedioDeeira.notmetered
meMA.Ode7C2T.incipit = "Christ our God came down…"
meMA.Ode7C2T.text = "Christ our God came down on you ⁄ as dew on the fleece of wool. ⁄ He came forth from you clothed in flesh, ⁄ uniting that which had been scattered. ⁄ He gave peace on earth and in heaven: ⁄ Blessed are You, the God of our fathers!"

meMA.Ode8C1H.ode = miscellanea_en_US_oca.misc.Ode8
meMA.Ode8C1H.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode8C1H.greekmelody = he.h.m4_en_US_oca.heHE.OTaSympantaFeron.greekname
meMA.Ode8C1H.melody = he.h.m4_en_US_oca.heHE.OTaSympantaFeron.melody
meMA.Ode8C1H.incipit = he.h.m4_en_US_oca.heHE.OTaSympantaFeron.incipit
meMA.Ode8C1H.text = he.h.m4_en_US_oca.heHE.OTaSympantaFeron.text

meMA.Ode8C11.ode = miscellanea_en_US_oca.misc.Ode8
meMA.Ode8C11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode8C11.greekmelody = he.h.m4_en_US_oca.heHE.OTaSympantaFeron.greekname
meMA.Ode8C11.melody = he.h.m4_en_US_oca.heHE.OTaSympantaFeron.notmetered
meMA.Ode8C11.incipit = "The Master foretold, Peter…"
meMA.Ode8C11.text = "The Master foretold, Peter, ⁄ that you would stretch out your hands ⁄ and they would bind you to the Cross. ⁄ He commanded you to follow Him and cry: ⁄ All works of the Lord, bless the Lord!"

meMA.Ode8C12.ode = miscellanea_en_US_oca.misc.Ode8
meMA.Ode8C12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode8C12.melody = he.h.m4_en_US_oca.heHE.OTaSympantaFeron.notmetered
meMA.Ode8C12.incipit = "Working wonders by the word…"
meMA.Ode8C12.text = "Working wonders by the word of grace, ⁄ you raised Aeneas, greatly stricken with palsy ⁄ and Tabitha who was dead, and who cried: ⁄ All works of the Lord, bless the Lord!"

meMA.Ode8C13.ode = miscellanea_en_US_oca.misc.Ode8
meMA.Ode8C13.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode8C13.melody = he.h.m4_en_US_oca.heHE.OTaSympantaFeron.notmetered
meMA.Ode8C13.incipit = "Christ, who plainly taught Peter…"
meMA.Ode8C13.text = "Christ, who plainly taught Peter that the gentiles were clean, ⁄ cleanse my thoughts also by the radiance of the Spirit as I cry: ⁄ All works of the Lord, bless the Lord!"

meMA.Ode8C1T.ode = miscellanea_en_US_oca.misc.Ode8
meMA.Ode8C1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode8C1T.melody = he.h.m4_en_US_oca.heHE.OTaSympantaFeron.notmetered
meMA.Ode8C1T.incipit = "One of the holy Persons…"
meMA.Ode8C1T.text = "One of the holy Persons has dwelt in you, Virgin, ⁄ and the Godhead has been entirely united with my being. ⁄ Therefore we bless you as the Theotokos."

meMA.Ode8C21.ode = miscellanea_en_US_oca.misc.Ode8
meMA.Ode8C21.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode8C21.greekmelody = he.h.m8_en_US_oca.heHE.TonEnOreiAgio.greekname
meMA.Ode8C21.melody = he.h.m8_en_US_oca.heHE.TonEnOreiAgio.notmetered
meMA.Ode8C21.incipit = "You were stirred by your…"
meMA.Ode8C21.text = "You were stirred by your love, Paul, ⁄ and left behind a good exchange: ⁄ for you did not live for yourself, glorious one, ⁄ but had the compassionate One living in you forever."

meMA.Ode8C22.ode = miscellanea_en_US_oca.misc.Ode8
meMA.Ode8C22.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode8C22.melody = he.h.m8_en_US_oca.heHE.TonEnOreiAgio.notmetered
meMA.Ode8C22.incipit = "You betrothed the Church…"
meMA.Ode8C22.text = "You betrothed the Church ⁄ as a Bride presented to the Bridegroom Christ, ⁄ for you, Paul, showed yourself as the leader of the Bride! ⁄ Therefore you are honored forever."

meMA.Ode8C23.ode = miscellanea_en_US_oca.misc.Ode8
meMA.Ode8C23.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode8C23.melody = he.h.m8_en_US_oca.heHE.TonEnOreiAgio.notmetered
meMA.Ode8C23.incipit = "You fought the good fight…"
meMA.Ode8C23.text = "You fought the good fight and fairly finished the course; ⁄ you kept the treasures of the faith, ⁄ and so you have gone rejoicing to Christ, leading one, ⁄ counted worthy of the crowns of righteousness, O Paul."

meMA.Ode8C2T.ode = miscellanea_en_US_oca.misc.Ode8
meMA.Ode8C2T.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode8C2T.melody = he.h.m8_en_US_oca.heHE.TonEnOreiAgio.notmetered
meMA.Ode8C2T.incipit = "Rejoice, fiery Throne of the…"
meMA.Ode8C2T.text = "Rejoice, fiery Throne of the Lord, ⁄ Rejoice, unwedded Bride! ⁄ Rejoice, radiant Cloud bringing forth the Sun of righteousness ⁄ Whom we exalt above all forever."

meMA.Ode9C1H.ode = miscellanea_en_US_oca.misc.Ode9
meMA.Ode9C1H.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode9C1H.greekmelody = he.h.m4_en_US_oca.heHE.SeTinYperendoxonNymfin.greekname
meMA.Ode9C1H.melody = he.h.m4_en_US_oca.heHE.SeTinYperendoxonNymfin.melody
meMA.Ode9C1H.incipit = he.h.m4_en_US_oca.heHE.SeTinYperendoxonNymfin.incipit
meMA.Ode9C1H.text = he.h.m4_en_US_oca.heHE.SeTinYperendoxonNymfin.text

meMA.Ode9C11.ode = miscellanea_en_US_oca.misc.Ode9
meMA.Ode9C11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode9C11.greekmelody = he.h.m4_en_US_oca.heHE.SeTinYperendoxonNymfin.greekname
meMA.Ode9C11.melody = he.h.m4_en_US_oca.heHE.SeTinYperendoxonNymfin.notmetered
meMA.Ode9C11.incipit = "Your eminent grace is manifested…"
meMA.Ode9C11.text = "Your eminent grace is manifested worthily, Peter, ⁄ and it banishes the suffering of the shadow of your weaknesses. ⁄ Therefore we magnify you."

meMA.Ode9C12.ode = miscellanea_en_US_oca.misc.Ode9
meMA.Ode9C12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode9C12.melody = he.h.m4_en_US_oca.heHE.SeTinYperendoxonNymfin.notmetered
meMA.Ode9C12.incipit = "Simon the magician was puffed…"
meMA.Ode9C12.text = "Simon the magician was puffed up with his mysteries, ⁄ but Peter brought him down ineffably with divine might and is blessed."

meMA.Ode9C13.ode = miscellanea_en_US_oca.misc.Ode9
meMA.Ode9C13.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode9C13.melody = he.h.m4_en_US_oca.heHE.SeTinYperendoxonNymfin.notmetered
meMA.Ode9C13.incipit = "By your prayers, Apostle…"
meMA.Ode9C13.text = "By your prayers, Apostle, ⁄ grant remission of sins and enlightenment of heart ⁄ with gladness of spirit to those who sing your memorial."

meMA.Ode9C1T.ode = miscellanea_en_US_oca.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode9C1T.melody = he.h.m4_en_US_oca.heHE.SeTinYperendoxonNymfin.notmetered
meMA.Ode9C1T.incipit = "Virgin who conceived God…"
meMA.Ode9C1T.text = "Virgin who conceived God, ⁄ you are named Theotokos. ⁄ Therefore the faithful rightly glorify you together, ⁄ and magnify you in songs."

meMA.Ode9C21.ode = miscellanea_en_US_oca.misc.Ode9
meMA.Ode9C21.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode9C21.greekmelody = he.h.m8_en_US_oca.heHE.OrousParilthes.greekname
meMA.Ode9C21.melody = he.h.m8_en_US_oca.heHE.OrousParilthes.notmetered
meMA.Ode9C21.incipit = "We venerate the chains you…"
meMA.Ode9C21.text = "We venerate the chains you wore ⁄ as a criminal for the sake of Christ, ⁄ and with our lips we honor the marks you bear ⁄ on your glorious and triumphant body."

meMA.Ode9C22.ode = miscellanea_en_US_oca.misc.Ode9
meMA.Ode9C22.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode9C22.melody = he.h.m8_en_US_oca.heHE.OrousParilthes.notmetered
meMA.Ode9C22.incipit = "Now you are set free…"
meMA.Ode9C22.text = "Now you are set free, Apostle, ⁄ to the One whom you desired from eternity. ⁄ Now you dwell with Him as His servant, ⁄ drawing your servants by your prayers."

meMA.Ode9C23.ode = miscellanea_en_US_oca.misc.Ode9
meMA.Ode9C23.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode9C23.melody = he.h.m8_en_US_oca.heHE.OrousParilthes.notmetered
meMA.Ode9C23.incipit = "Christ is no longer seen…"
meMA.Ode9C23.text = "Christ is no longer seen by you in hidden sayings ⁄ or in a dark glass, ⁄ but face to face you behold Him ⁄ revealing perfect understanding of the divine Nature to you."

meMA.Ode9C2T.ode = miscellanea_en_US_oca.misc.Ode9
meMA.Ode9C2T.mode = miscellanea_en_US_oca.misc.Mode8
meMA.Ode9C2T.melody = he.h.m8_en_US_oca.heHE.OrousParilthes.notmetered
meMA.Ode9C2T.incipit = "Virgin, you bore the Incarnate…"
meMA.Ode9C2T.text = "Virgin, you bore the Incarnate Word, ⁄ Who as the Word without flesh desired to renew my nature. ⁄ Therefore we magnify you without ceasing, Theotokos!"

