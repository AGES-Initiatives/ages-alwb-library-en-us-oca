meMA.Ode9C11.ode = miscellanea_en_US_oca.misc.Ode9
meMA.Ode9C11.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode9C11.melody = he.h.m4_en_US_oca.heHE.LithosAcheirotmitos.notmetered
meMA.Ode9C11.incipit = "You were shown…"
meMA.Ode9C11.text = "You were shown to be a splendid chariot, glorious one, ⁄ bearing the Word seated upon your beautiful and God-bearing soul. ⁄ You turn towards salvation those who magnify you."

meMA.Ode9C12.ode = miscellanea_en_US_oca.misc.Ode9
meMA.Ode9C12.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode9C12.melody = he.h.m4_en_US_oca.heHE.LithosAcheirotmitos.notmetered
meMA.Ode9C12.incipit = "…"
meMA.Ode9C12.text = "As we celebrate your most blessed memory, ⁄ we pray that we may be delivered from every stormy wave ⁄ by the boldness which you have as an apostle of Christ, ⁄ all-honored God-seer."

meMA.Ode9C13.ode = miscellanea_en_US_oca.misc.Ode9
meMA.Ode9C13.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode9C13.melody = he.h.m4_en_US_oca.heHE.LithosAcheirotmitos.notmetered
meMA.Ode9C13.incipit = "…"
meMA.Ode9C13.text = "Today we celebrate your memory, apostle Thomas. ⁄ Sanctify and forgive us, delivering us by faith ⁄ from all temptations, evil circumstances and passions ⁄ through your supplications."

meMA.Ode9C1T.ode = miscellanea_en_US_oca.misc.Ode9
meMA.Ode9C1T.mode = miscellanea_en_US_oca.misc.Mode4
meMA.Ode9C1T.melody = he.h.m4_en_US_oca.heHE.LithosAcheirotmitos.notmetered
meMA.Ode9C1T.incipit = "…"
meMA.Ode9C1T.text = "God the Word, the beautiful ear of Grain, ⁄ sprang forth from the soil of the Virgin. ⁄ Tasting of Him, faithful, we live and are established in Him. ⁄ Therefore, we magnify you, for you gave Him birth!"

