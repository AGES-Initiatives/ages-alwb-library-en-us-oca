meMA.Ode6C11.ode = misc_en_US_oca.Ode6
meMA.Ode6C11.mode = misc_en_US_oca.Mode6
meMA.Ode6C11.melody = he.h.m6_en_US_oca.TouViouTinThalassan.notmetered
meMA.Ode6C11.incipit = "Your beautiful feet truly proclaim…"
meMA.Ode6C11.text = "Your beautiful feet truly proclaim the glad tidings of transcendent peace ⁄ and your mind is glorious, most honored disciple of Christ."

meMA.Ode6C12.ode = misc_en_US_oca.Ode6
meMA.Ode6C12.mode = misc_en_US_oca.Mode6
meMA.Ode6C12.melody = he.h.m6_en_US_oca.TouViouTinThalassan.notmetered
meMA.Ode6C12.incipit = "You were granted to understand…"
meMA.Ode6C12.text = "You were granted to understand and preach, ⁄ ever-memorable James, ⁄ the awesome mystery of the incarnation of God! ⁄ You receive pure rays of light from the Most High Himself."

meMA.Ode6C13.ode = misc_en_US_oca.Ode6
meMA.Ode6C13.mode = misc_en_US_oca.Mode6
meMA.Ode6C13.melody = he.h.m6_en_US_oca.TouViouTinThalassan.notmetered
meMA.Ode6C13.incipit = "Supreme Apostle of Christ, ⁄…"
meMA.Ode6C13.text = "Supreme Apostle of Christ, ⁄ you fittingly shine with the splendor of ineffable light! ⁄ Earnestly pray for those who celebrate, glorious one, ⁄ your all-honorable memory."

meMA.Ode6C1T.ode = misc_en_US_oca.Ode6
meMA.Ode6C1T.mode = misc_en_US_oca.Mode6
meMA.Ode6C1T.melody = he.h.m6_en_US_oca.TouViouTinThalassan.notmetered
meMA.Ode6C1T.incipit = "All pure one, as you…"
meMA.Ode6C1T.text = "All pure one, as you gave birth to the good God ⁄ Who rules over all creation, ⁄ completely remove the wounds of my sins. ⁄ Turn the One who was born of you to compassion."

