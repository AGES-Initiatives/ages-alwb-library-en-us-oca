meMA.Ode1C11.ode = misc_en_US_oca.Ode1
meMA.Ode1C11.mode = misc_en_US_oca.Mode6
meMA.Ode1C11.melody = he.h.m6_en_US_oca.OsEnIpeiroPezefsas.notmetered
meMA.Ode1C11.incipit = "James, disciple of Christ and…"
meMA.Ode1C11.text = "James, disciple of Christ and seer of God, ⁄ hold out your hand to me ⁄ for I strive to praise your honored feast: ⁄ by your prayers, shine light upon my heart."

meMA.Ode1C12.ode = misc_en_US_oca.Ode1
meMA.Ode1C12.mode = misc_en_US_oca.Mode6
meMA.Ode1C12.melody = he.h.m6_en_US_oca.OsEnIpeiroPezefsas.notmetered
meMA.Ode1C12.incipit = "You were watered with streams…"
meMA.Ode1C12.text = "You were watered with streams of the wisdom of God, ⁄ flowing from the paradise of sweetness like a river. ⁄ You have indeed openly irrigated the face of the earth ⁄ with rivers of the faith."

meMA.Ode1C13.ode = misc_en_US_oca.Ode1
meMA.Ode1C13.mode = misc_en_US_oca.Mode6
meMA.Ode1C13.melody = he.h.m6_en_US_oca.OsEnIpeiroPezefsas.notmetered
meMA.Ode1C13.incipit = "You have been numbered, James…"
meMA.Ode1C13.text = "You have been numbered, James, ⁄ among the most holy choir of Christ's disciples, ⁄ fulfilling the number twelve. ⁄ Remember us as you stand with them before the Master."

meMA.Ode1C1T.ode = misc_en_US_oca.Ode1
meMA.Ode1C1T.mode = misc_en_US_oca.Mode6
meMA.Ode1C1T.melody = he.h.m6_en_US_oca.OsEnIpeiroPezefsas.notmetered
meMA.Ode1C1T.incipit = "The Lord Who stretched out…"
meMA.Ode1C1T.text = "The Lord Who stretched out the heavens ⁄ and founded the earth ⁄ has revealed you to us as an earthly heaven, ⁄ for He came forth from you in the flesh, all-pure Virgin."

