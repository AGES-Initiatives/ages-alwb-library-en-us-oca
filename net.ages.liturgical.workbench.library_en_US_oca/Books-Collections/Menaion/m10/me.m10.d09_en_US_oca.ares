meMA.Ode5C11.ode = misc_en_US_oca.Ode5
meMA.Ode5C11.mode = misc_en_US_oca.Mode6
meMA.Ode5C11.melody = he.h.m6_en_US_oca.ToTheioFengeiSou.notmetered
meMA.Ode5C11.incipit = "As you spoke with the…"
meMA.Ode5C11.text = "As you spoke with the spiritual Sun, ⁄ you assimilated His radiance, blessed one. ⁄ As He is by essence the first Light, ⁄ you became a second light through grace, all glorious one."

meMA.Ode5C12.ode = misc_en_US_oca.Ode5
meMA.Ode5C12.mode = misc_en_US_oca.Mode6
meMA.Ode5C12.melody = he.h.m6_en_US_oca.ToTheioFengeiSou.notmetered
meMA.Ode5C12.incipit = "You possessed a most pure…"
meMA.Ode5C12.text = "You possessed a most pure intelligence ⁄ and acquired a radiant and pure heart. ⁄ You comprehended the incarnate God ⁄ Who cannot be comprehended by the mind!"

meMA.Ode5C13.ode = misc_en_US_oca.Ode5
meMA.Ode5C13.mode = misc_en_US_oca.Mode6
meMA.Ode5C13.melody = he.h.m6_en_US_oca.ToTheioFengeiSou.notmetered
meMA.Ode5C13.incipit = "Every prophecy and law inclines…"
meMA.Ode5C13.text = "Every prophecy and law inclines towards you, most wealthy disciple, ⁄ for you were granted to see with your own eyes ⁄ the One who was proclaimed by them in days of old."

meMA.Ode5C1T.ode = misc_en_US_oca.Ode5
meMA.Ode5C1T.mode = misc_en_US_oca.Mode6
meMA.Ode5C1T.melody = he.h.m6_en_US_oca.ToTheioFengeiSou.notmetered
meMA.Ode5C1T.incipit = "Behold, Virgin, in accordance with…"
meMA.Ode5C1T.text = "Behold, Virgin, in accordance with the prophecy, ⁄ you conceived in your womb ⁄ the Master and Lord Who is over all! ⁄ You gave birth to Him ineffably, pure one, ⁄ remaining an incorrupt Virgin after giving birth."

