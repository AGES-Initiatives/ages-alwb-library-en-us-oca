meMA.Ode4C11.ode = misc_en_US_oca.Ode4
meMA.Ode4C11.mode = misc_en_US_oca.Mode6
meMA.Ode4C11.melody = he.h.m6_en_US_oca.ChristosMouDynamis.notmetered
meMA.Ode4C11.incipit = "The only holy Lord Himself…"
meMA.Ode4C11.text = "The only holy Lord Himself ⁄ numbered you among the honored choir of His disciples, blessed one, ⁄ foreseeing the honor and splendor of your life."

meMA.Ode4C12.ode = misc_en_US_oca.Ode4
meMA.Ode4C12.mode = misc_en_US_oca.Mode6
meMA.Ode4C12.melody = he.h.m6_en_US_oca.ChristosMouDynamis.notmetered
meMA.Ode4C12.incipit = "Filled with the miracles of…"
meMA.Ode4C12.text = "Filled with the miracles of the divine Spirit, ⁄ you worked wonders, herald of God, ⁄ driving out infirmities! ⁄ You have freed men from the wickedness of the evil spirits."

meMA.Ode4C13.ode = misc_en_US_oca.Ode4
meMA.Ode4C13.mode = misc_en_US_oca.Mode6
meMA.Ode4C13.melody = he.h.m6_en_US_oca.ChristosMouDynamis.notmetered
meMA.Ode4C13.incipit = "The radiance of God which…"
meMA.Ode4C13.text = "The radiance of God which descended upon your heart ⁄ making it shine with brilliance, blessed one, ⁄ and in a manner beyond understanding, ⁄ filled it with the gifts of the Spirit."

meMA.Ode4C1T.ode = misc_en_US_oca.Ode4
meMA.Ode4C1T.mode = misc_en_US_oca.Mode6
meMA.Ode4C1T.melody = he.h.m6_en_US_oca.ChristosMouDynamis.notmetered
meMA.Ode4C1T.incipit = "Through your divine childbearing, Virgin…"
meMA.Ode4C1T.text = "Through your divine childbearing, Virgin, ⁄ the most holy ranks of the angels ⁄ have been united to the assemblies of men, ⁄ for you gave birth to Christ the Savior, the Author of peace."

