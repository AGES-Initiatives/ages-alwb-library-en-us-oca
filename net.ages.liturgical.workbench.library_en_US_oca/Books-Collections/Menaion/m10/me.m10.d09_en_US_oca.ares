meVE.SticGlory.poet = poets_en_US_oca.Theofanous
meVE.SticGlory.mode = misc_en_US_oca.Mode4
meVE.SticGlory.melody = ""
meVE.SticGlory.incipit = "By receiving the grace of…"
meVE.SticGlory.text = "By receiving the grace of the Holy Spirit, James, ⁄ you were numbered with the holy choir of apostles; ⁄ for a mighty wind descended from heaven, ⁄ and a fiery tongue breathed upon you, ⁄ with which you burned away the thorns of the nations' godlessness. ⁄ Pray to Christ God, as His preacher, ⁄⁄ that He may save our souls!"

