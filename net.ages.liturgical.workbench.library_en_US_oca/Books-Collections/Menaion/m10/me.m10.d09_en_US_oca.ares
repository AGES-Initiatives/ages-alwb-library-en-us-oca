meMA.Ode7C11.mode = misc_en_US_oca.Mode6
meMA.Ode7C11.melody = he.h.m6_en_US_oca.OiPaidesEnVavyloni.notmetered
meMA.Ode7C11.incipit = "The multitude of the nations…"
meMA.Ode7C11.text = "The multitude of the nations ⁄ have you as a divine lamp ⁄ illumined by the splendor and radiance of the Spirit and they cry aloud: ⁄ Blessed are You, Lord God of our fathers!"

meMA.Ode7C12.ode = misc_en_US_oca.Ode7
meMA.Ode7C12.mode = misc_en_US_oca.Mode6
meMA.Ode7C12.melody = he.h.m6_en_US_oca.OiPaidesEnVavyloni.notmetered
meMA.Ode7C12.incipit = "You are adorned with divinely…"
meMA.Ode7C12.text = "You are adorned with divinely given wisdom ⁄ and you destroy the wisdom of the world, Apostle, ⁄ crying out with the wisdom of God: ⁄ Blessed are You, Lord God of our fathers!"

meMA.Ode7C13.ode = misc_en_US_oca.Ode7
meMA.Ode7C13.mode = misc_en_US_oca.Mode6
meMA.Ode7C13.melody = he.h.m6_en_US_oca.OiPaidesEnVavyloni.notmetered
meMA.Ode7C13.incipit = "Your sound has gone out…"
meMA.Ode7C13.text = "Your sound has gone out into all the earth like thunder, Apostle James, ⁄ and you have preached to all lands, ⁄ rightly proclaiming the incarnate Word."

meMA.Ode7C1T.ode = misc_en_US_oca.Ode7
meMA.Ode7C1T.mode = misc_en_US_oca.Mode6
meMA.Ode7C1T.melody = he.h.m6_en_US_oca.OiPaidesEnVavyloni.notmetered
meMA.Ode7C1T.incipit = "You were more holy than…"
meMA.Ode7C1T.text = "You were more holy than all the saints, Virgin Mother, ⁄ for you ineffably gave birth to God! ⁄ We sing hymns to Him and cry out unceasingly: ⁄ Blessed are You, Lord God of our fathers!"

