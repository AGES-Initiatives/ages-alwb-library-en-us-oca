meMA.Ode3C11.ode = misc_en_US_oca.Ode3
meMA.Ode3C11.mode = misc_en_US_oca.Mode6
meMA.Ode3C11.melody = he.h.m6_en_US_oca.OukEstinAgios.notmetered
meMA.Ode3C11.incipit = "You openly received the radiance…"
meMA.Ode3C11.text = "You openly received the radiance of the Spirit of God; ⁄ you have shown yourself to those in darkness as splendid as the dawn! ⁄ You proclaim to all the appearance of the spiritual Sun."

meMA.Ode3C12.ode = misc_en_US_oca.Ode3
meMA.Ode3C12.mode = misc_en_US_oca.Mode6
meMA.Ode3C12.melody = he.h.m6_en_US_oca.OukEstinAgios.notmetered
meMA.Ode3C12.incipit = "You danced in a godly…"
meMA.Ode3C12.text = "You danced in a godly manner before the just and good King ⁄ and trod the path to heaven, blessed one. ⁄ You openly set your feet in righteousness."

meMA.Ode3C13.ode = misc_en_US_oca.Ode3
meMA.Ode3C13.mode = misc_en_US_oca.Mode6
meMA.Ode3C13.melody = he.h.m6_en_US_oca.OukEstinAgios.notmetered
meMA.Ode3C13.incipit = "Your true instructor in the…"
meMA.Ode3C13.text = "Your true instructor in the mysteries, James, ⁄ was the unoriginate Son of God ⁄ Who creates all things in His divine goodness. ⁄ He taught you the knowledge of the Truth."

meMA.Ode3C1T.ode = misc_en_US_oca.Ode3
meMA.Ode3C1T.mode = misc_en_US_oca.Mode6
meMA.Ode3C1T.melody = he.h.m6_en_US_oca.OukEstinAgios.notmetered
meMA.Ode3C1T.incipit = "All-pure Mary, you are the…"
meMA.Ode3C1T.text = "All-pure Mary, you are the hope of all who place their trust in you! ⁄ As you gave birth to the incarnate Word for our sake, ⁄ deliver me from perils and misfortunes."

