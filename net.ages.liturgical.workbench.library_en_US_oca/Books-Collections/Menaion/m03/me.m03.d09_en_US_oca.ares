meMA.Ode4C1H.incipit = he.h.m2_en_US_oca.heHE.EisakikoaKyrieTinAkoin.incipit
meMA.Ode4C1H.text = he.h.m2_en_US_oca.heHE.EisakikoaKyrieTinAkoin.text

meMA.Ode4C11.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C11.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Ode4C11.greekmelody = he.h.m2_en_US_oca.heHE.EisakikoaKyrieTinAkoin.greekname
meMA.Ode4C11.melody = he.h.m2_en_US_oca.heHE.EisakikoaKyrieTinAkoin.notmetered
meMA.Ode4C11.incipit = "The sufferers said to the…"
meMA.Ode4C11.text = "The sufferers said to the tormentors: ⁄ Most sinful ones, you vainly offer a bad bargain!"

meMA.Ode4C12.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C12.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Ode4C12.melody = he.h.m2_en_US_oca.heHE.EisakikoaKyrieTinAkoin.notmetered
meMA.Ode4C12.incipit = "The tormentors offered the saints…"
meMA.Ode4C12.text = "The tormentors offered the saints of Christ ⁄ sharpened swords, wild beasts, the Cross and fire!"

meMA.Ode4C13.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C13.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Ode4C13.melody = he.h.m2_en_US_oca.heHE.EisakikoaKyrieTinAkoin.notmetered
meMA.Ode4C13.incipit = "The fire of Gehenna is…"
meMA.Ode4C13.text = "The fire of Gehenna is frightful to us, ⁄ but we are not afraid of any fellow servants! said the athletes."

meMA.Ode4C14.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C14.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Ode4C14.melody = he.h.m2_en_US_oca.heHE.EisakikoaKyrieTinAkoin.notmetered
meMA.Ode4C14.incipit = "Let our arms be cut…"
meMA.Ode4C14.text = "Let our arms be cut off, let our feet be burned, ⁄ the holy ones cried out, ⁄ for we shall receive them back incorruptible!"

meMA.Ode4C1T.ode = miscellanea_en_US_oca.misc.Ode4
meMA.Ode4C1T.mode = miscellanea_en_US_oca.misc.Mode2
meMA.Ode4C1T.melody = he.h.m2_en_US_oca.heHE.EisakikoaKyrieTinAkoin.notmetered
meMA.Ode4C1T.incipit = "We beseech you, all-pure One…"
meMA.Ode4C1T.text = "We beseech you, all-pure One who conceived God without seed: ⁄ ever pray for your servants!"

meMA.Ode5C1H.ode = miscellanea_en_US_oca.misc.Ode5
meMA.Ode5C1H.mode = miscellanea_en_US_oca.misc.Mode2
