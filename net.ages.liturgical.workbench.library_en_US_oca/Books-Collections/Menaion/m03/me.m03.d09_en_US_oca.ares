mePL.Glory.poet = poets_en_US_oca.po.KyprianouMonachou
mePL.Glory.mode = miscellanea_en_US_oca.misc.Mode2
mePL.Glory.melody = miscellanea_en_US_oca.misc.Idiomelon
mePL.Glory.incipit = "David cried out prophetically in…"
