ocMA.OdeS3C11.mode = misc_en_US_oca.Mode8
ocMA.OdeS3C11.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C11.melody = he.h.m8_en_US_oca.OStereosasKatArchas.notmetered
ocMA.OdeS3C11.incipit = "By the saving Passion…"
ocMA.OdeS3C11.text = "By the saving Passion of Your flesh, O Christ, You justified Adam who was condemned by the taste of sin, for in the trial, the sinless One, manifestly owed no debt to death."

ocMA.OdeS3C12.mode = misc_en_US_oca.Mode8
ocMA.OdeS3C12.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C12.melody = he.h.m8_en_US_oca.OStereosasKatArchas.notmetered
ocMA.OdeS3C12.incipit = "Jesus, my God…"
ocMA.OdeS3C12.text = "Jesus, my God, caused the light of the Resurrection to shine on those sitting in the darkness and the shadow of death ; and binding the strong man with His divinity, He plundered all his goods."

ocMA.OdeS3C1T.mode = misc_en_US_oca.Mode8
ocMA.OdeS3C1T.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C1T.melody = he.h.m8_en_US_oca.OStereosasKatArchas.notmetered
ocMA.OdeS3C1T.incipit = "You are proclaimed…"
ocMA.OdeS3C1T.text = "You are proclaimed to be higher than the cherubim and seraphim, for you alone, O undefiled Theotokos, received within your womb the uncontainable God. Therefore all the faithful bless you in hymns, O pure Virgin."

ocMA.OdeS3C21.mode = misc_en_US_oca.Mode8
ocMA.OdeS3C21.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C21.melody = he.h.m8_en_US_oca.OuraniasApsidos.notmetered
ocMA.OdeS3C21.incipit = "When, long go, I disobeyed…"
ocMA.OdeS3C21.text = "When, long go, I disobeyed the command of my Maker, You banished me and made me a stranger. Then, being fashioned in my likeness, teaching me obedience through crucifixion, You made me to be Your friend."

ocMA.OdeS3C22.mode = misc_en_US_oca.Mode8
ocMA.OdeS3C22.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C22.melody = he.h.m8_en_US_oca.OuraniasApsidos.notmetered
ocMA.OdeS3C22.incipit = "O Lord: You foreknow…"
ocMA.OdeS3C22.text = "O Lord: You foreknow all things in Your wisdom, and by Your understanding You laid the foundations of the earth: by Your descent, O Word of God, You did not count it unworthy of Yourself to raise up the one made in Your own image."

ocMA.OdeS3C2T.mode = misc_en_US_oca.Mode8
ocMA.OdeS3C2T.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C2T.melody = he.h.m8_en_US_oca.OuraniasApsidos.notmetered
ocMA.OdeS3C2T.incipit = "Having dwelt bodily…"
ocMA.OdeS3C2T.text = "Having dwelt bodily in the Virgin, O Lord, You appeared to men in a fitting and visible form, thus revealing her as the true Theotokos, and helper of the faithful, O only Lover of mankind."

