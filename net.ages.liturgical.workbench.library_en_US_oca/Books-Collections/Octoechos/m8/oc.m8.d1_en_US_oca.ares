ocMA.OdeS4C1H.mode = misc_en_US_oca.Mode8
ocMA.OdeS4C1H.ode = misc_en_US_oca.Ode4
ocMA.OdeS4C1H.melody = he.h.m8_en_US_oca.SyMouIschys.melody
ocMA.OdeS4C1H.incipit = he.h.m8_en_US_oca.SyMouIschys.incipit
ocMA.OdeS4C1H.text = he.h.m8_en_US_oca.SyMouIschys.text

ocMA.OdeS4C11.mode = misc_en_US_oca.Mode8
ocMA.OdeS4C11.ode = misc_en_US_oca.Ode4
ocMA.OdeS4C11.melody = he.h.m8_en_US_oca.SyMouIschys.notmetered
ocMA.OdeS4C11.incipit = "You greatly loved me…"
ocMA.OdeS4C11.text = "You greatly loved me, O merciful Saviour, when I was Your enemy. With unheard of self-emptying, You came to earth, not despising the final act of violence from my inner pride. While You remained in the heights of Your ineffable glory, You glorified me, disgraced in days of old."

ocMA.OdeS4C12.mode = misc_en_US_oca.Mode8
ocMA.OdeS4C12.ode = misc_en_US_oca.Ode4
ocMA.OdeS4C12.melody = he.h.m8_en_US_oca.SyMouIschys.notmetered
ocMA.OdeS4C12.incipit = "Who is not filled with wonder…"
ocMA.OdeS4C12.text = "Who is not filled with wonder, O Master, on beholding death destroyed through the Passion ? Corruption taking flight through the Cross ? Hades being emptied of wealth through a death ? This is the work of the divine authority of Your Crucifixion, O Lover of mankind."

ocMA.OdeS4C1T.mode = misc_en_US_oca.Mode8
ocMA.OdeS4C1T.ode = misc_en_US_oca.Ode4
ocMA.OdeS4C1T.melody = he.h.m8_en_US_oca.SyMouIschys.notmetered
ocMA.OdeS4C1T.incipit = "You are the boast of the faithful…"
ocMA.OdeS4C1T.text = "You are the boast of the faithful, O unwedded Lady, the advocate and Refuge of Christians, their wall and haven, for you bring our petitions to your Son, saving from danger those who acknowledge you in faith and love as the pure Theotokos."

ocMA.OdeS4C21.mode = misc_en_US_oca.Mode8
ocMA.OdeS4C21.ode = misc_en_US_oca.Ode4
ocMA.OdeS4C21.melody = he.h.m8_en_US_oca.EisakikoaKyrie.notmetered
ocMA.OdeS4C21.incipit = "The children of violence…"
ocMA.OdeS4C21.text = "The children of violence nailed You to the Cross, O Lover of mankind. Therefore, O Compassionate One, You save those who glorify Your sufferings."

ocMA.OdeS4C22.mode = misc_en_US_oca.Mode8
ocMA.OdeS4C22.ode = misc_en_US_oca.Ode4
ocMA.OdeS4C22.melody = he.h.m8_en_US_oca.EisakikoaKyrie.notmetered
ocMA.OdeS4C22.incipit = "Rising from the tomb…"
ocMA.OdeS4C22.text = "Rising from the tomb, You raised with Yourself the dead in Hades, shedding the light of Your compassion on those who glorify Your Resurrection."

ocMA.OdeS4C2T.mode = misc_en_US_oca.Mode8
ocMA.OdeS4C2T.ode = misc_en_US_oca.Ode4
ocMA.OdeS4C2T.melody = he.h.m8_en_US_oca.EisakikoaKyrie.notmetered
ocMA.OdeS4C2T.incipit = "You gave birth to God…"
ocMA.OdeS4C2T.text = "You gave birth to God, O undefiled Mary: through your prayers beseech Him to grant us forgiveness of sins."

