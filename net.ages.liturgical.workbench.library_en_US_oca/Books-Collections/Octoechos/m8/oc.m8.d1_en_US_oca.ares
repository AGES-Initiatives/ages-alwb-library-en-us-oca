ocMA.OdeS5C1H.ode = misc_en_US_oca.Ode5
ocMA.OdeS5C1H.melody = he.h.m8_en_US_oca.InaTiMeAposo.melody
ocMA.OdeS5C1H.incipit = he.h.m8_en_US_oca.InaTiMeAposo.incipit
ocMA.OdeS5C1H.text = he.h.m8_en_US_oca.InaTiMeAposo.text

ocMA.OdeS5C11.mode = misc_en_US_oca.Mode8
ocMA.OdeS5C11.ode = misc_en_US_oca.Ode5
ocMA.OdeS5C11.melody = he.h.m8_en_US_oca.InaTiMeAposo.notmetered
ocMA.OdeS5C11.incipit = "You were mocked…"
ocMA.OdeS5C11.text = "You were mocked before Your Passion, enduring a purple robe to clothe Adam’s formless nakedness. You were nailed naked to the Cross in the flesh, putting off the garment of death, O Christ."

ocMA.OdeS5C12.mode = misc_en_US_oca.Mode8
ocMA.OdeS5C12.ode = misc_en_US_oca.Ode5
ocMA.OdeS5C12.melody = he.h.m8_en_US_oca.InaTiMeAposo.notmetered
ocMA.OdeS5C12.incipit = "Rising from the dust of death…"
ocMA.OdeS5C12.text = "Rising from the dust of death, O Christ, You built again my fallen being and granted it immortality, revealing once again my royal image shining with the light of incorruption."

ocMA.OdeS5C1T.mode = misc_en_US_oca.Mode8
ocMA.OdeS5C1T.ode = misc_en_US_oca.Ode5
ocMA.OdeS5C1T.melody = he.h.m8_en_US_oca.InaTiMeAposo.notmetered
ocMA.OdeS5C1T.incipit = "We entreat you…"
ocMA.OdeS5C1T.text = "We entreat you, O most pure Lady, for you have the boldness of a Mother before your Son: do not fail in your providential care for us, your people. For we Christians bring you alone as our favourable offering before the Master."

ocMA.OdeS5C21.mode = misc_en_US_oca.Mode8
ocMA.OdeS5C21.ode = misc_en_US_oca.Ode5
ocMA.OdeS5C21.melody = he.h.m8_en_US_oca.FotisonImas.notmetered
ocMA.OdeS5C21.incipit = "Guide us by the might…"
ocMA.OdeS5C21.text = "Guide us by the might of Your Cross, through which we fall down before You, and grant us Your peace, O Christ, as the Lover of mankind."

ocMA.OdeS5C22.mode = misc_en_US_oca.Mode8
ocMA.OdeS5C22.ode = misc_en_US_oca.Ode5
ocMA.OdeS5C22.melody = he.h.m8_en_US_oca.FotisonImas.notmetered
ocMA.OdeS5C22.incipit = "Direct our lives…"
ocMA.OdeS5C22.text = "Direct our lives, as we now sing the praises of Your Resurrection, and grant us Your peace, our God, as the Lover of mankind."

ocMA.OdeS5C2T.mode = misc_en_US_oca.Mode8
ocMA.OdeS5C2T.ode = misc_en_US_oca.Ode5
ocMA.OdeS5C2T.melody = he.h.m8_en_US_oca.FotisonImas.notmetered
ocMA.OdeS5C2T.incipit = "O Holy Mary…"
ocMA.OdeS5C2T.text = "O Holy Mary, the unwedded pure One: entreat your Son and our God that He may send down upon us, the faithful, great mercy."

