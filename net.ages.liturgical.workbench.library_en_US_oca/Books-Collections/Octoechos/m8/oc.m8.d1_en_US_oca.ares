ocMA.OdeS1C1H.mode = misc_en_US_oca.Mode8
ocMA.OdeS1C1H.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C1H.melody = he.h.m8_en_US_oca.ArmatilatinFarao.melody
ocMA.OdeS1C1H.incipit = he.h.m8_en_US_oca.ArmatilatinFarao.incipit
ocMA.OdeS1C1H.text = he.h.m8_en_US_oca.ArmatilatinFarao.text

ocMA.OdeS1C11.mode = misc_en_US_oca.Mode8
ocMA.OdeS1C11.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C11.melody = he.h.m8_en_US_oca.ArmatilatinFarao.notmetered
ocMA.OdeS1C11.incipit = "How shall we not wonder…"
ocMA.OdeS1C11.text = "How shall we not wonder at the almighty Divinity of Christ? For incorruption and freedom from passion flow to all the faithful from His Passion; a fountain of immortality falls from His holy side and eternal life from His tomb."

ocMA.OdeS1C12.mode = misc_en_US_oca.Mode8
ocMA.OdeS1C12.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C12.melody = he.h.m8_en_US_oca.ArmatilatinFarao.notmetered
ocMA.OdeS1C12.incipit = "How beautiful is the angel…"
ocMA.OdeS1C12.text = "How beautiful is the angel who now reveals himself, proclaiming to the women by his own shining form the brightness of the Resurrection. He bears the brilliant signs of the natural purity belonging to immateriality and he cries out: The Lord is risen."

ocMA.OdeS1C1T.mode = misc_en_US_oca.Mode8
ocMA.OdeS1C1T.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C1T.melody = he.h.m8_en_US_oca.ArmatilatinFarao.notmetered
ocMA.OdeS1C1T.incipit = "Glorious things are spoken…"
ocMA.OdeS1C1T.text = "Glorious things are spoken of you from generation to generation, O Virgin Theotokos, for you remained pure while containing God in your womb. Therefore we honour you as our advocate after God."

ocMA.OdeS1C21.mode = misc_en_US_oca.Mode8
ocMA.OdeS1C21.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C21.melody = he.h.m8_en_US_oca.YgranDiodefsas.notmetered
ocMA.OdeS1C21.incipit = "The gates of distress…"
ocMA.OdeS1C21.text = "The gates of distress were lifted up, and the gatekeepers of Hades trembled with fear as they beheld in the lowest depths of the earth the One who dwells on high beyond every form of creation."

ocMA.OdeS1C22.mode = misc_en_US_oca.Mode8
ocMA.OdeS1C22.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C22.melody = he.h.m8_en_US_oca.YgranDiodefsas.notmetered
ocMA.OdeS1C22.incipit = "The ranks of angels were amazed…"
ocMA.OdeS1C22.text = "The ranks of angels were amazed to see human nature once banished and held in the lowest depths of the earth, now seated on the Father's throne. "

ocMA.OdeS1C2T.mode = misc_en_US_oca.Mode8
ocMA.OdeS1C2T.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C2T.melody = he.h.m8_en_US_oca.YgranDiodefsas.notmetered
ocMA.OdeS1C2T.incipit = "The ranks of angels together …"
ocMA.OdeS1C2T.text = "The ranks of angels together with mortal men, sing your praises unceasingly, O unwedded Mother, for you carried as a Child in your arms the Creator of them all."

