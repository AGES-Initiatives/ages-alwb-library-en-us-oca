ocMA.OdeS6C1H.ode = misc_en_US_oca.Ode6
ocMA.OdeS6C1H.melody = he.h.m8_en_US_oca.IlasthitiMoiSotir.melody
ocMA.OdeS6C1H.incipit = he.h.m8_en_US_oca.IlasthitiMoiSotir.incipit
ocMA.OdeS6C1H.text = he.h.m8_en_US_oca.IlasthitiMoiSotir.text

ocMA.OdeS6C11.mode = misc_en_US_oca.Mode8
ocMA.OdeS6C11.ode = misc_en_US_oca.Ode6
ocMA.OdeS6C11.melody = he.h.m8_en_US_oca.IlasthitiMoiSotir.notmetered
ocMA.OdeS6C11.incipit = "The source of evil…"
ocMA.OdeS6C11.text = "The source of evil powerfully overthrew me through a tree ; when You were hung upon the Cross, O Christ, You overthrew the one who was stronger than I, making him a spectacle, and raising me, the fallen one."

ocMA.OdeS6C12.mode = misc_en_US_oca.Mode8
ocMA.OdeS6C12.ode = misc_en_US_oca.Ode6
ocMA.OdeS6C12.melody = he.h.m8_en_US_oca.IlasthitiMoiSotir.notmetered
ocMA.OdeS6C12.incipit = "You took pity on Sion…"
ocMA.OdeS6C12.text = "You took pity on Sion, and rising from the tomb, You perfected the new Sion in place of the old by Your precious blood, O Compassionate One, and now, O Christ, You reign in her forever."

ocMA.OdeS6C1T.mode = misc_en_US_oca.Mode8
ocMA.OdeS6C1T.ode = misc_en_US_oca.Ode6
ocMA.OdeS6C1T.melody = he.h.m8_en_US_oca.IlasthitiMoiSotir.notmetered
ocMA.OdeS6C1T.incipit = "May we be saved…"
ocMA.OdeS6C1T.text = "May we be saved by your prayers, O pure Mother of God, and obtain the divine illumination of the Son of God Who was ineffably made flesh of you, O all-holy One."

ocMA.OdeS6C21.mode = misc_en_US_oca.Mode8
ocMA.OdeS6C21.ode = misc_en_US_oca.Ode6
ocMA.OdeS6C21.melody = he.h.m8_en_US_oca.TinDeisinEkcheo.notmetered
ocMA.OdeS6C21.incipit = "Spreading out Your hands…"
ocMA.OdeS6C21.text = "Spreading out Your hands upon the Cross, You healed the hand of the first-formed man that was outstretched with greed in Eden. Tasting, in Your turn, the gall from the bitter wood, You have saved those who glorify Your sufferings, O Almighty Christ."

ocMA.OdeS6C22.mode = misc_en_US_oca.Mode8
ocMA.OdeS6C22.ode = misc_en_US_oca.Ode6
ocMA.OdeS6C22.melody = he.h.m8_en_US_oca.TinDeisinEkcheo.notmetered
ocMA.OdeS6C22.incipit = "The Redeemer tasted death…"
ocMA.OdeS6C22.text = "The Redeemer tasted death that He might destroy the ancient curse and rule of corruption. Christ who descended to Hades as the all-powerful One, rose and saved those who sing the praises of His Resurrection."

ocMA.OdeS6C2T.mode = misc_en_US_oca.Mode8
ocMA.OdeS6C2T.ode = misc_en_US_oca.Ode6
ocMA.OdeS6C2T.melody = he.h.m8_en_US_oca.TinDeisinEkcheo.notmetered
ocMA.OdeS6C2T.incipit = "O Virgin Theotokos…"
ocMA.OdeS6C2T.text = "O Virgin Theotokos: do not cease to intercede for us with your Son. For you are the support of the faithful, and we are strengthened by our hope in you. With love and desire we glorify you, and the One ineffably made flesh from you."

