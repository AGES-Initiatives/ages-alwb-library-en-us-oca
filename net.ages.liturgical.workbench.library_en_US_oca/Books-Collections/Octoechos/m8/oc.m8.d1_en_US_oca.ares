ocMA.OdeS7C1H.ode = misc_en_US_oca.Ode7
ocMA.OdeS7C1H.melody = he.h.m8_en_US_oca.TheouSynkatavasin.melody
ocMA.OdeS7C1H.incipit = he.h.m8_en_US_oca.TheouSynkatavasin.incipit
ocMA.OdeS7C1H.text = he.h.m8_en_US_oca.TheouSynkatavasin.text

ocMA.OdeS7C11.mode = misc_en_US_oca.Mode8
ocMA.OdeS7C11.ode = misc_en_US_oca.Ode7
ocMA.OdeS7C11.melody = he.h.m8_en_US_oca.TheouSynkatavasin.notmetered
ocMA.OdeS7C11.incipit = "The glorious emptying…"
ocMA.OdeS7C11.text = "The glorious emptying, and divine wealth of Your poverty, amazed the angels, O Christ, as they beheld You nailed to the Cross to save those who cry out in faith to You:  Blessed are You, O God of our fathers."

ocMA.OdeS7C12.mode = misc_en_US_oca.Mode8
ocMA.OdeS7C12.ode = misc_en_US_oca.Ode7
ocMA.OdeS7C12.melody = he.h.m8_en_US_oca.TheouSynkatavasin.notmetered
ocMA.OdeS7C12.incipit = "You have filled the lowest depths…"
ocMA.OdeS7C12.text = "You have filled the lowest depths of the earth with light by Your divine descent, driving out the tormenting darkness. Therefore the prisoners in age-old bonds cry out: Blessed are You, O God of our fathers."

ocMA.OdeS7C1Tr.mode = misc_en_US_oca.Mode8
ocMA.OdeS7C1Tr.ode = misc_en_US_oca.Ode7
ocMA.OdeS7C1Tr.melody = he.h.m8_en_US_oca.TheouSynkatavasin.notmetered
ocMA.OdeS7C1Tr.incipit = "Stating the teaching…"
ocMA.OdeS7C1Tr.text = "Stating the teaching of the Orthodox Faith, we proclaim You the Lord of all, O Father of the One and only-begotten Son, and we know the One true Spirit who proceeds from You, Co-eternal and One in essence."

ocMA.OdeS7C21.mode = misc_en_US_oca.Mode8
ocMA.OdeS7C21.ode = misc_en_US_oca.Ode7
ocMA.OdeS7C21.melody = he.h.m8_en_US_oca.OiEkTisIoudaias.notmetered
ocMA.OdeS7C21.incipit = "You have wrought salvation…"
ocMA.OdeS7C21.text = "You have wrought salvation in the midst of the earth as was prophesied, O God, for lifted upon the Cross, You called back all who cry in faith: Blessed are You, the God of our fathers."

ocMA.OdeS7C22.mode = misc_en_US_oca.Mode8
ocMA.OdeS7C22.ode = misc_en_US_oca.Ode7
ocMA.OdeS7C22.melody = he.h.m8_en_US_oca.OiEkTisIoudaias.notmetered
ocMA.OdeS7C22.incipit = "Rising from the tomb…"
ocMA.OdeS7C22.text = "Rising from the tomb as from sleep, You have raised the world with Yourself, O Merciful One. Creation has come to Your Resurrection through the preaching of the apostles: Blessed are You, the God of our fathers."

ocMA.OdeS7C2T.mode = misc_en_US_oca.Mode8
ocMA.OdeS7C2T.ode = misc_en_US_oca.Ode7
ocMA.OdeS7C2T.melody = he.h.m8_en_US_oca.OiEkTisIoudaias.notmetered
ocMA.OdeS7C2T.incipit = "You are equal to the Father…"
ocMA.OdeS7C2T.text = "You are equal to the Father and the Spirit in power and might. At their good pleasure You have taken form in the womb of the Virgin, O Co-eternal One. Blessed are You, the God of our fathers."

