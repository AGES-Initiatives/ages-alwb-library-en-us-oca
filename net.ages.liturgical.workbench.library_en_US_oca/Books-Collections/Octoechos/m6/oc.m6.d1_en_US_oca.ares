ocMA.OdeS1C11.mode = miscellanea_en_US_oca.misc.Mode6
ocMA.OdeS1C11.ode = miscellanea_en_US_oca.misc.Ode1
ocMA.OdeS1C11.melody = miscellanea_en_US_oca.misc.HeirmosNotMetered
ocMA.OdeS1C11.incipit = "O good Jesus…"
ocMA.OdeS1C11.text = "O good Jesus: with Your arms stretched out upon the Cross, You fulfilled perfectly the good will of the Father. Let us sing a song of victory to You."

ocMA.OdeS1C12.mode = miscellanea_en_US_oca.misc.Mode6
ocMA.OdeS1C12.ode = miscellanea_en_US_oca.misc.Ode1
ocMA.OdeS1C12.melody = he.h.m6_en_US_oca.OsEnIpeiroPezefsas.notmetered
ocMA.OdeS1C12.incipit = "O Lord of life…"
ocMA.OdeS1C12.text = "O Lord of life: death approached You in fear, as an ordered slave. Through him you award to us eternal life and Resurrection."

ocMA.OdeS1C1T.mode = miscellanea_en_US_oca.misc.Mode6
ocMA.OdeS1C1T.ode = miscellanea_en_US_oca.misc.Ode1
ocMA.OdeS1C1T.melody = he.h.m6_en_US_oca.OsEnIpeiroPezefsas.notmetered
ocMA.OdeS1C1T.incipit = "O Pure One…"
ocMA.OdeS1C1T.text = "O Pure One: beyond all understanding, you received your Creator, made flesh of His own will in your seedless womb: you are proclaimed in truth to be Queen of all creation."

ocMA.OdeS1C21.mode = miscellanea_en_US_oca.misc.Mode6
ocMA.OdeS1C21.ode = miscellanea_en_US_oca.misc.Ode1
ocMA.OdeS1C21.melody = he.h.m6_en_US_oca.KymatiThalassis.notmetered
ocMA.OdeS1C21.incipit = "Willingly condemned…"
ocMA.OdeS1C21.text = "Willingly condemned by a lawless sentence, the Judge stands before the seat of Pilate, and God, before whom earth and heaven tremble, is struck in the face by unjust hands."

ocMA.OdeS1C22.mode = miscellanea_en_US_oca.misc.Mode6
ocMA.OdeS1C22.ode = miscellanea_en_US_oca.misc.Ode1
ocMA.OdeS1C22.melody = he.h.m6_en_US_oca.KymatiThalassis.notmetered
ocMA.OdeS1C22.incipit = "O Saviour: stretching out…"
ocMA.OdeS1C22.text = "O Saviour: stretching out Your sacred hands on Your pure and life-giving Cross, You led all nations to knowledge of You, O Master, as they worship Your glorious Crucifixion, O Lord."

ocMA.OdeS1C2T.mode = miscellanea_en_US_oca.misc.Mode6
ocMA.OdeS1C2T.ode = miscellanea_en_US_oca.misc.Ode1
ocMA.OdeS1C2T.melody = he.h.m6_en_US_oca.KymatiThalassis.notmetered
ocMA.OdeS1C2T.incipit = "Shedding a fountain of tears…"
ocMA.OdeS1C2T.text = "Shedding a fountain of tears, the blameless Virgin stood by Your Cross, O Saviour, as she beheld the drops of blood from Your side, and she glorified Your incomparable compassion."

