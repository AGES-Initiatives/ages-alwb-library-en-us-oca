ocMA.OdeS5C11.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS5C11.ode = miscellanea_en_US_oca.misc.Ode5
ocMA.OdeS5C11.melody = he.h.m3_en_US_oca.heHE.ProsSeOrthrizo.notmetered
ocMA.OdeS5C11.incipit = "O All-seeing Lord…"
ocMA.OdeS5C11.text = "O All-seeing Lord: You were delivered to an unjust judge, through the envy of the Jews. You judged all the world in righteousness, and delivered Adam from the ancient condemnation."

ocMA.OdeS5C12.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS5C12.ode = miscellanea_en_US_oca.misc.Ode5
ocMA.OdeS5C12.melody = he.h.m3_en_US_oca.heHE.ProsSeOrthrizo.notmetered
ocMA.OdeS5C12.incipit = "O Christ: as You are risen…"
ocMA.OdeS5C12.text = "O Christ: as You are risen from the dead, grant peace to Your Church, and by the virtue of Your Cross, save our souls."

ocMA.OdeS5C1T.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS5C1T.ode = miscellanea_en_US_oca.misc.Ode5
ocMA.OdeS5C1T.melody = he.h.m3_en_US_oca.heHE.ProsSeOrthrizo.notmetered
ocMA.OdeS5C1T.incipit = "O Holy Tabernacle…"
ocMA.OdeS5C1T.text = "O Holy Tabernacle, more spacious than the heavens: you received the Word of God whom all creation cannot contain. You alone are the ever-Virgin."

ocMA.OdeS5C21.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS5C21.ode = miscellanea_en_US_oca.misc.Ode5
ocMA.OdeS5C21.greekmelody = he.h.m3_en_US_oca.heHE.EpiTisGis.greekname
ocMA.OdeS5C21.melody = he.h.m3_en_US_oca.heHE.EpiTisGis.notmetered
ocMA.OdeS5C21.incipit = "As Your side was pierced…"
ocMA.OdeS5C21.text = "As Your side was pierced by a spear, O my Christ, You set the woman free from the curse, she who was formed from the side of man, and was the mediator of ruin for all mankind."

ocMA.OdeS5C22.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS5C22.ode = miscellanea_en_US_oca.misc.Ode5
ocMA.OdeS5C22.melody = he.h.m3_en_US_oca.heHE.EpiTisGis.notmetered
ocMA.OdeS5C22.incipit = "O Christ our Saviour…"
ocMA.OdeS5C22.text = "O Christ our Saviour, equal in essence to the Father: You raised from the dead Your most pure and sacred body."

