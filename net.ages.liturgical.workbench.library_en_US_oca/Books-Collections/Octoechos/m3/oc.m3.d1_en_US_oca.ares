ocMA.OdeS6C1H.greekmelody = he.h.m3_en_US_oca.heHE.AvyssosEschati.greekname
ocMA.OdeS6C1H.melody = he.h.m3_en_US_oca.heHE.AvyssosEschati.melody
ocMA.OdeS6C1H.incipit = he.h.m3_en_US_oca.heHE.AvyssosEschati.incipit
ocMA.OdeS6C1H.text = he.h.m3_en_US_oca.heHE.AvyssosEschati.text

ocMA.OdeS6C11.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS6C11.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C11.melody = he.h.m3_en_US_oca.heHE.AvyssosEschati.notmetered
ocMA.OdeS6C11.incipit = "A depth of mercy…"
ocMA.OdeS6C11.text = "A depth of mercy and compassion surrounds me from Your merciful descent, for made flesh, and taking the form of a servant, You made me as God, and glorify me with Yourself, O Master."

ocMA.OdeS6C12.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS6C12.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C12.melody = he.h.m3_en_US_oca.heHE.AvyssosEschati.notmetered
ocMA.OdeS6C12.incipit = "When he saw the dead…"
ocMA.OdeS6C12.text = "When he saw the dead come to life, the one who deals in death died. These things are the tokens of Your Resurrection, O Christ, and the rewards of Your pure Passion."

ocMA.OdeS6C1T.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS6C1T.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C1T.melody = he.h.m3_en_US_oca.heHE.AvyssosEschati.notmetered
ocMA.OdeS6C1T.incipit = "O All pure One…"
ocMA.OdeS6C1T.text = "O All pure One, beyond all understanding: you alone are the mediator between the Creator and mankind. Entreat the mercy of your Son for your erring servants, and be our defender."

ocMA.OdeS6C21.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS6C21.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C21.greekmelody = he.h.m3_en_US_oca.heHE.SkinoseosIonas.greekname
ocMA.OdeS6C21.melody = he.h.m3_en_US_oca.heHE.SkinoseosIonas.notmetered
ocMA.OdeS6C21.incipit = "Put Yourself to the test…"
ocMA.OdeS6C21.text = "Put Yourself to the test of wounds and blows, by Your suffering on the Cross, You raised with Yourself the wounded in Hades. Therefore I cry out to You: O Lover of mankind: deliver me from corruption."

ocMA.OdeS6C22.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS6C22.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C22.melody = he.h.m3_en_US_oca.heHE.SkinoseosIonas.notmetered
ocMA.OdeS6C22.incipit = "The gates of Hades…"
ocMA.OdeS6C22.text = "The gates of Hades opened to You in fear, O Christ, while the spoil of the enemy was taken away. Thus when the women met You with sorrow, in its place, they carried joy away."

