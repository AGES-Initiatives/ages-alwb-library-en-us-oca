ocMA.OdeS3C11.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS3C11.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C11.melody = he.h.m3_en_US_oca.heHE.OEkMiOnton.notmetered
ocMA.OdeS3C11.incipit = "The ungodly was brought…"
ocMA.OdeS3C11.text = "The ungodly was brought to shame by Your Cross; he dug a pit, and himself fell into the hole which he made, but the horn of the humble is exalted by Your Resurrection, O Christ."

ocMA.OdeS3C12.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS3C12.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C12.melody = he.h.m3_en_US_oca.heHE.OEkMiOnton.notmetered
ocMA.OdeS3C12.incipit = "The proclamation of God…"
ocMA.OdeS3C12.text = "The proclamation of God has gone out to the nations as the waters cover the seas, for You rose from the tomb, Lover of mankind, revealing the Light of the Trinity to mankind."

ocMA.OdeS3C1T.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS3C1T.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C1T.melody = he.h.m3_en_US_oca.heHE.OEkMiOnton.notmetered
ocMA.OdeS3C1T.incipit = "Glorious things are spoken…"
ocMA.OdeS3C1T.text = "Glorious things are spoken of you, O living city of the King who reigns forever; for through you, O Lady, God dwelt among those on earth. "

ocMA.OdeS3C21.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS3C21.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C21.greekmelody = he.h.m3_en_US_oca.heHE.ToxonEthrafsasEchthron.greekname
ocMA.OdeS3C21.melody = he.h.m3_en_US_oca.heHE.ToxonEthrafsasEchthron.notmetered
ocMA.OdeS3C21.incipit = "O Pure Cross:…"
ocMA.OdeS3C21.text = "O Pure Cross: you are shown to be the cleansing of all the symbols of purification, for Jesus, the Most High God, stretched out His hands on you."

ocMA.OdeS3C22.mode = miscellanea_en_US_oca.misc.Mode3
ocMA.OdeS3C22.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C22.melody = he.h.m3_en_US_oca.heHE.ToxonEthrafsasEchthron.notmetered
ocMA.OdeS3C22.incipit = "Let all the faithful venerate…"
ocMA.OdeS3C22.text = "Let all the faithful venerate you, O life-bearing tomb, for Christ our true God was buried in you and He arose from you."

