ocMA.OdeS6C1H.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS6C1H.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C1H.melody = he.h.m4_en_US_oca.ThysoSoiMetaFonis.melody
ocMA.OdeS6C1H.incipit = he.h.m4_en_US_oca.ThysoSoiMetaFonis.incipit
ocMA.OdeS6C1H.text = he.h.m4_en_US_oca.ThysoSoiMetaFonis.text

ocMA.OdeS6C11.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS6C11.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C11.melody = he.h.m4_en_US_oca.ThysoSoiMetaFonis.notmetered
ocMA.OdeS6C11.incipit = "You went up on the Cross…"
ocMA.OdeS6C11.text = "You went up on the Cross, girded with strength, and wrestled with the tyrant. As God, You cast him down from the heights, and raised up Adam with Your invincible hand."

ocMA.OdeS6C12.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS6C12.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C12.melody = he.h.m4_en_US_oca.ThysoSoiMetaFonis.notmetered
ocMA.OdeS6C12.incipit = "You rose from the tomb…"
ocMA.OdeS6C12.text = "You rose from the tomb, O Christ, in a lightning flash of beauty. You scattered all Your enemies by Your divine might, and as God You filled all things with gladness."

ocMA.OdeS6C1T.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS6C1T.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C1T.melody = he.h.m4_en_US_oca.ThysoSoiMetaFonis.notmetered
ocMA.OdeS6C1T.incipit = "O strange and new wonder…"
ocMA.OdeS6C1T.text = "O strange and new wonder: A Virgin who did not know a man conceived in her womb the One who holds the whole world in His hands, yet she did not confine Him."

ocMA.OdeS6C2H.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS6C2H.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C2H.melody = miscellanea_en_US_oca.misc.HeirmosNotMetered
ocMA.OdeS6C2H.incipit = "I have come to the depths…"
ocMA.OdeS6C2H.text = "I have come to the depths of the sea, and a storm of many sins has wrecked me in the deep; but, as God, snatch my life from corruption, O greatly merciful one."

ocMA.OdeS6C21.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS6C21.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C21.melody = he.h.m4_en_US_oca.IlthonEisTaVathi.notmetered
ocMA.OdeS6C21.incipit = "Hades opened his mouth…"
ocMA.OdeS6C21.text = "Hades opened his mouth and swallowed me, and the fool puffed up his soul; but Christ descended and led me up again, as the Lover of mankind."

ocMA.OdeS6C22.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS6C22.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C22.melody = he.h.m4_en_US_oca.IlthonEisTaVathi.notmetered
ocMA.OdeS6C22.incipit = "Death was overcome…"
ocMA.OdeS6C22.text = "Death was overcome by death: for He who was dead arose, and has given me incorruption. He, the Immortal One, appeared to the women greeting them with great joy."

ocMA.OdeS6C2T.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS6C2T.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C2T.melody = he.h.m4_en_US_oca.IlthonEisTaVathi.notmetered
ocMA.OdeS6C2T.incipit = "O Theotokos: your pure womb…"
ocMA.OdeS6C2T.text = "O Theotokos: your pure womb has been proclaimed to be the dwelling of God, the incomprehensible Divinity, upon whom the ranks of angels gaze with fear."

