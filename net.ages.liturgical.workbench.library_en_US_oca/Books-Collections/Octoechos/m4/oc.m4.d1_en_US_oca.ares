ocMA.OdeS3C1H.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C1H.melody = he.h.m4_en_US_oca.EffrainetaiEpiSoi.melody
ocMA.OdeS3C1H.incipit = he.h.m4_en_US_oca.EffrainetaiEpiSoi.incipit
ocMA.OdeS3C1H.text = he.h.m4_en_US_oca.EffrainetaiEpiSoi.text

ocMA.OdeS3C11.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS3C11.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C11.melody = he.h.m4_en_US_oca.EffrainetaiEpiSoi.notmetered
ocMA.OdeS3C11.incipit = "The Tree of life…"
ocMA.OdeS3C11.text = "The Tree of life, the true and spiritual Vine, has hung upon the Cross, pouring forth the drink of life to all."

ocMA.OdeS3C12.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS3C12.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C12.melody = he.h.m4_en_US_oca.EffrainetaiEpiSoi.notmetered
ocMA.OdeS3C12.incipit = "As the fearful and mighty One…"
ocMA.OdeS3C12.text = "As the fearful and mighty One, trampling the pride of Hades, You are risen in the flesh as immortal God."

ocMA.OdeS3C1T.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS3C1T.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C1T.melody = he.h.m4_en_US_oca.EffrainetaiEpiSoi.notmetered
ocMA.OdeS3C1T.incipit = "As the Theotokos…"
ocMA.OdeS3C1T.text = "As the Theotokos, you alone among those born an earth are the means to procure the things beyond nature. Therefore, we bring you the greeting: Rejoice."

ocMA.OdeS3C2H.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS3C2H.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C2H.melody = miscellanea_en_US_oca.misc.HeirmosNotMetered
ocMA.OdeS3C2H.incipit = "You freely came down…"
ocMA.OdeS3C2H.text = "You freely came down from on high to earth, O sovereign Lord of all, lifting up mankind’s frail and mortal nature from the lowest depths of hades. For none is holy but you, O Lover of mankind."

ocMA.OdeS3C21.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS3C21.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C21.melody = he.h.m4_en_US_oca.TousSousYmnologous.notmetered
ocMA.OdeS3C21.incipit = "You broke the venomous teeth…"
ocMA.OdeS3C21.text = "You broke the venomous teeth of the serpent that had pierced me, O Saviour, by the nails that pierced Your hands, O Almighty Master, for none is holy but You, O Lover of mankind."

ocMA.OdeS3C22.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS3C22.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C22.melody = he.h.m4_en_US_oca.TousSousYmnologous.notmetered
ocMA.OdeS3C22.incipit = "O Bestower of life…"
ocMA.OdeS3C22.text = "O Bestower of life: You freely willed to be seen dead in the tomb. You opened the gates of Hades for the souls lying there from all the ages. For none is holy but You, O Lover of mankind."

ocMA.OdeS3C2T.mode = miscellanea_en_US_oca.misc.Mode4
ocMA.OdeS3C2T.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C2T.melody = he.h.m4_en_US_oca.TousSousYmnologous.notmetered
ocMA.OdeS3C2T.incipit = "You are the untilled earth…"
ocMA.OdeS3C2T.text = "You are the untilled earth that brought forth the living ear of wheat, the Source of immortality, for all who partake: the Holy One who rests in holiness among the saints."

