ocMA.OdeS6C11.mode = misc_en_US_oca.Mode7
ocMA.OdeS6C11.ode = misc_en_US_oca.Ode6
ocMA.OdeS6C11.melody = he.h.m7_en_US_oca.NaftionToSalo.notmetered
ocMA.OdeS6C11.incipit = "The souls of the righteous…"
ocMA.OdeS6C11.text = "The souls of the righteous imprisoned and abandoned in hades remembered You and prayed to You for salvation. You granted it through Your Cross, O Christ, descending to the depths of the earth, Compassionate One."

ocMA.OdeS6C12.mode = misc_en_US_oca.Mode7
ocMA.OdeS6C12.ode = misc_en_US_oca.Ode6
ocMA.OdeS6C12.melody = he.h.m7_en_US_oca.NaftionToSalo.notmetered
ocMA.OdeS6C12.incipit = "The choir of the apostles…"
ocMA.OdeS6C12.text = "The choir of the apostles lost hope that they would see again the Living Temple not made by human hands destroyed by all that fell upon It in the Passion. But then beyond hope, they worshipped and proclaimed everywhere that He is risen from the dead."

ocMA.OdeS6C1T.mode = misc_en_US_oca.Mode7
ocMA.OdeS6C1T.ode = misc_en_US_oca.Ode6
ocMA.OdeS6C1T.melody = he.h.m7_en_US_oca.NaftionToSalo.notmetered
ocMA.OdeS6C1T.incipit = "Who can tell of your ineffable…"
ocMA.OdeS6C1T.text = "Who can tell of your ineffable childbearing for our sake, O Virgin Bride of God ? For God the Word who cannot be circumscribed, was united to you and became flesh from you."

ocMA.OdeS6C21.mode = misc_en_US_oca.Mode7
ocMA.OdeS6C21.ode = misc_en_US_oca.Ode6
ocMA.OdeS6C21.melody = he.h.m7_en_US_oca.OIonasEkKoilias.notmetered
ocMA.OdeS6C21.incipit = "Lifted up of Your own will…"
ocMA.OdeS6C21.text = "Lifted up of Your own will upon the Cross, nailing to it, O Good Saviour, the written scroll of our sins, You have led captive the dominion of the enemy."

ocMA.OdeS6C22.mode = misc_en_US_oca.Mode7
ocMA.OdeS6C22.ode = misc_en_US_oca.Ode6
ocMA.OdeS6C22.melody = he.h.m7_en_US_oca.OIonasEkKoilias.notmetered
ocMA.OdeS6C22.incipit = "Rising in power from the dead…"
ocMA.OdeS6C22.text = "Rising in power from the dead, O Saviour, You raised the race of man with Yourself, bestowing on us life and immortality as the Lover of mankind."

ocMA.OdeS6C2T.mode = misc_en_US_oca.Mode7
ocMA.OdeS6C2T.ode = misc_en_US_oca.Ode6
ocMA.OdeS6C2T.melody = he.h.m7_en_US_oca.OIonasEkKoilias.notmetered
ocMA.OdeS6C2T.incipit = "O pure and ever-Virgin…"
ocMA.OdeS6C2T.text = "O pure and ever-Virgin: pray ceaselessly to our God, Whom you bore beyond reason, O Theotokos, to save from peril those who sing your praises."

