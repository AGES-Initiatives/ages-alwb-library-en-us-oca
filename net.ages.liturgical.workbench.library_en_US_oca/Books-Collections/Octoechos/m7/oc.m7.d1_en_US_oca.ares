ocMA.OdeS3C11.mode = misc_en_US_oca.Mode7
ocMA.OdeS3C11.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C11.melody = he.h.m7_en_US_oca.OKatArchasTousOuranous.notmetered
ocMA.OdeS3C11.incipit = "Ascending the Cross…"
ocMA.OdeS3C11.text = "Ascending the Cross, O compassionate Saviour, of Your own will You suffered pain for us, enduring the wounding, the means of salvation and peace for the faithful, reconciling us all to Your Father, O merciful One."

ocMA.OdeS3C12.mode = misc_en_US_oca.Mode7
ocMA.OdeS3C12.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C12.melody = he.h.m7_en_US_oca.OKatArchasTousOuranous.notmetered
ocMA.OdeS3C12.incipit = "O Christ: You cleansed me…"
ocMA.OdeS3C12.text = "O Christ: You cleansed me from the wound my soul suffered at the bite of the serpent. You showed light to me as I lay in darkness and corruption, for by Your Cross You descended to hades, raising me up with Yourself."

ocMA.OdeS3C1T.mode = misc_en_US_oca.Mode7
ocMA.OdeS3C1T.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C1T.melody = he.h.m7_en_US_oca.OKatArchasTousOuranous.notmetered
ocMA.OdeS3C1T.incipit = "O Saviour: by the prayers…"
ocMA.OdeS3C1T.text = "O Saviour: by the prayers of Your unwedded Mother, grant peace to the world ; grant victory to Orthodox Christians over their adversaries, and count them worthy of Your ineffable glory."

ocMA.OdeS3C21.mode = misc_en_US_oca.Mode7
ocMA.OdeS3C21.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C21.melody = he.h.m7_en_US_oca.OOuranousToLogo.notmetered
ocMA.OdeS3C21.incipit = "You endured the sufferings…"
ocMA.OdeS3C21.text = "You endured the sufferings on the Cross and opened Paradise to the thief as God and Benefactor ; now, O only Lover of mankind, firmly establish my mind upon Your will."

ocMA.OdeS3C22.mode = misc_en_US_oca.Mode7
ocMA.OdeS3C22.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C22.melody = he.h.m7_en_US_oca.OOuranousToLogo.notmetered
ocMA.OdeS3C22.incipit = "You rose from the tomb…"
ocMA.OdeS3C22.text = "You rose from the tomb on the third day, and made life shine on earth, as God and Giver of Life. Now, O only Lover of mankind, firmly establish my mind upon Your will."

ocMA.OdeS3C2T.mode = misc_en_US_oca.Mode7
ocMA.OdeS3C2T.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C2T.melody = he.h.m7_en_US_oca.OOuranousToLogo.notmetered
ocMA.OdeS3C2T.incipit = "O Mary, Virgin and Mother…"
ocMA.OdeS3C2T.text = "O Mary, Virgin and Mother: you conceived God without seed and freed Eve from the curse; entreat God made flesh of you to save your flock."

