ocMA.OdeS1C1H.mode = misc_en_US_oca.Mode7
ocMA.OdeS1C1H.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C1H.melody = he.h.m7_en_US_oca.NefseiSouProsGeodi.melody
ocMA.OdeS1C1H.incipit = he.h.m7_en_US_oca.NefseiSouProsGeodi.incipit
ocMA.OdeS1C1H.text = he.h.m7_en_US_oca.NefseiSouProsGeodi.text

ocMA.OdeS1C11.mode = misc_en_US_oca.Mode7
ocMA.OdeS1C11.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C11.melody = he.h.m7_en_US_oca.NefseiSouProsGeodi.notmetered
ocMA.OdeS1C11.incipit = "The tyranny of death…"
ocMA.OdeS1C11.text = "The tyranny of death has been condemned through the Tree of the Cross ; by Your condemnation, O Lord, to an unjust death. Thus the prince of darkness lost the battle and has justly been cast out."

ocMA.OdeS1C12.mode = misc_en_US_oca.Mode7
ocMA.OdeS1C12.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C12.melody = he.h.m7_en_US_oca.NefseiSouProsGeodi.notmetered
ocMA.OdeS1C12.incipit = "Hades approached You…"
ocMA.OdeS1C12.text = "Hades approached You, but his jaws were broken, for his teeth were not strong enough to crush Your body. Therefore, O Saviour, having destroyed the pains of death, You are risen on the third day."

ocMA.OdeS1C1T.mode = misc_en_US_oca.Mode7
ocMA.OdeS1C1T.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C1T.melody = he.h.m7_en_US_oca.NefseiSouProsGeodi.notmetered
ocMA.OdeS1C1T.incipit = "The pains of our mother…"
ocMA.OdeS1C1T.text = "The pains of our mother Eve have been destroyed, for not knowing a man, you bore a child without pain. Therefore, beholding you clearly as the Mother of God, we glorify you, O most pure One."

ocMA.OdeS1C21.mode = misc_en_US_oca.Mode7
ocMA.OdeS1C21.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C21.melody = he.h.m7_en_US_oca.PontoEkalypse.notmetered
ocMA.OdeS1C21.incipit = "…"
ocMA.OdeS1C21.text = "The Saviour on the Cross poured out for us two life-bearing fountains from His wounded side. Let us sing to Him, for He is glorified."

ocMA.OdeS1C22.mode = misc_en_US_oca.Mode7
ocMA.OdeS1C22.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C22.melody = he.h.m7_en_US_oca.PontoEkalypse.notmetered
ocMA.OdeS1C22.incipit = "Christ dwelt in the tomb…"
ocMA.OdeS1C22.text = "Christ dwelt in the tomb and rose on the third day, granting mortal man the hope of incorruption. Let us sing to Him for He is glorified."

ocMA.OdeS1C2T.mode = misc_en_US_oca.Mode7
ocMA.OdeS1C2T.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C2T.melody = he.h.m7_en_US_oca.PontoEkalypse.notmetered
ocMA.OdeS1C2T.incipit = "You alone are seen…"
ocMA.OdeS1C2T.text = "You alone are seen to be Virgin after childbearing, for you bore the incarnate Creator of the world. Therefore, we all cry to you: Rejoice."

