ocMA.OdeS4C11.mode = misc_en_US_oca.Mode7
ocMA.OdeS4C11.ode = misc_en_US_oca.Ode4
ocMA.OdeS4C11.melody = he.h.m7_en_US_oca.OPatrikousKolpous.notmetered
ocMA.OdeS4C11.incipit = "He who was made flesh…"
ocMA.OdeS4C11.text = "He who was made flesh of the Virgin, gave His back to scourging. The innocent Master was tortured for the slave who had sinned, freeing me from my reproach."

ocMA.OdeS4C12.mode = misc_en_US_oca.Mode7
ocMA.OdeS4C12.ode = misc_en_US_oca.Ode4
ocMA.OdeS4C12.melody = he.h.m7_en_US_oca.OPatrikousKolpous.notmetered
ocMA.OdeS4C12.incipit = "The One who judges…"
ocMA.OdeS4C12.text = "The One who judges the whole world in righteousness stood trial before lawless judges and He who is the Creator of man was convicted and struck on the face by a hand of clay."

ocMA.OdeS4C1T.mode = misc_en_US_oca.Mode7
ocMA.OdeS4C1T.ode = misc_en_US_oca.Ode4
ocMA.OdeS4C1T.melody = he.h.m7_en_US_oca.OPatrikousKolpous.notmetered
ocMA.OdeS4C1T.incipit = "O True Theotokos…"
ocMA.OdeS4C1T.text = "O True Theotokos: pray to your Creator and Son to guide me to the haven of His glorious will, O immaculate One."

ocMA.OdeS4C21.mode = misc_en_US_oca.Mode7
ocMA.OdeS4C21.ode = misc_en_US_oca.Ode4
ocMA.OdeS4C21.melody = he.h.m7_en_US_oca.KatanoonOProfitis.notmetered
ocMA.OdeS4C21.incipit = "O Lord: You did not know sin…"
ocMA.OdeS4C21.text = "O Lord: You did not know sin, yet because of it, You became what You were not. You took another form and fashioned it anew to save the world and lure the tyrant to his death."

ocMA.OdeS4C22.mode = misc_en_US_oca.Mode7
ocMA.OdeS4C22.ode = misc_en_US_oca.Ode4
ocMA.OdeS4C22.melody = he.h.m7_en_US_oca.KatanoonOProfitis.notmetered
ocMA.OdeS4C22.incipit = "O Lord: You hung upon the Cross…"
ocMA.OdeS4C22.text = "O Lord: You hung upon the Cross and destroyed the sin of our first father Adam. You filled our mother Eve with joy, for You come to save Your anointed."

ocMA.OdeS4C2T.mode = misc_en_US_oca.Mode7
ocMA.OdeS4C2T.ode = misc_en_US_oca.Ode4
ocMA.OdeS4C2T.melody = he.h.m7_en_US_oca.KatanoonOProfitis.notmetered
ocMA.OdeS4C2T.incipit = "You were born of a Virgin…"
ocMA.OdeS4C2T.text = "You were born of a Virgin and You died, bringing to life Adam, who had wandered in understanding. Death trembled in fear at Your strength, for You came to save all those held in its corruption."

