ocMA.OdeS5C11.mode = misc_en_US_oca.Mode7
ocMA.OdeS5C11.ode = misc_en_US_oca.Ode5
ocMA.OdeS5C11.melody = he.h.m7_en_US_oca.NyxAfengis.notmetered
ocMA.OdeS5C11.incipit = "You were sold…"
ocMA.OdeS5C11.text = "You were sold for Your slaves, O Christ, and enduring blows, You purchased freedom for those who rise early in the morning and come to You and sing the praises of Your divinity."

ocMA.OdeS5C12.mode = misc_en_US_oca.Mode7
ocMA.OdeS5C12.ode = misc_en_US_oca.Ode5
ocMA.OdeS5C12.melody = he.h.m7_en_US_oca.NyxAfengis.notmetered
ocMA.OdeS5C12.incipit = "By Your divine strength…"
ocMA.OdeS5C12.text = "By Your divine strength in the weakness of the flesh, O Christ, You cast down the strong one. Through the Resurrection, Saviour, You revealed me as a conqueror of death."

ocMA.OdeS5C1T.mode = misc_en_US_oca.Mode7
ocMA.OdeS5C1T.ode = misc_en_US_oca.Ode5
ocMA.OdeS5C1T.melody = he.h.m7_en_US_oca.NyxAfengis.notmetered
ocMA.OdeS5C1T.incipit = "O Pure Mother…"
ocMA.OdeS5C1T.text = "O Pure Mother: you gave birth to God made flesh from you in a way divinely fitting. For you, O all-praised One did not know a man, but conceived by the Holy Spirit."

ocMA.OdeS5C21.mode = misc_en_US_oca.Mode7
ocMA.OdeS5C21.ode = misc_en_US_oca.Ode5
ocMA.OdeS5C21.melody = he.h.m7_en_US_oca.KyrieOTheosMou.notmetered
ocMA.OdeS5C21.incipit = "You were numbered among…"
ocMA.OdeS5C21.text = "You were numbered among the transgressors and lifted up on Golgotha. The lights of heaven hid themselves ; the earth was seized with violent trembling. The veil of the temple was torn in two, revealing the defection of the people."

ocMA.OdeS5C22.mode = misc_en_US_oca.Mode7
ocMA.OdeS5C22.ode = misc_en_US_oca.Ode5
ocMA.OdeS5C22.melody = he.h.m7_en_US_oca.KyrieOTheosMou.notmetered
ocMA.OdeS5C22.incipit = "We glorify You in song…"
ocMA.OdeS5C22.text = "We glorify You in song for You have destroyed the power of the tyrant by the strength of Your incomprehensible divinity, and raised the dead by Your Resurrection."

ocMA.OdeS5C2T.mode = misc_en_US_oca.Mode7
ocMA.OdeS5C2T.ode = misc_en_US_oca.Ode5
ocMA.OdeS5C2T.melody = he.h.m7_en_US_oca.KyrieOTheosMou.notmetered
ocMA.OdeS5C2T.incipit = "O All-praised Theotokos…"
ocMA.OdeS5C2T.text = "O All-praised Theotokos, Mother of the King and God: by your prayers, grant purification of sins to the faithful who unceasingly sing to your honour with faith and love."

