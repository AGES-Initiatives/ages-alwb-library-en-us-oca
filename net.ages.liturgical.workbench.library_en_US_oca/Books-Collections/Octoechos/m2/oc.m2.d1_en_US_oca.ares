ocMA.OdeS5C11.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS5C11.ode = miscellanea_en_US_oca.misc.Ode5
ocMA.OdeS5C11.melody = he.h.m2_en_US_oca.MesitisTheou.notmetered
ocMA.OdeS5C11.incipit = "O Christ: although it was…"
ocMA.OdeS5C11.text = "O Christ: although it was as strong as a cedar, You broke the insolence of the enemy, for You willed to be lifted up in the flesh upon the cypress, the pine, and the cedar, O Lord."

ocMA.OdeS5C12.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS5C12.ode = miscellanea_en_US_oca.misc.Ode5
ocMA.OdeS5C12.melody = he.h.m2_en_US_oca.MesitisTheou.notmetered
ocMA.OdeS5C12.incipit = "O Christ: they laid You…"
ocMA.OdeS5C12.text = "O Christ: they laid You, without breath, in the lowest pit, but by Your wounds You have raised with Yourself the wounded and forgotten who slept in the graves."

ocMA.OdeS5C1T.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS5C1T.ode = miscellanea_en_US_oca.misc.Ode5
ocMA.OdeS5C1T.melody = he.h.m2_en_US_oca.MesitisTheou.notmetered
ocMA.OdeS5C1T.incipit = "Pray to your Son and Lord…"
ocMA.OdeS5C1T.text = "Pray to your Son and Lord, O pure Virgin, to grant deliverance to prisoners, and peace from the enemies surrounding those who trust in you."

ocMA.OdeS5C21.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS5C21.ode = miscellanea_en_US_oca.misc.Ode5
ocMA.OdeS5C21.melody = he.h.m2_en_US_oca.OAnthraxToIsaia.notmetered
ocMA.OdeS5C21.incipit = "The first Adam…"
ocMA.OdeS5C21.text = "The first Adam, refusing to fast, tastes from the tree of death, but the second is crucified and utterly destroys his sin."

ocMA.OdeS5C22.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS5C22.ode = miscellanea_en_US_oca.misc.Ode5
ocMA.OdeS5C22.melody = he.h.m2_en_US_oca.OAnthraxToIsaia.notmetered
ocMA.OdeS5C22.incipit = "O Christ: You are impassible…"
ocMA.OdeS5C22.text = "O Christ: You are impassible in Your immaterial Godhead; You underwent suffering in death in Your human nature. You raised the dead from the depths of Hades, making them incorruptible."

