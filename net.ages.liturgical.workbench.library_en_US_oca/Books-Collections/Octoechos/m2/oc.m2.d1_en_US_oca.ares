ocMA.OdeS3C11.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS3C11.ode = miscellanea_en_US_oca.misc.Ode4
ocMA.OdeS3C11.melody = he.h.m2_en_US_oca.ExinthisenIErimos.notmetered
ocMA.OdeS3C11.incipit = "Creation was confounded…"
ocMA.OdeS3C11.text = "Creation was confounded at Your Passion, as it beheld You, who had established the universe by mere inclination of Your will appear in poverty and mocked by lawless men."

ocMA.OdeS3C12.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS3C12.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C12.melody = he.h.m2_en_US_oca.ExinthisenIErimos.notmetered
ocMA.OdeS3C12.incipit = "With Your hand, O Christ…"
ocMA.OdeS3C12.text = "With Your hand, O Christ, You fashioned me from the dust in Your own image, and when through sin, I lay in the dust of death, You went down to Hades with me, raising me up with Yourself."

ocMA.OdeS3C1T.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS3C1T.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C1T.melody = he.h.m2_en_US_oca.ExinthisenIErimos.notmetered
ocMA.OdeS3C1T.incipit = "The ranks of angels…"
ocMA.OdeS3C1T.text = "The ranks of angels were amazed at your giving birth, and the hearts of men trembled, O all-pure Virgin. Therefore we honour you in faith, O Theotokos."

ocMA.OdeS3C21.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS3C21.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C21.melody = he.h.m2_en_US_oca.ToxonSynetriviDynaston.notmetered
ocMA.OdeS3C21.incipit = "Christ, who is above…"
ocMA.OdeS3C21.text = "Christ, who is above all things, by His passion in the flesh was made lower than the angels."

ocMA.OdeS3C22.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS3C22.ode = miscellanea_en_US_oca.misc.Ode3
ocMA.OdeS3C22.melody = he.h.m2_en_US_oca.ToxonSynetriviDynaston.notmetered
ocMA.OdeS3C22.incipit = "Numbered as dead…"
ocMA.OdeS3C22.text = "Numbered as dead among the transgressors, O Christ, You appeared to the women after Your Resurrection shining in a crown of glory."

