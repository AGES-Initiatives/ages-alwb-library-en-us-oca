ocMA.OdeS6C11.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS6C11.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C11.melody = he.h.m2_en_US_oca.EnAvyssoPtaismaton.notmetered
ocMA.OdeS6C11.incipit = "The Righteous One…"
ocMA.OdeS6C11.text = "The Righteous One was condemned as an evil doer, and nailed to the Tree with the lawless, granting forgiveness with His own blood to the guilty."

ocMA.OdeS6C12.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS6C12.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C12.melody = he.h.m2_en_US_oca.EnAvyssoPtaismaton.notmetered
ocMA.OdeS6C12.incipit = "As by one man…"
ocMA.OdeS6C12.text = "As by one man, the first Adam, in days of old, death came to the world, so by one man, the Son of God, resurrection has been revealed."

ocMA.OdeS6C1T.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS6C1T.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C1T.melody = he.h.m2_en_US_oca.EnAvyssoPtaismaton.notmetered
ocMA.OdeS6C1T.incipit = "O Virgin: you conceived…"
ocMA.OdeS6C1T.text = "O Virgin: you conceived, without knowing a man, and remain forever Virgin, revealing the signs of the true divinity of your Son and God."

ocMA.OdeS6C21.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS6C21.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C21.melody = he.h.m2_en_US_oca.IchouRimatonDeiseos.notmetered
ocMA.OdeS6C21.incipit = "You set the cherubim…"
ocMA.OdeS6C21.text = "You set the cherubim before the tree of life, as guards against fallen mankind. But beholding You, they opened the gates, for You came to show the thief the way into Paradise."

ocMA.OdeS6C22.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS6C22.ode = miscellanea_en_US_oca.misc.Ode6
ocMA.OdeS6C22.melody = he.h.m2_en_US_oca.IchouRimatonDeiseos.notmetered
ocMA.OdeS6C22.incipit = "Through the death of one man…"
ocMA.OdeS6C22.text = "Through the death of one man, Hades became a barren wilderness, for Christ emptied its store of wealth for the sake of us all."

