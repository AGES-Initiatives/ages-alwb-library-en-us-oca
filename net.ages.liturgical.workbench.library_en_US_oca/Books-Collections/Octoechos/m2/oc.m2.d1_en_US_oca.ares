ocMA.OdeS1C1H.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS1C1H.ode = miscellanea_en_US_oca.misc.Ode1
ocMA.OdeS1C1H.melody = he.h.m2_en_US_oca.EnVythoKatestrose.melody
ocMA.OdeS1C1H.incipit = he.h.m2_en_US_oca.EnVythoKatestrose.incipit
ocMA.OdeS1C1H.text = he.h.m2_en_US_oca.EnVythoKatestrose.text

ocMA.OdeS1C11.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS1C11.ode = miscellanea_en_US_oca.misc.Ode1
ocMA.OdeS1C11.melody = he.h.m2_en_US_oca.EnVythoKatestrose.notmetered
ocMA.OdeS1C11.incipit = "For disobeying Your commandment…"
ocMA.OdeS1C11.text = "For disobeying Your commandment, we were enrolled in the books of the prince of this world. But, O Good One, he has been condemned by Your Cross. For attacking you as a mortal, he fell under the dominion of your power and was exposed in his weakness."

ocMA.OdeS1C12.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS1C12.ode = miscellanea_en_US_oca.misc.Ode1
ocMA.OdeS1C12.melody = he.h.m2_en_US_oca.EnVythoKatestrose.notmetered
ocMA.OdeS1C12.incipit = "You came into the world…"
ocMA.OdeS1C12.text = "You came into the world, O Deliverer of the human race and Author of incorruptible life. Through Your Resurrection, which we glorify, You rent the grave clothes of death:  for gloriously has He been glorified."

ocMA.OdeS1C1T.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS1C1T.ode = miscellanea_en_US_oca.misc.Ode1
ocMA.OdeS1C1T.melody = he.h.m2_en_US_oca.EnVythoKatestrose.notmetered
ocMA.OdeS1C1T.incipit = "You appeared, higher…"
ocMA.OdeS1C1T.text = "You appeared, higher than all the visible and invisible creation, O pure and ever Virgin, for you bore the Creator Who willed to take flesh from your womb. Pray to Him with boldness to save our souls."

ocMA.OdeS1C21.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS1C21.ode = miscellanea_en_US_oca.misc.Ode1
ocMA.OdeS1C21.melody = he.h.m2_en_US_oca.AtriptonAsynithi.notmetered
ocMA.OdeS1C21.incipit = "O Christ: by Your Passion…"
ocMA.OdeS1C21.text = "O Christ: by Your Passion in the flesh, You have become the strength of the weak, the Resurrection of the fallen, and incorruption for the dead: for gloriously has He been glorified."

ocMA.OdeS1C22.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS1C22.ode = miscellanea_en_US_oca.misc.Ode1
ocMA.OdeS1C22.melody = he.h.m2_en_US_oca.AtriptonAsynithi.notmetered
ocMA.OdeS1C22.incipit = "The creating and restoring…"
ocMA.OdeS1C22.text = "The creating and restoring God who was dead took pity on His fallen and broken image and raised it up: for gloriously has He been glorified."

ocMA.OdeS1C31.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS1C31.ode = miscellanea_en_US_oca.misc.Ode1
