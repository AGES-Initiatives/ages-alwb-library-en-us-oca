ocMA.OdeS4C1H.melody = he.h.m2_en_US_oca.ElilythasEkParthenou.melody
ocMA.OdeS4C1H.incipit = he.h.m2_en_US_oca.ElilythasEkParthenou.incipit
ocMA.OdeS4C1H.text = he.h.m2_en_US_oca.ElilythasEkParthenou.text

ocMA.OdeS4C11.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS4C11.ode = miscellanea_en_US_oca.misc.Ode4
ocMA.OdeS4C11.melody = he.h.m2_en_US_oca.ElilythasEkParthenou.notmetered
ocMA.OdeS4C11.incipit = "You are my God…"
ocMA.OdeS4C11.text = "You are my God who stood condemned before the judgment seat. You did not cry out, O Lord, as judgment was brought upon the nations. Therefore You have wrought salvation in the earth through Your Passion, O Christ."

ocMA.OdeS4C12.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS4C12.ode = miscellanea_en_US_oca.misc.Ode4
ocMA.OdeS4C12.melody = he.h.m2_en_US_oca.ElilythasEkParthenou.notmetered
ocMA.OdeS4C12.incipit = "The weapons of the enemy…"
ocMA.OdeS4C12.text = "The weapons of the enemy failed before Your Passion. The cities of the adversary were destroyed and the tyrant’s insolence overthrown by Your descent into Hades."

ocMA.OdeS4C1T.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS4C1T.ode = miscellanea_en_US_oca.misc.Ode4
ocMA.OdeS4C1T.melody = he.h.m2_en_US_oca.ElilythasEkParthenou.notmetered
ocMA.OdeS4C1T.incipit = "We, the faithful…"
ocMA.OdeS4C1T.text = "We, the faithful, all know you, O Lady Theotokos, as a haven of salvation, the rampart of an invincible city, for by your prayers you deliver our souls from danger."

ocMA.OdeS4C21.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS4C21.ode = miscellanea_en_US_oca.misc.Ode4
ocMA.OdeS4C21.melody = he.h.m2_en_US_oca.AkikoaKyrieTinEndoxon.notmetered
ocMA.OdeS4C21.incipit = "As she beheld You…"
ocMA.OdeS4C21.text = "As she beheld You, nailed to the Wood, the Virgin who bore You without pain, O Christ, suffered the anguish of a mother."

ocMA.OdeS4C22.mode = miscellanea_en_US_oca.misc.Mode2
ocMA.OdeS4C22.ode = miscellanea_en_US_oca.misc.Ode4
ocMA.OdeS4C22.melody = he.h.m2_en_US_oca.AkikoaKyrieTinEndoxon.notmetered
ocMA.OdeS4C22.incipit = "Death is conquered…"
ocMA.OdeS4C22.text = "Death is conquered. The One who died has despoiled the gates of Hades. He who devours all has been devoured, and all things past nature are given to me."

