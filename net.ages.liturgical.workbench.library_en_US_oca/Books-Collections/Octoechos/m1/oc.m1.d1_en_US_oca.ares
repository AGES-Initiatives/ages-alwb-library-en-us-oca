ocMA.OdeS9C11.mode = misc_en_US_oca.Mode1
ocMA.OdeS9C11.ode = misc_en_US_oca.Ode9
ocMA.OdeS9C11.melody = he.h.m1_en_US_oca.TyponTisAgnis.notmetered
ocMA.OdeS9C11.incipit = "Behold how the lawless…"
ocMA.OdeS9C11.text = "Behold how the lawless and disobedient people justified the accursed and godless man. They condemned the righteous One, the Lord of Glory, to the Tree: Him whom we rightly magnify."

ocMA.OdeS9C12.mode = misc_en_US_oca.Mode1
ocMA.OdeS9C12.ode = misc_en_US_oca.Ode9
ocMA.OdeS9C12.melody = he.h.m1_en_US_oca.TyponTisAgnis.notmetered
ocMA.OdeS9C12.incipit = "We glorify You…"
ocMA.OdeS9C12.text = "We glorify You, O Saviour, the innocent Lamb, Who take away the sins of the world, risen on the third day. Together with the Father and Your divine Spirit, we acknowledge You as God and magnify You, the Lord of glory."

ocMA.OdeS9C1T.mode = misc_en_US_oca.Mode1
ocMA.OdeS9C1T.ode = misc_en_US_oca.Ode9
ocMA.OdeS9C1T.melody = he.h.m1_en_US_oca.TyponTisAgnis.notmetered
ocMA.OdeS9C1T.incipit = "O Lord: save Your people…"
ocMA.OdeS9C1T.text = "O Lord: save Your people, whom You purchased with Your precious blood. Grant victories to the Orthodox Christians over their Adversary, and through the prayers of the Theotokos, preserve Your Churches in peace."

ocMA.OdeS9C21.mode = misc_en_US_oca.Mode1
ocMA.OdeS9C21.ode = misc_en_US_oca.Ode9
ocMA.OdeS9C21.melody = he.h.m1_en_US_oca.MystirionXenon.notmetered
ocMA.OdeS9C21.incipit = "Your Cross, O Lord…"
ocMA.OdeS9C21.text = "Your Cross, O Lord, is glorified by Your ineffable strength, for Your weakness is revealed as stronger than all power. Through it the mighty are cast down to the earth, and the poor are brought in safety back to heaven."

ocMA.OdeS9C22.mode = misc_en_US_oca.Mode1
ocMA.OdeS9C22.ode = misc_en_US_oca.Ode9
ocMA.OdeS9C22.melody = he.h.m1_en_US_oca.MystirionXenon.notmetered
ocMA.OdeS9C22.incipit = "The shadow of death…"
ocMA.OdeS9C22.text = "The shadow of death has been slain, for You, O Christ, have appeared to those in Hades. You granted them resurrection from the dead. Therefore we praise and magnify You, for You are Yourself the Life, the Resurrection and the Light."

ocMA.OdeS9C23.mode = misc_en_US_oca.Mode1
ocMA.OdeS9C23.ode = misc_en_US_oca.Ode9
ocMA.OdeS9C23.melody = he.h.m1_en_US_oca.MystirionXenon.notmetered
ocMA.OdeS9C23.incipit = "The Nature without beginning…"
ocMA.OdeS9C23.text = "The Nature without beginning or end is known in three single divine Persons. One Godhead in the Father, the Son and the Spirit, in whom Orthodox Christians put their trust, and are saved."

