ocMA.OdeS3C11.mode = misc_en_US_oca.Mode1
ocMA.OdeS3C11.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C11.melody = he.h.m1_en_US_oca.OMonosEidos.notmetered
ocMA.OdeS3C11.incipit = "O Lord of goodness…"
ocMA.OdeS3C11.text = "O Lord of goodness: You are my God, Who in compassion for the fallen have come down to me. By Your Crucifixion, You raised me up that I may sing to You: Holy is the Lord of glory; His goodness is beyond compare."

ocMA.OdeS3C12.mode = misc_en_US_oca.Mode1
ocMA.OdeS3C12.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C12.melody = he.h.m1_en_US_oca.OMonosEidos.notmetered
ocMA.OdeS3C12.incipit = "O Christ, the divinely existing Life…"
ocMA.OdeS3C12.text = "O Christ, the divinely existing Life: as compassionate God, You clothed Yourself with my corruptible form. You descended to the dust of death, breaking the chains of man's mortality, And on the third day, You rose, Clothing me with incorruption. "

ocMA.OdeS3C1T.mode = misc_en_US_oca.Mode1
ocMA.OdeS3C1T.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C1T.melody = he.h.m1_en_US_oca.OMonosEidos.notmetered
ocMA.OdeS3C1T.incipit = "You conceived God…"
ocMA.OdeS3C1T.text = "You conceived God in your womb through the Holy Spirit, and yet remained unconsumed, O Virgin. The bush unconsumed by the fire clearly foretold you to the law-giver Moses for you received the Fire that cannot be endured."

ocMA.OdeS3C21.mode = misc_en_US_oca.Mode1
ocMA.OdeS3C21.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C21.melody = he.h.m1_en_US_oca.ToProTonAionon.notmetered
ocMA.OdeS3C21.incipit = "Let us cry out to the One…"
ocMA.OdeS3C21.text = "Let us cry out to the One who took the straying sheep upon His shoulders, and through the Tree took off its sin: You have raised our horn; Holy are You, O Lord."

ocMA.OdeS3C22.mode = misc_en_US_oca.Mode1
ocMA.OdeS3C22.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C22.melody = he.h.m1_en_US_oca.ToProTonAionon.notmetered
ocMA.OdeS3C22.incipit = "Faithful, let us come…"
ocMA.OdeS3C22.text = "Faithful, let us come and bring our worship in truth to the Divine Spirit who brought Christ, the Great Shepherd, back from Hades. In His dispensation, through His apostles, He tended the nations in wisdom. "

ocMA.OdeS3C2T.mode = misc_en_US_oca.Mode1
ocMA.OdeS3C2T.ode = misc_en_US_oca.Ode3
ocMA.OdeS3C2T.melody = he.h.m1_en_US_oca.ToProTonAionon.notmetered
ocMA.OdeS3C2T.incipit = "Let us cry out to the Son…"
ocMA.OdeS3C2T.text = "Let us cry out to the Son, made flesh by His own will without seed from the Virgin, for He kept her as a pure Virgin after giving birth: You are God of all, Holy are You, O Lord."

