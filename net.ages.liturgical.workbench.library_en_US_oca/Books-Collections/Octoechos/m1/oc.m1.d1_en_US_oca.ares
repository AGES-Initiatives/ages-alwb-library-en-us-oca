ocMA.OdeS1C1H.mode = misc_en_US_oca.Mode1
ocMA.OdeS1C1H.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C1H.melody = misc_en_US_oca.HeirmosNotMetered
ocMA.OdeS1C1H.incipit = he.h.m1_en_US_oca.SouITropaiouchos.incipit
ocMA.OdeS1C1H.text = he.h.m1_en_US_oca.SouITropaiouchos.text

ocMA.OdeS1C11.mode = misc_en_US_oca.Mode1
ocMA.OdeS1C11.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C11.melody = he.h.m1_en_US_oca.SouITropaiouchos.notmetered
ocMA.OdeS1C11.incipit = "In the beginning…"
ocMA.OdeS1C11.text = "In the beginning, You fashioned me from dust, working with Your pure hands in divine activity. You spread out Your hands upon the Cross, recalling my corruptible body from the earth: the flesh You took from the Virgin."

ocMA.OdeS1C12.mode = misc_en_US_oca.Mode1
ocMA.OdeS1C12.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C12.melody = he.h.m1_en_US_oca.SouITropaiouchos.notmetered
ocMA.OdeS1C12.incipit = "The one who planted…"
ocMA.OdeS1C12.text = "The one who planted a soul within me by His divine breath, submitted Himself to slaughter and surrendered His soul to death. He loosed the everlasting bonds, and has raised the dead with Himself, glorifying them in incorruption."

ocMA.OdeS1C1T.mode = misc_en_US_oca.Mode1
ocMA.OdeS1C1T.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C1T.melody = he.h.m1_en_US_oca.SouITropaiouchos.notmetered
ocMA.OdeS1C1T.incipit = "Rejoice, O Fountain of grace…"
ocMA.OdeS1C1T.text = "Rejoice, O Fountain of grace. Rejoice, O Ladder and Gateway to heaven. Rejoice, O Candlestick and vessel of gold: the unhewn mountain who brought Christ, the Giver of Life to the world."

ocMA.OdeS1C21.mode = misc_en_US_oca.Mode1
ocMA.OdeS1C21.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C21.melody = he.h.m1_en_US_oca.ChristosGennatai.notmetered
ocMA.OdeS1C21.incipit = "Christ is made flesh…"
ocMA.OdeS1C21.text = "Christ is made flesh, and makes me divine; Christ is humbled, setting me on high; Christ, the Giver of life, enduring the Passion in His nature of flesh, has given me freedom from passions. Therefore I sing to Him a song of thanksgiving, for He has been glorified."

ocMA.OdeS1C22.mode = misc_en_US_oca.Mode1
ocMA.OdeS1C22.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C22.melody = he.h.m1_en_US_oca.ChristosGennatai.notmetered
ocMA.OdeS1C22.incipit = "The crucified Christ…"
ocMA.OdeS1C22.text = "The crucified Christ sets me on high; The dead Christ raises me with Himself. Christ has given me life. Therefore I clap my hands and sing with joy a hymn of victory to the Saviour, for He has been glorified."

ocMA.OdeS1C2T.mode = misc_en_US_oca.Mode1
ocMA.OdeS1C2T.ode = misc_en_US_oca.Ode1
ocMA.OdeS1C2T.melody = he.h.m1_en_US_oca.ChristosGennatai.notmetered
ocMA.OdeS1C2T.incipit = "You conceived God…"
ocMA.OdeS1C2T.text = "You conceived God, O all-holy Virgin, and by a virgin birth you bore Christ, made flesh of you: the only-begotten Son, one Person in two natures, for He has been glorified."

