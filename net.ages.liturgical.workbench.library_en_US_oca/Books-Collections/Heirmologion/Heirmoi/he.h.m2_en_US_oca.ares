heHE.IdeteIdeteOEnThalassi.text = "Behold; behold! I am he ⁄ who saved the people of Israel in the sea ⁄ and nourished them in the wilderness. ⁄ I drew water from a rock for mortal men ⁄ so that I might raise up the one who had fallen into corruption, ⁄ and draw him to me by my mercy."

heHE.IisousOZoisArchigos.greekname = ""
heHE.IisousOZoisArchigos.name = "Jesus, the Prince of life. (N/M)"
heHE.IisousOZoisArchigos.incipit = "Jesus, the Prince of life. (N/M)…"
heHE.IisousOZoisArchigos.notmetered = "N/M (He is Jesus.)"
