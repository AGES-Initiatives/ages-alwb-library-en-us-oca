trMA.Ode1C11.ode = miscellanea_en_US_oca.misc.Ode1
trMA.Ode1C11.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode1C11.melody = he.h.m1_en_US_oca.AnastaseosImera.name
trMA.Ode1C11.incipit = "This is a festival day…"
trMA.Ode1C11.text = "This is a festival day: ⁄ at the awakening of Christ, death has fled away; ⁄ The light of life has dawned; ⁄ Adam has risen and dances for joy! ⁄ Therefore let us cry aloud and sing a song of victory!"

trMA.Ode1C12.ode = miscellanea_en_US_oca.misc.Ode1
trMA.Ode1C12.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode1C12.melody = he.h.m1_en_US_oca.AnastaseosImera.name
trMA.Ode1C12.incipit = "…"
trMA.Ode1C12.text = "This is the day of the veneration of the Precious Cross: ⁄ now it is brought before us, ⁄ shining with the brightness of Christ's Resurrection! ⁄ Let us all draw near ⁄ and kiss it with great rejoicing in our souls!"

trMA.Ode1C13.ode = miscellanea_en_US_oca.misc.Ode1
trMA.Ode1C13.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode1C13.melody = he.h.m1_en_US_oca.AnastaseosImera.name
trMA.Ode1C13.incipit = "…"
trMA.Ode1C13.text = "Mighty Cross of the Lord, ⁄ manifest yourself, showing me the divine vision of your beauty! ⁄ Grant that I may worthily venerate you, ⁄ for I speak to you and embrace you as alive!"

trMA.Ode1C14.ode = miscellanea_en_US_oca.misc.Ode1
trMA.Ode1C14.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode1C14.melody = he.h.m1_en_US_oca.AnastaseosImera.name
trMA.Ode1C14.incipit = "…"
trMA.Ode1C14.text = "Let earth and heaven give praise with one accord, ⁄ for the all-blessed Cross is now set out before us all. ⁄ Christ's Body was nailed there when He was offered in sacrifice: ⁄ Let us venerate it with great rejoicing in our souls!"

trMA.Ode1C1Tr.ode = miscellanea_en_US_oca.misc.Ode1
trMA.Ode1C1Tr.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode1C1Tr.melody = he.h.m1_en_US_oca.AnastaseosImera.name
trMA.Ode1C1Tr.incipit = "…"
trMA.Ode1C1Tr.text = "Trinity of Persons, Unity of Essence, ⁄ Father, Son and Spirit, ⁄ equal in might, one in will and purpose, rule and dominion: ⁄ watch over Your world and grant it peace!"

trMA.Ode1C1T.ode = miscellanea_en_US_oca.misc.Ode1
trMA.Ode1C1T.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode1C1T.melody = he.h.m1_en_US_oca.AnastaseosImera.name
trMA.Ode1C1T.incipit = "…"
trMA.Ode1C1T.text = "You conceived without seed, unwedded Virgin: ⁄ the birth is pure and sinless! ⁄ The Child you bear is the Maker of all, Christ our God. ⁄ Entreat Him to grant peace to the world!"

trMA.Ode3C11.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C11.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode3C11.melody = he.h.m1_en_US_oca.DeftePomaPiomen.name
trMA.Ode3C11.incipit = "Come, let us sing…"
trMA.Ode3C11.text = "Come, let us sing a new song ⁄ to celebrate the conquering of hell, ⁄ for Christ is risen from the tomb; ⁄ He has taken death captive, saving the whole world!"

trMA.Ode3C12.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C12.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode3C12.melody = he.h.m1_en_US_oca.DeftePomaPiomen.name
trMA.Ode3C12.incipit = "…"
trMA.Ode3C12.text = "Come, all faithful, let us drink, ⁄ not from a well of earthly water that passes away, ⁄ but as we venerate the Cross of Christ, ⁄ let us drink from the fountain of light, ⁄ for His Cross is our glory!"

trMA.Ode3C13.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C13.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode3C13.melody = he.h.m1_en_US_oca.DeftePomaPiomen.name
trMA.Ode3C13.incipit = "…"
trMA.Ode3C13.text = "Now as we venerate Your Cross, ⁄ which Moses once prefigured with his outstretched arms, ⁄ we put to flight the invisible Amalek, O Christ our Master, ⁄ and so we gain salvation!"

trMA.Ode3C14.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C14.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode3C14.melody = he.h.m1_en_US_oca.DeftePomaPiomen.name
trMA.Ode3C14.incipit = "…"
trMA.Ode3C14.text = "Faithful, let us joyfully venerate the Cross of the Lord ⁄ with pure eyes and lips, ⁄ singing a song of exultation!"

trMA.Ode3C1Tr.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C1Tr.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode3C1Tr.melody = he.h.m1_en_US_oca.DeftePomaPiomen.name
trMA.Ode3C1Tr.incipit = "…"
trMA.Ode3C1Tr.text = "I honor one God without beginning, ⁄ three in Persons but undivided in Essence: ⁄ the Father, the Son and the Spirit of life, ⁄ in whose Name we were baptized!"

trMA.Ode3C1T.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C1T.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode3C1T.melody = he.h.m1_en_US_oca.DeftePomaPiomen.name
trMA.Ode3C1T.incipit = "…"
trMA.Ode3C1T.text = "Moses saw your mystery prefigured in the bush ⁄ in days of old, holy Virgin: ⁄ as the flames did not consume it, ⁄ so the fire of the Godhead did not consume your womb."

