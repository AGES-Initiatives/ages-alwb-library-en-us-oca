trMA.Ode3Kathisma4.melody = ""
trMA.Ode3Kathisma4.incipit = oc.m6.d1_en_US_oca.ocMA.Kathisma23.incipit
trMA.Ode3Kathisma4.text = oc.m6.d1_en_US_oca.ocMA.Kathisma23.text

trMA.Ode4C11.ode = miscellanea_en_US_oca.misc.Ode4
trMA.Ode4C11.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode4C11.melody = he.h.m1_en_US_oca.EpiTisTheiasFylakis.name
trMA.Ode4C11.incipit = "Behold, Christ is risen…"
trMA.Ode4C11.text = "Behold, Christ is risen! said the angel to the Myrrh-bearing women! ⁄ Do not lament, but go and say to the apostles: ⁄ Rejoice, for today is the world's salvation! ⁄ The tyranny of the enemy has been destroyed ⁄ through the death of Christ!"

trMA.Ode4C12.ode = miscellanea_en_US_oca.misc.Ode4
trMA.Ode4C12.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode4C12.melody = he.h.m1_en_US_oca.EpiTisTheiasFylakis.name
trMA.Ode4C12.incipit = "…"
trMA.Ode4C12.text = "Today as we celebrate the joyful veneration of Your life-giving Cross, ⁄ we prepare ourselves for Your most holy Passion, O Christ our Savior; ⁄ for You have wrought the salvation of the world ⁄ in Your almighty power!"

trMA.Ode4C13.ode = miscellanea_en_US_oca.misc.Ode4
trMA.Ode4C13.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode4C13.melody = he.h.m1_en_US_oca.EpiTisTheiasFylakis.name
trMA.Ode4C13.incipit = "…"
trMA.Ode4C13.text = "Today there is joy in earth and heaven, ⁄ for the sign of the Cross is made manifest to all the world! ⁄ The thrice-blessed Cross is set before us; ⁄ a fountain of ever-flowing grace to all who show it honor!"

trMA.Ode4C14.ode = miscellanea_en_US_oca.misc.Ode4
trMA.Ode4C14.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode4C14.melody = he.h.m1_en_US_oca.EpiTisTheiasFylakis.name
trMA.Ode4C14.incipit = "…"
trMA.Ode4C14.text = "What shall we offer You, Christ, ⁄ for You have given us Your precious Cross to venerate: ⁄ where Your holy blood was shed, and Your flesh was fixed by nails: ⁄ we kiss it with love, giving thanks to You!"

trMA.Ode4C1Tr.ode = miscellanea_en_US_oca.misc.Ode4
trMA.Ode4C1Tr.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode4C1Tr.melody = he.h.m1_en_US_oca.EpiTisTheiasFylakis.name
trMA.Ode4C1Tr.incipit = "…"
trMA.Ode4C1Tr.text = "I sing the praises of the three Persons in one Godhead; ⁄ I proclaim one simple, undivided Nature: ⁄ Father eternal, Son and Holy Spirit, ⁄ One in throne and dominion, ⁄ One single Kingdom, ⁄ One everlasting Power!"

trMA.Ode4C1T.ode = miscellanea_en_US_oca.misc.Ode4
trMA.Ode4C1T.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode4C1T.melody = he.h.m1_en_US_oca.EpiTisTheiasFylakis.name
trMA.Ode4C1T.incipit = "…"
trMA.Ode4C1T.text = "In you alone among women, pure Lady, ⁄ a fearful and wonderful thing was revealed: ⁄ you renewed nature,conceiving without seed and remaining a virgin, ⁄ for the Child you have borne is the true God!"

trMA.Ode5C11.ode = miscellanea_en_US_oca.misc.Ode5
trMA.Ode5C11.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode5C11.melody = he.h.m1_en_US_oca.OrthrisomenOrthrouVatheos.name
trMA.Ode5C11.incipit = "You have risen…"
trMA.Ode5C11.text = "You have risen from the tomb, never-setting Light, ⁄ shining upon the world with the bright dawn of incorruption! ⁄ In Your compassion You have driven out the dark sorrow of death ⁄ from the farthest corners of the earth!"

trMA.Ode5C12.ode = miscellanea_en_US_oca.misc.Ode5
trMA.Ode5C12.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode5C12.melody = he.h.m1_en_US_oca.OrthrisomenOrthrouVatheos.name
trMA.Ode5C12.incipit = "…"
trMA.Ode5C12.text = "Let us draw near, being cleansed by abstinence, ⁄ and let us venerate the all-holy Wood with fervent praise, ⁄ where Christ was crucified when He saved the world ⁄ in His compassion."

trMA.Ode5C13.ode = miscellanea_en_US_oca.misc.Ode5
trMA.Ode5C13.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode5C13.melody = he.h.m1_en_US_oca.OrthrisomenOrthrouVatheos.name
trMA.Ode5C13.incipit = "…"
trMA.Ode5C13.text = "Today the ranks of angels dance with gladness ⁄ at the veneration of Your Cross! ⁄ For through the Cross, O Christ, ⁄ You have shattered the hosts of devils, ⁄ bringing mankind salvation!"

trMA.Ode5C14.ode = miscellanea_en_US_oca.misc.Ode5
trMA.Ode5C14.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode5C14.melody = he.h.m1_en_US_oca.OrthrisomenOrthrouVatheos.name
trMA.Ode5C14.incipit = "…"
trMA.Ode5C14.text = "The Church is revealed as a second Paradise, ⁄ having a tree of life, as the first Paradise of old: ⁄ by touching Your Cross, O Lord, ⁄ we share in immortality!"

trMA.Ode5C1Tr.ode = miscellanea_en_US_oca.misc.Ode5
trMA.Ode5C1Tr.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode5C1Tr.melody = he.h.m1_en_US_oca.OrthrisomenOrthrouVatheos.name
trMA.Ode5C1Tr.incipit = "…"
trMA.Ode5C1Tr.text = "I glorify three co-eternal Persons in one Essence: ⁄ Father, Son and Spirit! ⁄ A single Light in threefold brightness: ⁄ one Power and Kingdom in unconfused Unity!"

trMA.Ode5C1T.ode = miscellanea_en_US_oca.misc.Ode5
trMA.Ode5C1T.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode5C1T.melody = he.h.m1_en_US_oca.OrthrisomenOrthrouVatheos.name
trMA.Ode5C1T.incipit = "…"
trMA.Ode5C1T.text = "You conceived by the law of nature, ⁄ yet above that law; ⁄ for you alone bore a child without seed. ⁄ We fear to think or speak of your childbearing, ⁄ all-blameless Virgin!"

trMA.Ode6C11.ode = miscellanea_en_US_oca.misc.Ode6
trMA.Ode6C11.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode6C11.melody = he.h.m1_en_US_oca.KatilthesEnToisKatotatois.name
trMA.Ode6C11.incipit = "You crushed death…"
trMA.Ode6C11.text = "You crushed death, O Christ, and rose as a mighty King, ⁄ recalling us from the depths of hell! ⁄ You brought us to the land of immortality, ⁄ granting us the joy of the Kingdom of Heaven!"

trMA.Ode6C12.ode = miscellanea_en_US_oca.misc.Ode6
trMA.Ode6C12.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode6C12.melody = he.h.m1_en_US_oca.KatilthesEnToisKatotatois.name
trMA.Ode6C12.incipit = "…"
trMA.Ode6C12.text = "Faithful, let us cry aloud with joy ⁄ as we greet the Cross of the Lord. ⁄ Let us sing triumphantly to God, ⁄ for it is a fountain of holiness to all in the world!"

trMA.Ode6C13.ode = miscellanea_en_US_oca.misc.Ode6
trMA.Ode6C13.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode6C13.melody = he.h.m1_en_US_oca.KatilthesEnToisKatotatois.name
trMA.Ode6C13.incipit = "…"
trMA.Ode6C13.text = "The words of the Psalmist are fulfilled, ⁄ for behold, we worship at the footstool of Your most pure feet, ⁄ at Your precious Cross, all-powerful Lord, ⁄ the thrice-blessed Wood!"

trMA.Ode6C14.ode = miscellanea_en_US_oca.misc.Ode6
trMA.Ode6C14.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode6C14.melody = he.h.m1_en_US_oca.KatilthesEnToisKatotatois.name
trMA.Ode6C14.incipit = "…"
trMA.Ode6C14.text = "We venerate Your Cross, merciful Lord: ⁄ the Tree which the Prophet of lamentation saw with its fruit. ⁄ and we sing in praise of Your bonds and tomb, ⁄ of the spears and the nails."

trMA.Ode6C15.ode = miscellanea_en_US_oca.misc.Ode6
trMA.Ode6C15.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode6C15.melody = he.h.m1_en_US_oca.KatilthesEnToisKatotatois.name
trMA.Ode6C15.incipit = "…"
trMA.Ode6C15.text = "We kiss the holy Cross, O Christ, ⁄ which You were pleased to bear on Your shoulders, ⁄ on which You accepted to be lifted up and crucified in the flesh! ⁄ We receive from it strength against our invisible enemies."

trMA.Ode6C1Tr.ode = miscellanea_en_US_oca.misc.Ode6
trMA.Ode6C1Tr.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode6C1Tr.melody = he.h.m1_en_US_oca.KatilthesEnToisKatotatois.name
trMA.Ode6C1Tr.incipit = "…"
trMA.Ode6C1Tr.text = "I praise the Unity in three Persons ⁄ and the Trinity worshipped in one Nature; ⁄ the Triune God, the threefold Light: ⁄ Father, Son and Holy Spirit."

trMA.Ode6C1T.ode = miscellanea_en_US_oca.misc.Ode6
trMA.Ode6C1T.mode = miscellanea_en_US_oca.misc.Mode1
trMA.Ode6C1T.melody = he.h.m1_en_US_oca.KatilthesEnToisKatotatois.name
trMA.Ode6C1T.incipit = "…"
trMA.Ode6C1T.text = "The greatest of all wonders was revealed in you, ⁄ ewe without blemish, ⁄ for you bore the Lamb that takes away the sin of the world! ⁄ Entreat Him fervently for those who sing your praises!"

