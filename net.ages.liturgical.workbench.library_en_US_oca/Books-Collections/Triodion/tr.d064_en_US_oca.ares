trMA.Ode7C11.ode = miscellanea_en_US_oca.misc.Ode7
trMA.Ode7C11.mode = miscellanea_en_US_oca.misc.Mode4
trMA.Ode7C11.greekmelody = he.h.m4_en_US_oca.heHE.ODiasosasEnPyri.greekname
trMA.Ode7C11.melody = he.h.m4_en_US_oca.heHE.ODiasosasEnPyri.notmetered
trMA.Ode7C11.incipit = "The people knelt…"
trMA.Ode7C11.text = "The people knelt before You with palms in their hands. ⁄ They rejoiced with the disciples, ⁄ rying: Hosanna to the Son of David! ⁄ Blessed are You, most praised Lord, ⁄ The God of our fathers!"

trMA.Ode7C12.ode = miscellanea_en_US_oca.misc.Ode7
trMA.Ode7C12.mode = miscellanea_en_US_oca.misc.Mode4
trMA.Ode7C12.melody = he.h.m4_en_US_oca.heHE.ODiasosasEnPyri.notmetered
trMA.Ode7C12.incipit = "The innocent children…"
trMA.Ode7C12.text = "The innocent children sang a hymn worthy for You, ⁄ King of Israel and King of angels. ⁄ Blessed are You, Most praised Lord, ⁄ The God of our fathers!"

trMA.Ode7C13.ode = miscellanea_en_US_oca.misc.Ode7
trMA.Ode7C13.mode = miscellanea_en_US_oca.misc.Mode4
trMA.Ode7C13.melody = he.h.m4_en_US_oca.heHE.ODiasosasEnPyri.notmetered
trMA.Ode7C13.incipit = "Bearing palms and branches…"
trMA.Ode7C13.text = "Bearing palms and branches, ⁄ The crowd hailed You, Son of David, crying out: ⁄ Blessed is the King of the new age to come! ⁄ Blessed are You, most praised Lord, ⁄ The God of our fathers!"

