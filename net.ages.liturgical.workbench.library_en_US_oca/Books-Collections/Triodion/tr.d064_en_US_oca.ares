trMA.Ode3C1H.incipit = he.h.m4_en_US_oca.heHE.NaousanAkrotomon.incipit
trMA.Ode3C1H.text = he.h.m4_en_US_oca.heHE.NaousanAkrotomon.text

trMA.Ode3C11.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C11.mode = miscellanea_en_US_oca.misc.Mode4
trMA.Ode3C11.greekmelody = he.h.m4_en_US_oca.heHE.NaousanAkrotomon.greekname
trMA.Ode3C11.melody = he.h.m4_en_US_oca.heHE.NaousanAkrotomon.notmetered
trMA.Ode3C11.incipit = "Lazarus had been dead…"
trMA.Ode3C11.text = "Lazarus had been dead four days, ⁄ but hell trembled at Your command, and released him, ⁄ For You, O Christ, are the Resurrection and the Life. ⁄ Upon You the Church is established, crying out: ⁄ Hosanna! Blessed is He that comes in the Name of the Lord!"

trMA.Ode3C12.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C12.mode = miscellanea_en_US_oca.misc.Mode4
trMA.Ode3C12.melody = he.h.m4_en_US_oca.heHE.NaousanAkrotomon.notmetered
trMA.Ode3C12.incipit = "…"
trMA.Ode3C12.text = "Come, O faithful, sing to God worthy hymns in Zion! ⁄ The Messiah has come to Jerusalem in glory and dominion. ⁄ Upon Him the Church is established, crying out: ⁄ Hosanna! Blessed is He that comes in the Name of the Lord!"

