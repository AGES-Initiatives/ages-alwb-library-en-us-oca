trMA.Ode6C11.ode = miscellanea_en_US_oca.misc.Ode6
trMA.Ode6C11.mode = miscellanea_en_US_oca.misc.Mode4
trMA.Ode6C11.greekmelody = he.h.m4_en_US_oca.heHE.EvoisanEnEffrosyni.greekname
trMA.Ode6C11.melody = he.h.m4_en_US_oca.heHE.EvoisanEnEffrosyni.notmetered
trMA.Ode6C11.incipit = "Israel, receive God's Kingdom…"
trMA.Ode6C11.text = "Israel, receive God's Kingdom! ⁄ Behold, a great light is rising on those who dwell in the shadow of death. ⁄ Let the peoples be renewed through sprinkling with blood divine!"

trMA.Ode6C12.ode = miscellanea_en_US_oca.misc.Ode6
trMA.Ode6C12.mode = miscellanea_en_US_oca.misc.Mode4
trMA.Ode6C12.melody = he.h.m4_en_US_oca.heHE.EvoisanEnEffrosyni.notmetered
trMA.Ode6C12.incipit = "…"
trMA.Ode6C12.text = "Release your ransomed prisoners, O Zion! ⁄ Lead them up from the waterless pit of ignorance! ⁄ Let the peoples be renewed through sprinkling with blood divine!"

