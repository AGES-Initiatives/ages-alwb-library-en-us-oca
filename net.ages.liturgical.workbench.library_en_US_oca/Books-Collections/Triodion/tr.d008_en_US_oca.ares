trMA.Ode1C11.ode = misc_en_US_oca.Ode1
trMA.Ode1C11.mode = misc_en_US_oca.Mode2
trMA.Ode1C11.melody = he.h.m2_en_US_oca.TinMoseosOdin.notmetered
trMA.Ode1C11.incipit = "Jesus my God…"
trMA.Ode1C11.text = "Jesus my God, accept me also now in repentance ⁄ as You did the Prodigal Son: ⁄ I have lived all my life in carelessness ⁄ and provoked You to anger."

trMA.Ode1C12.ode = misc_en_US_oca.Ode1
trMA.Ode1C12.mode = misc_en_US_oca.Mode2
trMA.Ode1C12.melody = he.h.m2_en_US_oca.TinMoseosOdin.notmetered
trMA.Ode1C12.incipit = "…"
trMA.Ode1C12.text = "I have sinfully wasted the divine wealth that once You gave me; ⁄ I have departed far from You and lived as the Prodigal. ⁄ Compassionate Father, accept me now also as I return."

trMA.Ode1C13.ode = misc_en_US_oca.Ode1
trMA.Ode1C13.mode = misc_en_US_oca.Mode2
trMA.Ode1C13.melody = he.h.m2_en_US_oca.TinMoseosOdin.notmetered
trMA.Ode1C13.incipit = "…"
trMA.Ode1C13.text = "Open Your arms to embrace me as a Father now, ⁄ accepting me also as the Prodigal Son, ⁄ that I may glorify You with thanksgiving ⁄ most merciful Lord!"

trMA.Ode1C1T.ode = misc_en_US_oca.Ode1
trMA.Ode1C1T.mode = misc_en_US_oca.Mode2
trMA.Ode1C1T.melody = he.h.m2_en_US_oca.TinMoseosOdin.notmetered
trMA.Ode1C1T.incipit = "…"
trMA.Ode1C1T.text = "Bestow the fullness of Your grace upon me, O God; ⁄ be a Benefactor to me, ⁄ and at the holy prayers of Your Mother ⁄ overlook the great number of my offenses."

trMA.Ode3C11.ode = misc_en_US_oca.Ode3
trMA.Ode3C11.mode = misc_en_US_oca.Mode2
trMA.Ode3C11.melody = he.h.m2_en_US_oca.SteirothentaMouTonNoun.notmetered
trMA.Ode3C11.incipit = "Completely beside myself…"
trMA.Ode3C11.text = "Completely beside myself in madness, ⁄ I have clung to the sins suggested to me by the passions. ⁄ But accept me, O Christ, as the Prodigal."

trMA.Ode3C12.ode = misc_en_US_oca.Ode3
trMA.Ode3C12.mode = misc_en_US_oca.Mode2
trMA.Ode3C12.melody = he.h.m2_en_US_oca.SteirothentaMouTonNoun.notmetered
trMA.Ode3C12.incipit = "…"
trMA.Ode3C12.text = "With the words of the Prodigal, I cry aloud: ⁄ I have sinned, O Father! ⁄ Receive me now like him in Your embrace ⁄ and do not reject me."

trMA.Ode3C13.ode = misc_en_US_oca.Ode3
trMA.Ode3C13.mode = misc_en_US_oca.Mode2
trMA.Ode3C13.melody = he.h.m2_en_US_oca.SteirothentaMouTonNoun.notmetered
trMA.Ode3C13.incipit = "…"
trMA.Ode3C13.text = "Open Your arms, O Christ, and receive me in loving kindness ⁄ as I return from a far country of sin and passions."

trMA.Ode3C1T.ode = misc_en_US_oca.Ode3
trMA.Ode3C1T.mode = misc_en_US_oca.Mode2
trMA.Ode3C1T.melody = he.h.m2_en_US_oca.SteirothentaMouTonNoun.notmetered
trMA.Ode3C1T.incipit = "…"
trMA.Ode3C1T.text = "My sins have brought me to poverty, ⁄ O fairest among women: ⁄ Enrich me, pure Virgin, with the vision of beauty, ⁄ that I may glorify you."

trMA.Ode3Kathisma1.mode = misc_en_US_oca.Mode1
trMA.Ode3Kathisma1.melody = he.a.m1_en_US_oca.TonTafonSouSotir.notmetered
trMA.Ode3Kathisma1.incipit = oc.m1.d2_en_US_oca.ocMA.Kathisma21.incipit
trMA.Ode3Kathisma1.text = oc.m1.d2_en_US_oca.ocMA.Kathisma21.text

trMA.Ode3Kathisma2.mode = misc_en_US_oca.Mode1
trMA.Ode3Kathisma2.melody = he.a.m1_en_US_oca.TonTafonSouSotir.notmetered
trMA.Ode3Kathisma2.incipit = "You are the only guardian…"
trMA.Ode3Kathisma2.text = "You are the only guardian and protection of the faithful, ⁄ pure and unwedded virgin Theotokos: ⁄ deliver from danger, distress and affliction ⁄ all who have put their hope in you ⁄ and by your divine intercession ⁄⁄ save our souls, O Maiden."

trMA.Ode4C11.ode = misc_en_US_oca.Ode4
trMA.Ode4C11.mode = misc_en_US_oca.Mode2
trMA.Ode4C11.melody = he.h.m2_en_US_oca.TinEkParthenou.notmetered
trMA.Ode4C11.incipit = "The wealth of blessings…"
trMA.Ode4C11.text = "The wealth of blessings which You gave me, heavenly Father ⁄ I have wrongfully wasted, becoming the slave of strangers. ⁄ Therefore I cry aloud to You: ⁄ Father, I have sinned against You! ⁄ Receive me like the Prodigal of old, ⁄ and open Your arms to me."

trMA.Ode4C12.ode = misc_en_US_oca.Ode4
trMA.Ode4C12.mode = misc_en_US_oca.Mode2
trMA.Ode4C12.melody = he.h.m2_en_US_oca.TinEkParthenou.notmetered
trMA.Ode4C12.incipit = "…"
trMA.Ode4C12.text = "I have become enslaved to every evil in my wretchedness, ⁄ bowing down before the demons that provoke the passions; ⁄ Through heedlessness I have lost possession of myself, O Savior; ⁄ Take pity on me as I flee to Your many mercies for refuge, heavenly Father."

trMA.Ode4C13.ode = misc_en_US_oca.Ode4
trMA.Ode4C13.mode = misc_en_US_oca.Mode2
trMA.Ode4C13.melody = he.h.m2_en_US_oca.TinEkParthenou.notmetered
trMA.Ode4C13.incipit = "…"
trMA.Ode4C13.text = "Filled with shame, I dare not look up to the height of heaven, ⁄ for I have foolishly bowed down to sin. ⁄ But now I return and cry out in compunction: ⁄ I have sinned against You; receive me, O King of all."

trMA.Ode4C1T.ode = misc_en_US_oca.Ode4
trMA.Ode4C1T.mode = misc_en_US_oca.Mode2
trMA.Ode4C1T.melody = he.h.m2_en_US_oca.TinEkParthenou.notmetered
trMA.Ode4C1T.incipit = "…"
trMA.Ode4C1T.text = "You are the help of men, and the refuge of the saved, ⁄ the sure hope of all Christians, undefiled Virgin. ⁄ Save me as a Mother by your intercessions, ⁄ counting me worthy of the life to come."

trMA.Ode5C11.ode = misc_en_US_oca.Ode5
trMA.Ode5C11.mode = misc_en_US_oca.Mode2
trMA.Ode5C11.melody = he.h.m2_en_US_oca.TisNyktosDielthousis.notmetered
trMA.Ode5C11.incipit = "I was enslaved to strangers…"
trMA.Ode5C11.text = "I was enslaved to strangers; ⁄ filled with shame, and an exile in the land of corruption. ⁄ But now I return, O merciful Lord, ⁄ and I cry to You: I have sinned!"

trMA.Ode5C12.ode = misc_en_US_oca.Ode5
trMA.Ode5C12.mode = misc_en_US_oca.Mode2
trMA.Ode5C12.melody = he.h.m2_en_US_oca.TisNyktosDielthousis.notmetered
trMA.Ode5C12.incipit = "…"
trMA.Ode5C12.text = "Accept me now in Your compassion, heavenly Father, ⁄ as I return from evil ⁄ and in Your exceeding mercy, do not reject me."

trMA.Ode5C13.ode = misc_en_US_oca.Ode5
trMA.Ode5C13.mode = misc_en_US_oca.Mode2
trMA.Ode5C13.melody = he.h.m2_en_US_oca.TisNyktosDielthousis.notmetered
trMA.Ode5C13.incipit = "…"
trMA.Ode5C13.text = "I have angered You beyond measure, O Christ, ⁄ and I dare not look up at the height of heaven. ⁄ But knowing Your compassion, merciful Lord, I cry: ⁄ I have sinned! Be merciful to me and save me!"

trMA.Ode5C1T.ode = misc_en_US_oca.Ode5
trMA.Ode5C1T.mode = misc_en_US_oca.Mode2
trMA.Ode5C1T.melody = he.h.m2_en_US_oca.TisNyktosDielthousis.notmetered
trMA.Ode5C1T.incipit = "…"
trMA.Ode5C1T.text = "All-holy Virgin, full of grace, ⁄ you have borne the Redemption of all! ⁄ By your prayers, lighten the great burden of my sins."

trMA.Ode6C11.ode = misc_en_US_oca.Ode6
trMA.Ode6C11.mode = misc_en_US_oca.Mode2
trMA.Ode6C11.melody = he.h.m2_en_US_oca.VythoAmartimaton.notmetered
trMA.Ode6C11.incipit = "The depths of sin…"
trMA.Ode6C11.text = "The depths of sin ever hold me fast, ⁄ and the tempest of transgressions overwhelms me! ⁄ Pilot me, O Christ my God, to the haven of life ⁄ and save me, King of glory!"

trMA.Ode6C12.ode = misc_en_US_oca.Ode6
trMA.Ode6C12.mode = misc_en_US_oca.Mode2
trMA.Ode6C12.melody = he.h.m2_en_US_oca.VythoAmartimaton.notmetered
trMA.Ode6C12.incipit = "…"
trMA.Ode6C12.text = "I have wasted in evil living the wealth which the Father gave me, ⁄ and now I am brought to poverty. ⁄ I am filled with shame and enslaved to fruitless thoughts. ⁄ Therefore I cry to You, Lover of mankind: ⁄ Take pity on me and save me!"

trMA.Ode6C13.ode = misc_en_US_oca.Ode6
trMA.Ode6C13.mode = misc_en_US_oca.Mode2
trMA.Ode6C13.melody = he.h.m2_en_US_oca.VythoAmartimaton.notmetered
trMA.Ode6C13.incipit = "…"
trMA.Ode6C13.text = "I am wasted with hunger, deprived of every blessing, ⁄ and an exile from Your presence. ⁄ O Christ, supreme in loving kindness, ⁄ take pity on me now as I return and save me ⁄ as I sing Your praises, Lover of mankind!"

trMA.Ode6C1T.ode = misc_en_US_oca.Ode6
trMA.Ode6C1T.mode = misc_en_US_oca.Mode2
trMA.Ode6C1T.melody = he.h.m2_en_US_oca.VythoAmartimaton.notmetered
trMA.Ode6C1T.incipit = "…"
trMA.Ode6C1T.text = "Maiden who conceived Christ the Savior and Master, ⁄ count me worthy of salvation. ⁄ I lack all good in my poverty, pure Virgin: ⁄ Grant that I may sing the praises of your majesty!"

trMA.Kontakion1.mode = misc_en_US_oca.Mode3
trMA.Kontakion1.melody = he.a.m3_en_US_oca.IParthenosSimeron.notmetered
trMA.Kontakion1.incipit = "…"
