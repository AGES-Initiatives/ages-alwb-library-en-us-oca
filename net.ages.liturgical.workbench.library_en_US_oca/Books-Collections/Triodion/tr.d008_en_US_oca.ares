trMA.Ode7C11.ode = misc_en_US_oca.Ode7
trMA.Ode7C11.mode = misc_en_US_oca.Mode2
trMA.Ode7C11.melody = he.h.m2_en_US_oca.TaCherouvimMimoumenoi.notmetered
trMA.Ode7C11.incipit = "I have miserably bowed down…"
trMA.Ode7C11.text = "I have miserably bowed down to the pleasures of the body ⁄ becoming wholly enslaved to the demons that provoke the passions. ⁄ I have become a stranger to You, Lover of mankind. ⁄ But now I cry with the voice of the Prodigal: ⁄ I have sinned, O Christ, despise me not, ⁄ for You alone are merciful."

trMA.Ode7C12.ode = misc_en_US_oca.Ode7
trMA.Ode7C12.mode = misc_en_US_oca.Mode2
trMA.Ode7C12.melody = he.h.m2_en_US_oca.TaCherouvimMimoumenoi.notmetered
trMA.Ode7C12.incipit = "…"
trMA.Ode7C12.text = "I do not dare look up at the height of heaven, O King of all: ⁄ I cry out: I have sinned; ⁄ for in my foolishness, I alone have angered You, ⁄ rejecting Your commandments. ⁄ Therefore, only Good One, ⁄ do not cast me away from Your presence."

trMA.Ode7C13.ode = misc_en_US_oca.Ode7
trMA.Ode7C13.mode = misc_en_US_oca.Mode2
trMA.Ode7C13.melody = he.h.m2_en_US_oca.TaCherouvimMimoumenoi.notmetered
trMA.Ode7C13.incipit = "…"
trMA.Ode7C13.text = "At the prayers of the apostles, martyrs and prophets, ⁄ the holy saints and the righteous, O Christ my Lord, ⁄ forgive me all the offenses which have provoked You to anger in Your goodness, ⁄ and I shall sing Your praises for evermore."

trMA.Ode7C1T.ode = misc_en_US_oca.Ode7
trMA.Ode7C1T.mode = misc_en_US_oca.Mode2
trMA.Ode7C1T.melody = he.h.m2_en_US_oca.TaCherouvimMimoumenoi.notmetered
trMA.Ode7C1T.incipit = "…"
trMA.Ode7C1T.text = "Theotokos, more glorious than the cherubim and seraphim, ⁄ and all the heavenly hosts! ⁄ Together with them, undefiled Virgin, ⁄ entreat Him who took flesh from you: ⁄ God the Word from the Father without beginning, ⁄ that we may all be counted worthy of eternal blessings!"

trMA.Ode8C11.ode = misc_en_US_oca.Ode8
trMA.Ode8C11.mode = misc_en_US_oca.Mode2
trMA.Ode8C11.melody = he.h.m2_en_US_oca.TonEnTiVatoMosi.notmetered
trMA.Ode8C11.incipit = "In great mercy You came down…"
trMA.Ode8C11.text = "In great mercy You came down upon earth ⁄ to save the world through Your voluntary poverty: ⁄ Save me in Your compassion, ⁄ for I am poor in all good works."

trMA.Ode8C12.ode = misc_en_US_oca.Ode8
trMA.Ode8C12.mode = misc_en_US_oca.Mode2
trMA.Ode8C12.melody = he.h.m2_en_US_oca.TonEnTiVatoMosi.notmetered
trMA.Ode8C12.incipit = "…"
trMA.Ode8C12.text = "I have departed far from Your commandments, ⁄ and am enslaved to the deceiver in utter wretchedness. ⁄ But now I turn back as the Prodigal of old: ⁄ Accept me as I fall before You, heavenly Father."

trMA.Ode8C13.ode = misc_en_US_oca.Ode8
trMA.Ode8C13.mode = misc_en_US_oca.Mode2
trMA.Ode8C13.melody = he.h.m2_en_US_oca.TonEnTiVatoMosi.notmetered
trMA.Ode8C13.incipit = "…"
trMA.Ode8C13.text = "I am full of darkness and cut off from You, ⁄ ruled by corrupting thoughts, ⁄ I have lost all possession of myself, merciful Lord. ⁄ Therefore save me as I fall before You in repentance."

trMA.Ode8C1T.ode = misc_en_US_oca.Ode8
trMA.Ode8C1T.mode = misc_en_US_oca.Mode2
trMA.Ode8C1T.melody = he.h.m2_en_US_oca.TonEnTiVatoMosi.notmetered
trMA.Ode8C1T.incipit = "…"
trMA.Ode8C1T.text = "Pure Mother of God, the only restoration of the fallen, ⁄ raise me up, for I am wholly crushed; ⁄ humbled by every kind of sin."

trMA.C1.acrostic = "" //Acrostic for Ode 9
trMA.Ode9C11.ode = misc_en_US_oca.Ode9
trMA.Ode9C11.mode = misc_en_US_oca.Mode2
trMA.Ode9C11.melody = he.h.m2_en_US_oca.TonGigenonTisIkouse.notmetered
trMA.Ode9C11.incipit = "Behold the affliction of my heart…"
trMA.Ode9C11.text = "Behold the affliction of my heart, O Christ: ⁄ look upon my turning back; ⁄ behold my tears, O Savior, and do not despise me, ⁄ but in Your compassion, embrace me once again, ⁄ and count me with the number of the saved, ⁄ that with thanksgiving I may sing the praises of Your mercy."

trMA.Ode9C12.ode = misc_en_US_oca.Ode9
trMA.Ode9C12.mode = misc_en_US_oca.Mode2
trMA.Ode9C12.melody = he.h.m2_en_US_oca.TonGigenonTisIkouse.notmetered
trMA.Ode9C12.incipit = "…"
trMA.Ode9C12.text = "As the Thief I cry to You: Remember me! ⁄ As the Publican, with downcast eyes, I beat my breast and say: Be merciful! ⁄ As the Prodigal, deliver me from every evil, O King who pities all, ⁄ that I may sing the praises of Your boundless compassion."

trMA.Ode9C13.ode = misc_en_US_oca.Ode9
trMA.Ode9C13.mode = misc_en_US_oca.Mode2
trMA.Ode9C13.melody = he.h.m2_en_US_oca.TonGigenonTisIkouse.notmetered
trMA.Ode9C13.incipit = "…"
trMA.Ode9C13.text = "Groan now, my all-wretched soul, and cry aloud to Christ: ⁄ Lord who for my sake voluntarily became poor, ⁄ in my poverty I lack every good work. ⁄ Make me rich with the abundance of Your blessings, ⁄ for You alone are full of love and mercy."

trMA.Ode9C14.ode = misc_en_US_oca.Ode9
trMA.Ode9C14.mode = misc_en_US_oca.Mode2
trMA.Ode9C14.melody = he.h.m2_en_US_oca.TonGigenonTisIkouse.notmetered
trMA.Ode9C14.incipit = "…"
trMA.Ode9C14.text = "Once You rejoiced at the voluntary return of the Prodigal, loving Lord: ⁄ Rejoice now because of me, wretch though I am. ⁄ Open Your holy embrace to me, ⁄ that being saved I may sing the praises of Your boundless compassion."

trMA.Ode9C1T.ode = misc_en_US_oca.Ode9
trMA.Ode9C1T.mode = misc_en_US_oca.Mode2
trMA.Ode9C1T.melody = he.h.m2_en_US_oca.TonGigenonTisIkouse.notmetered
trMA.Ode9C1T.incipit = "…"
trMA.Ode9C1T.text = "I pray you, Virgin, through your light-giving intercessions, ⁄ enlighten the eyes of my mind, darkened by evil. ⁄ Lead me into the paths of repentance, ⁄ so shall I rightly sing your praises, ⁄ for you gave flesh to the ineffable Word!"

