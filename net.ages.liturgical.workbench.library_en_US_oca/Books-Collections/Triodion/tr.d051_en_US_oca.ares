trVE.Stichera01.mode = miscellanea_en_US_oca.misc.Mode3
trVE.Stichera01.greekmelody = he.a.m3_en_US_oca.heAU.MegaliTouStavrouSou.greekname
trVE.Stichera01.melody = he.a.m3_en_US_oca.heAU.MegaliTouStavrouSou.notmetered
trVE.Stichera01.incipit = "Believers, in this season of…"
trVE.Stichera01.text = "Believers, in this season of abstinence, ⁄ let us exert every effort, ⁄ that we may attain great glory, escaping the fire of hell, ⁄⁄ by the mercy of God, the great King."

trVE.Stichera02.mode = miscellanea_en_US_oca.misc.Mode3
trVE.Stichera02.greekmelody = he.a.m3_en_US_oca.heAU.MegaliTouStavrouSou.greekname
trVE.Stichera02.melody = he.a.m3_en_US_oca.heAU.MegaliTouStavrouSou.notmetered
trVE.Stichera02.incipit = "Having passed half the period…"
trVE.Stichera02.text = "Having passed half the period of this holy fast, ⁄ let us clearly show forth the beginning of divine glory! ⁄ Let us fervently hasten to the achievement of good behavior, ⁄⁄ that we may receive the eternal joy."

trVE.Stichera03.mode = miscellanea_en_US_oca.misc.Mode7
trVE.Stichera03.greekmelody = he.a.m7_en_US_oca.heAU.KatafronisantesPanton.greekname
trVE.Stichera03.melody = he.a.m7_en_US_oca.heAU.KatafronisantesPanton.notmetered
trVE.Stichera03.incipit = "Having passed half the distance…"
trVE.Stichera03.text = "Having passed half the distance of this holy fast, ⁄ let us strive, rejoicing with steadfastness, to its end. ⁄ Let us anoint ourselves with the oil of charity, ⁄ that we may be worthy to adore the noble Passion of Christ our God, ⁄⁄ and attain his most honored and holy resurrection!"

trVE.AposIdiomelon1.mode = miscellanea_en_US_oca.misc.Mode7
trVE.AposIdiomelon1.melody = miscellanea_en_US_oca.misc.Idiomelon
trVE.AposIdiomelon1.incipit = "The Savior who planted the…"
trVE.AposIdiomelon1.text = "The Savior who planted the vineyard and called the laborers is near indeed! ⁄ Come, therefore, workers in the fast: ⁄ let us receive our hire, ⁄ for the Giver is rich and merciful. ⁄⁄ even if we have labored little we receive great mercy!"

trVE.AposIdiomelon2.poet = poets_en_US_oca.po.Stefanou
trVE.AposIdiomelon2.mode = miscellanea_en_US_oca.misc.Mode6
trVE.AposIdiomelon2.melody = miscellanea_en_US_oca.misc.Idiomelon
trVE.AposIdiomelon2.incipit = "When Adam fell among the…"
trVE.AposIdiomelon2.text = "When Adam fell among the robbers of thought, ⁄ he was robbed of his mind; ⁄ his soul was wounded, and he was cast out naked and without any aid. ⁄ The priest who came before the law did not listen to him. ⁄ The Levite who came after the law had no compassion on him: ⁄ only you, O God, who came not from Samaria but from the Theotokos! ⁄⁄ O Lord, glory to you!"

