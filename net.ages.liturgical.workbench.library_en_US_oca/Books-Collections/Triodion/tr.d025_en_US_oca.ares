trVE.AposMartyric.text = he.a.m8_en_US_oca.heAU.OiMartyresSouKyrie.text

trVE.AposTheotokion.mode = miscellanea_en_US_oca.misc.Mode8
trVE.AposTheotokion.greekmelody = he.a.m8_en_US_oca.heAU.OTouParadoxou.greekname
trVE.AposTheotokion.melody = he.a.m8_en_US_oca.heAU.OTouParadoxou.notmetered
trVE.AposTheotokion.incipit = oc.m8.d4_en_US_oca.ocVE.SticTheotokion.incipit
trVE.AposTheotokion.text = oc.m8.d4_en_US_oca.ocVE.SticTheotokion.text

trMA.Kathisma21.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Kathisma21.melody = miscellanea_en_US_oca.misc.NotMetered
trMA.Kathisma21.incipit = "Through your Passion, loving Lord…"
trMA.Kathisma21.text = "Through your Passion, loving Lord, ⁄ you have given all men freedom from passions, ⁄ putting to death the passions of my flesh by your Cross. ⁄ Count me worthy, then, to see your divine Passion: ⁄ that having been well-pleasing to your glory in the fast, ⁄⁄ I may receive your great and abundant mercy!"

trMA.Kathisma22.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Kathisma22.melody = miscellanea_en_US_oca.misc.NotMetered
trMA.Kathisma22.incipit = "Beholding you, O Christ, stretched…"
trMA.Kathisma22.text = "Beholding you, O Christ, stretched dead upon the tree, ⁄ your virgin Mother cried out with bitter tears: ⁄ My Son, what is this fearful mystery? ⁄ How do you give eternal life to all, ⁄⁄ and yet suffer willingly a shameful death upon the Cross?"

trMA.Kathisma31.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Kathisma31.melody = miscellanea_en_US_oca.misc.NotMetered
trMA.Kathisma31.incipit = "Lord, you have consecrated and…"
trMA.Kathisma31.text = "Lord, you have consecrated and given us this light-giving season of abstinence, ⁄ enable us all to pass through it in compunction and sincerity, ⁄ living in peace by the power of the Cross, ⁄⁄ only Lover of mankind!"

trMA.Kathisma32.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Kathisma32.melody = miscellanea_en_US_oca.misc.NotMetered
trMA.Kathisma32.incipit = "Guarded by the precious Cross…"
trMA.Kathisma32.text = "Guarded by the precious Cross of your Son, ⁄ pure Lady and Theotokos, ⁄ we easily turn away all the assaults of the enemy! ⁄ Therefore we rightly call you blessed, ⁄⁄ for you are the Mother of Light and the only hope of our souls!"

trMA.Ode3C11.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C11.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode3C11.greekmelody = he.h.m2_en_US_oca.heHE.ExinthisenIErimos.greekname
trMA.Ode3C11.melody = he.h.m2_en_US_oca.heHE.ExinthisenIErimos.notmetered
trMA.Ode3C11.incipit = "Let us crucify our members…"
trMA.Ode3C11.text = "Let us crucify our members through abstinence; ⁄ and as it is written, let us be vigilant in prayer, ⁄ living after the example of him who suffered; ⁄ by his Passion, putting to death the passions."

trMA.Ode3C12.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C12.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode3C12.melody = he.h.m2_en_US_oca.heHE.ExinthisenIErimos.notmetered
trMA.Ode3C12.incipit = "Rejecting the bitter food of…"
trMA.Ode3C12.text = "Rejecting the bitter food of sin, ⁄ let us seek to please Christ, ⁄ who of his own will tasted gall ⁄ and by the Cross cast down the author of evil."

trMA.Ode3C13.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C13.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode3C13.melody = he.h.m2_en_US_oca.heHE.ExinthisenIErimos.notmetered
trMA.Ode3C13.incipit = "Sin has become a habit…"
trMA.Ode3C13.text = "Sin has become a habit for me, ⁄ and it drags me down to complete perdition. ⁄ But by your Cross deliver me from my sinfulness, ⁄ compassionate Lord of many mercies!"

trMA.Ode3C1T.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C1T.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode3C1T.melody = he.h.m2_en_US_oca.heHE.ExinthisenIErimos.notmetered
trMA.Ode3C1T.incipit = "Lady who bore the Master…"
trMA.Ode3C1T.text = "Lady who bore the Master, ⁄ Queen of all creation, ⁄ set me free from bondage to the deceitful enemy!"

trMA.Ode3C21.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C21.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode3C21.greekmelody = he.h.m2_en_US_oca.heHE.StereosonImasEnSoiKyrie.greekname
trMA.Ode3C21.melody = he.h.m2_en_US_oca.heHE.StereosonImasEnSoiKyrie.notmetered
trMA.Ode3C21.incipit = "From the tree of the…"
trMA.Ode3C21.text = "From the tree of the Cross ⁄ the flower of abstinence grows for all the world. ⁄ Then let us accept the fast with love, ⁄ and take pleasure in the fruit of Christ’s divine commandments."

trMA.Ode3C22.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C22.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode3C22.melody = he.h.m2_en_US_oca.heHE.StereosonImasEnSoiKyrie.notmetered
trMA.Ode3C22.incipit = "Abstaining from the passions, ⁄…"
trMA.Ode3C22.text = "Abstaining from the passions, ⁄ let us crucify our flesh for the sake of the Lord: ⁄ by our life in Christ, ⁄ let us all show that the pride of the flesh is dead."

trMA.Ode3C2Tr.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C2Tr.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode3C2Tr.melody = he.h.m2_en_US_oca.heHE.StereosonImasEnSoiKyrie.notmetered
trMA.Ode3C2Tr.incipit = "I glorify three persons in…"
trMA.Ode3C2Tr.text = "I glorify three persons in one nature, ⁄ Father, Son and Spirit, ⁄ the one power of the Godhead, ⁄ one kingdom over all and one brightness!"

trMA.Ode3C2T.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C2T.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode3C2T.melody = he.h.m2_en_US_oca.heHE.StereosonImasEnSoiKyrie.notmetered
trMA.Ode3C2T.incipit = "Your childbearing, pure Lady, fills…"
trMA.Ode3C2T.text = "Your childbearing, pure Lady, fills our hearts with fear, ⁄ for it is God who has become man, ⁄ beyond time, begotten from the Father, ⁄ and in the last days brought forth by you through a virgin birth."

trMA.Ode3C2A1.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C2A1.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode3C2A1.melody = he.h.m2_en_US_oca.heHE.StereosonImasEnSoiKyrie.notmetered
trMA.Ode3C2A1.incipit = "I praise your crucifixion, O…"
trMA.Ode3C2A1.text = "I praise your crucifixion, O Christ, ⁄ and the piercing of your divine side, ⁄ from which each day I draw an immortal drink ⁄ and am sanctified."

trMA.Ode3C2H.ode = miscellanea_en_US_oca.misc.Ode3
trMA.Ode3C2H.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode3C2H.greekmelody = he.h.m2_en_US_oca.heHE.StereosonImasEnSoiKyrie.greekname
trMA.Ode3C2H.melody = he.h.m2_en_US_oca.heHE.StereosonImasEnSoiKyrie.melody
trMA.Ode3C2H.incipit = he.h.m2_en_US_oca.heHE.StereosonImasEnSoiKyrie.incipit
trMA.Ode3C2H.text = he.h.m2_en_US_oca.heHE.StereosonImasEnSoiKyrie.text

trMA.Ode8C11.ode = miscellanea_en_US_oca.misc.Ode8
trMA.Ode8C11.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode8C11.greekmelody = he.h.m2_en_US_oca.heHE.KaminosPote.greekname
trMA.Ode8C11.melody = he.h.m2_en_US_oca.heHE.KaminosPote.notmetered
trMA.Ode8C11.incipit = "The furnace of the passions…"
trMA.Ode8C11.text = "The furnace of the passions consumes my soul; ⁄ but quench its flames with the dew of your mercy. ⁄ For in your loving self-abasement, Benefactor, ⁄ at your crucifixion, ⁄ you made a fountain of dispassion to flow from your pure side."

trMA.Ode8C12.ode = miscellanea_en_US_oca.misc.Ode8
trMA.Ode8C12.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode8C12.melody = he.h.m2_en_US_oca.heHE.KaminosPote.notmetered
trMA.Ode8C12.incipit = "Lifted upon your Cross, O…"
trMA.Ode8C12.text = "Lifted upon your Cross, O Christ, ⁄ you have raised up us who were fallen into evil. ⁄ I have slipped into the pit of sin: ⁄ bring me out and set me firmly on the rock of salvation, ⁄ that I may glorify your power!"

trMA.Ode8C13.ode = miscellanea_en_US_oca.misc.Ode8
trMA.Ode8C13.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode8C13.melody = he.h.m2_en_US_oca.heHE.KaminosPote.notmetered
trMA.Ode8C13.incipit = "Through the lance that pierced…"
trMA.Ode8C13.text = "Through the lance that pierced your side, O Christ, ⁄ you have cleansed my heart from the corruption of the passions. ⁄ I have been wounded by the poisonous bite of the serpent. ⁄ Heal me in every part, ⁄ and make me walk unwavering upon the paths of God."

trMA.Ode8C1T.ode = miscellanea_en_US_oca.misc.Ode8
trMA.Ode8C1T.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode8C1T.melody = he.h.m2_en_US_oca.heHE.KaminosPote.notmetered
trMA.Ode8C1T.incipit = "We all honor you, undefiled…"
trMA.Ode8C1T.text = "We all honor you, undefiled Virgin, ⁄ as the shining lamp and candlestick ⁄ in which the fire of the Godhead came to dwell, ⁄ bringing light to those held fast in the dark night of corruption, ⁄ and we bless your childbearing, blessed among women!"

trMA.Ode8C21.ode = miscellanea_en_US_oca.misc.Ode8
trMA.Ode8C21.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode8C21.greekmelody = he.h.m2_en_US_oca.heHE.TonEnTiVatoMosi.greekname
trMA.Ode8C21.melody = he.h.m2_en_US_oca.heHE.TonEnTiVatoMosi.notmetered
trMA.Ode8C21.incipit = "Let us praise him who…"
trMA.Ode8C21.text = "Let us praise him who was crucified upon the tree between two thieves, ⁄ and whose life-giving side was pierced by a lance: ⁄ let us bless and exalt him above all forever!"

trMA.Ode8C22.ode = miscellanea_en_US_oca.misc.Ode8
trMA.Ode8C22.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode8C22.melody = he.h.m2_en_US_oca.heHE.TonEnTiVatoMosi.notmetered
trMA.Ode8C22.incipit = "You judge all the earth…"
trMA.Ode8C22.text = "You judge all the earth ⁄ yet you stood before the judgment seat of Pilate! ⁄ You were struck on the cheek, mocked, and hung upon the Cross, ⁄ delivering me forever from the corruption of the ancient sin."

trMA.Ode8C2Tr.ode = miscellanea_en_US_oca.misc.Ode8
trMA.Ode8C2Tr.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode8C2Tr.melody = he.h.m2_en_US_oca.heHE.TonEnTiVatoMosi.notmetered
trMA.Ode8C2Tr.incipit = "One Godhead, one in Trinity…"
trMA.Ode8C2Tr.text = "One Godhead, one in Trinity, ⁄ undivided in nature yet divided in persons, ⁄ power that shall never be destroyed, ⁄ Father, Son and Spirit, ⁄ we sing your praises forever!"

trMA.Ode8C2T.ode = miscellanea_en_US_oca.misc.Ode8
trMA.Ode8C2T.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode8C2T.melody = he.h.m2_en_US_oca.heHE.TonEnTiVatoMosi.notmetered
trMA.Ode8C2T.incipit = "Pure Mother of God, ⁄…"
trMA.Ode8C2T.text = "Pure Mother of God, ⁄ gate of heaven and door of salvation, ⁄ accept the prayer of all Christians who call you blessed forever!"

trMA.Ode8C2A1.ode = miscellanea_en_US_oca.misc.Ode8
trMA.Ode8C2A1.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode8C2A1.melody = he.h.m2_en_US_oca.heHE.TonEnTiVatoMosi.notmetered
trMA.Ode8C2A1.incipit = "Cross of Christ, you brought…"
trMA.Ode8C2A1.text = "Cross of Christ, you brought the thief to faith, ⁄ and me to the season of the fast: ⁄ count me worthy to venerate you at the feast that is to come, ⁄ and to be filled with life."

trMA.Ode8C2H.ode = miscellanea_en_US_oca.misc.Ode8
trMA.Ode8C2H.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode8C2H.greekmelody = he.h.m2_en_US_oca.heHE.TonEnTiVatoMosi.greekname
trMA.Ode8C2H.melody = he.h.m2_en_US_oca.heHE.TonEnTiVatoMosi.melody
trMA.Ode8C2H.incipit = he.h.m2_en_US_oca.heHE.TonEnTiVatoMosi.incipit
trMA.Ode8C2H.text = he.h.m2_en_US_oca.heHE.TonEnTiVatoMosi.text

trMA.Ode9C11.ode = miscellanea_en_US_oca.misc.Ode9
trMA.Ode9C11.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode9C11.greekmelody = he.h.m2_en_US_oca.heHE.AnarchouGennitoros.greekname
trMA.Ode9C11.melody = he.h.m2_en_US_oca.heHE.AnarchouGennitoros.notmetered
trMA.Ode9C11.incipit = "The fast shines upon us…"
trMA.Ode9C11.text = "The fast shines upon us all more brightly than the sun, ⁄ bringing us the light of grace, ⁄ proclaiming the good news of the Cross, ⁄ of the precious Passion, and the saving day of resurrection."

trMA.Ode9C12.ode = miscellanea_en_US_oca.misc.Ode9
trMA.Ode9C12.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode9C12.melody = he.h.m2_en_US_oca.heHE.AnarchouGennitoros.notmetered
trMA.Ode9C12.incipit = "Let us love chastity and…"
trMA.Ode9C12.text = "Let us love chastity and flee from fornication! ⁄ Let us gird our loins with temperance ⁄ that we may appear in purity before the Savior of our souls, ⁄ who alone is pure and desires the purification of us all."

trMA.Ode9C13.ode = miscellanea_en_US_oca.misc.Ode9
trMA.Ode9C13.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode9C13.melody = he.h.m2_en_US_oca.heHE.AnarchouGennitoros.notmetered
trMA.Ode9C13.incipit = "Nail my flesh with the…"
trMA.Ode9C13.text = "Nail my flesh with the fear of you, O Christ, ⁄ who nailed the sin of adam to the Cross. ⁄ Loose me from the bonds of wickedness, ⁄ shatter the arrows of the evil one with your lance, Master, ⁄ and deliver me from all his mischief."

trMA.Ode9C1T.ode = miscellanea_en_US_oca.misc.Ode9
trMA.Ode9C1T.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode9C1T.melody = he.h.m2_en_US_oca.heHE.AnarchouGennitoros.notmetered
trMA.Ode9C1T.incipit = "Virgin Maiden who bore Christ…"
trMA.Ode9C1T.text = "Virgin Maiden who bore Christ the Lord, ⁄ the just Judge who alone is ever ready to forgive, ⁄ deliver me from the condemnation, ⁄ from the fire and torment that I deserve to suffer ⁄ because of my delight in sin."

trMA.Ode9C21.ode = miscellanea_en_US_oca.misc.Ode9
trMA.Ode9C21.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode9C21.greekmelody = he.h.m2_en_US_oca.heHE.TinAgninKaiAchranton.greekname
trMA.Ode9C21.melody = he.h.m2_en_US_oca.heHE.TinAgninKaiAchranton.notmetered
trMA.Ode9C21.incipit = "How great is your compassion!…"
trMA.Ode9C21.text = "How great is your compassion! ⁄ For you endured the Cross, the nails and the lance, O Lord, ⁄ for my sake, who am condemned to corruption. ⁄ Therefore I sing your praises, O Christ."

trMA.Ode9C22.ode = miscellanea_en_US_oca.misc.Ode9
trMA.Ode9C22.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode9C22.melody = he.h.m2_en_US_oca.heHE.TinAgninKaiAchranton.notmetered
trMA.Ode9C22.incipit = "With all your people we…"
trMA.Ode9C22.text = "With all your people we worship your life-giving Passion, ⁄ the Cross, the reed, the nails and the lance, ⁄ praising you in our songs, O Christ."

trMA.Ode9C2Tr.ode = miscellanea_en_US_oca.misc.Ode9
trMA.Ode9C2Tr.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode9C2Tr.melody = he.h.m2_en_US_oca.heHE.TinAgninKaiAchranton.notmetered
trMA.Ode9C2Tr.incipit = "Unity in three coequal persons…"
trMA.Ode9C2Tr.text = "Unity in three coequal persons, ⁄ undivided Trinity, sovereign nature: ⁄ Father, Son and divine Spirit, save us all."

trMA.Ode9C2T.ode = miscellanea_en_US_oca.misc.Ode9
trMA.Ode9C2T.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode9C2T.melody = he.h.m2_en_US_oca.heHE.TinAgninKaiAchranton.notmetered
trMA.Ode9C2T.incipit = "Rejoice, Theotokos, ⁄ mercy-seat of…"
trMA.Ode9C2T.text = "Rejoice, Theotokos, ⁄ mercy-seat of the world! ⁄ As we ever run to you for refuge, ⁄ we sinners gain reconciliation with God."

trMA.Ode9C2A1.ode = miscellanea_en_US_oca.misc.Ode9
trMA.Ode9C2A1.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode9C2A1.melody = he.h.m2_en_US_oca.heHE.TinAgninKaiAchranton.notmetered
trMA.Ode9C2A1.incipit = "Through your Cross, loving Lord…"
trMA.Ode9C2A1.text = "Through your Cross, loving Lord, you gave me strength: ⁄ grant that I may complete the time of the fast with good courage!"

trMA.Ode9C2H.ode = miscellanea_en_US_oca.misc.Ode9
trMA.Ode9C2H.mode = miscellanea_en_US_oca.misc.Mode2
trMA.Ode9C2H.greekmelody = he.h.m2_en_US_oca.heHE.TinAgninKaiAchranton.greekname
trMA.Ode9C2H.melody = he.h.m2_en_US_oca.heHE.TinAgninKaiAchranton.melody
trMA.Ode9C2H.incipit = he.h.m2_en_US_oca.heHE.TinAgninKaiAchranton.incipit
trMA.Ode9C2H.text = he.h.m2_en_US_oca.heHE.TinAgninKaiAchranton.text

trMA.AposIdiomelon1.mode = miscellanea_en_US_oca.misc.Mode8
trMA.AposIdiomelon1.melody = miscellanea_en_US_oca.misc.Idiomelon
trMA.AposIdiomelon1.incipit = "Come, let us battle thoughts…"
trMA.AposIdiomelon1.text = "Come, let us battle thoughts of passion through fasting, ⁄ shielding ourselves with spiritual wings ⁄ so that we may endure the enemy’s storm with ease! ⁄ That we may be worthy of adoring the Cross ⁄ of the Son of God who was voluntarily slain for the world, ⁄ that we may spiritually celebrate the Savior’s resurrection from the dead! ⁄ Ascending the mountain with the disciples, ⁄ we will glorify the one who received all authority from the Father, ⁄⁄ the Son and Lover of mankind!"

trMA.AposMartyric.mode = miscellanea_en_US_oca.misc.Mode8
trMA.AposMartyric.melody = ""
trMA.AposMartyric.incipit = "Invincible martyrs of Christ, ⁄…"
trMA.AposMartyric.text = "Invincible martyrs of Christ, ⁄ you defeated deceit with the power of the Cross ⁄ and received the grace of eternal life! ⁄ The threats of your torturers, you did not fear! ⁄ You were glad of being wounded by them, ⁄ and now your blood is the healing of our souls! ⁄⁄ pray that our souls may be saved!"

trMA.AposTheotokion.mode = miscellanea_en_US_oca.misc.Mode8
trMA.AposTheotokion.greekmelody = he.a.m8_en_US_oca.heAU.OTouParadoxou.greekname
trMA.AposTheotokion.melody = he.a.m8_en_US_oca.heAU.OTouParadoxou.notmetered
trMA.AposTheotokion.incipit = "What is this sight I…"
trMA.AposTheotokion.text = "What is this sight I behold with my eyes, Master? ⁄ The one who sustains all creation is raised on the wood! ⁄ The one who gives life to all is made to die! ⁄ The most pure Theotokos lamented in tears, ⁄ when she beheld the God and man on the Cross ⁄⁄ who ineffably shone from her!"

trTE.Troparion.mode = miscellanea_en_US_oca.misc.Mode4
trTE.Troparion.melody = ""
trTE.Troparion.incipit = "You know our frame, our…"
trTE.Troparion.text = "You know our frame, our weakness, Lover of mankind! ⁄ We have sinned, but have not turned from you, O God, ⁄ nor have we raised our hands to a foreign god: ⁄⁄ pity us in your goodness, compassionate One!"

trTE.Troparion2.text = "Pity us in your goodness, compassionate One!"
