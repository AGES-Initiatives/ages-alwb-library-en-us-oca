trShortCommemoration1.text = "St. Gregory Palamas"
trInsert1 = "our father among the saints Gregory Palamas, Archbishop of Thessaloniki," //for use in petitions and dismissal
trInsert2 = "whose memeory we celebrate," //for use in petitions and dismissal
trNote.text = ""
